import string<br>import random<br><br>def code(a):<br>    b = a.split(&quot; &quot;)<br>    newstring = []<br>    for i in b:<br>        if len(i) &lt; 3:<br>            newstring.append(i[::-1])<br>        else:<br>            z = i[0]<br>            i = i[1:]<br>            i = i + z<br>            tmp1 = &#39;&#39;.join(random.choices(string.ascii_letters + string.digits, k =3))<br>            tmp2 = &#39;&#39;.join(random.choices(string.ascii_letters + string.digits, k =3))<br>            i = tmp1 + i + tmp2<br>            newstring.append(i)<br>            encryptedstring = &#39; &#39;.join(newstring)<br>    print(encryptedstring)<br><br><br>def decode(a):<br>    newlist = a.split(&quot; &quot;)<br>    decodedlist = []<br>    for i in newlist:<br>        if len(i) &lt; 3:<br>            decodedlist.append(i[::-1])<br>        else:<br>            i = i[3:-3]<br>            i = i[-1] + i[:-1]<br>            decodedlist.append(i)<br>            decryptedstring = &quot; &quot;.join(decodedlist)<br>    print(decryptedstring)<br><br>a = input(&quot;Enter you string for code/decode: &quot;)<br>while True:<br>    b = int(input(&quot;Enter 1 for coding 2 for decoding: &quot;))<br>    match b:<br>        case 1:<br>            code(a)<br>            break<br>        case 2:<br>            decode(a)<br>            break<br>        case _:<br>            print(&quot;you&#39;ve entered wrong option, Enter again!&quot;)<br>            continue
