def add(x, y):<br>    return x + y<br><br>def subtract(x, y):<br>    return x - y<br><br>def multiply(x, y):<br>    return x * y<br><br>def divide(x, y):<br>    if y != 0:<br>        return x / y<br>    else:<br>        return &quot;Cannot divide by zero&quot;<br><br># Example usage:<br>num1 = float(input(&quot;Enter first number: &quot;))<br>operator = input(&quot;Enter operator (+, -, *, /): &quot;)<br>num2 = float(input(&quot;Enter second number: &quot;))<br><br>if operator == &#39;+&#39;:<br>    result = add(num1, num2)<br>elif operator == &#39;-&#39;:<br>    result = subtract(num1, num2)<br>elif operator == &#39;*&#39;:<br>    result = multiply(num1, num2)<br>elif operator == &#39;/&#39;:<br>    result = divide(num1, num2)<br>else:<br>    result = &quot;Invalid operator&quot;<br><br>print(f&quot;Result: {result}&quot;)
