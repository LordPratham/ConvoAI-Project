# Factorial of a number.<br><br>def factorial(n):<br>    if n == 0:<br>        return 1<br>    else:<br>        return n * factorial(n - 1)<br><br><br>print(factorial(4))
# Sorting in ascending order<br><br>import array as arr<br><br>my_array = arr.array(&#39;i&#39;, [4, 2, 7, 1, 8, 3])<br>sorted_array = sorted(my_array)<br>print(sorted_array)<br><br># Output: [1, 2, 3, 4, 7, 8]<br><br><a href=""http://www.youtube.com/results?search_query=%23sorting"">#Sorting</a> in descending order<br>import array as arr<br><br>my_array = arr.array(&#39;i&#39;, [4, 2, 7, 1, 8, 3])<br>sorted_array = sorted(my_array, reverse=True)<br>print(sorted_array)<br><br><br># Output: [8, 7, 4, 3, 2, 1]
