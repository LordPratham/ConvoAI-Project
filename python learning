import string<br>import random<br>randomLetter = random.choice(string.ascii_letters)<br>randomLetter2 = random.choice(string.ascii_letters)<br>cd =int(input(&quot;enter 1 for encoding and 2 for decoding: &quot;))<br>if cd ==1:<br>    encode= input(&quot;Enter a letter you wanna encode: &quot;)<br>    enlen=(len(encode))<br>    if enlen&lt;=3:<br>     print(encode[::-1])<br>    else:  <br>     print(f&quot;{randomLetter}&quot;+encode[1:len(encode)]+encode[0]+f&quot;{randomLetter2}&quot;)<br><br>elif cd ==2:<br>    encode= input(&quot;Enter a letter you wanna Decode: &quot;)<br>    enlen=(len(encode))<br>    if enlen&lt;=3:<br>        print(encode[::-1])<br>    else:<br>       s=encode[1:-1]<br>       l= int(len(s))<br>       print(f&quot;the decode value of {encode} is &quot;,s[-1]+s[0:l-1])
lst = []<br>str = &quot;&quot;<br>prefixSet={&#39;dlx&#39;,&#39;jct&#39;,&#39;ozq&#39;,&#39;dpw&#39;,&#39;htd&#39;,&#39;ytr&#39;,&#39;mdr&#39;}<br>suffixSet={&#39;emt&#39;,&#39;gqt&#39;,&#39;auf&#39;,&#39;gsf&#39;,&#39;bht&#39;,&#39;sht&#39;,&#39;spt&#39;}<br>Encode_Decode = input(&quot;Enter what you want to do (Encode/Decode): &quot;)<br>text = input(&quot;Enter the secret message of maximum 7 words that you want to encode/decode: &quot;)<br>listText = text.split()<br>if Encode_Decode.lower() == &#39;encode&#39;:   <br>    for words in listText:<br>        if len(words) &gt;= 3:<br>            listChar = list(words)<br>            listChar.append(listChar[0])<br>            listChar.remove(listChar[0])<br>            if len(prefixSet)&gt;0:<br>                prefix=prefixSet.pop()<br>            else: <br>                raise IndexError(&quot;The number of elements in the prefix set are not sufficient. Please add some more elements in the prefix set.&quot;)<br>            if len(suffixSet)&gt;0:<br>                suffix=suffixSet.pop()<br>            else: <br>                raise IndexError(&quot;The number of elements in the suffix set are not sufficient. Please add some more elements in the suffix set.&quot;)<br><br>            listChar.append(suffix)<br>            listChar.insert(0, prefix)<br>            lst.append(listChar)   <br>        else:<br>            listChar = list(words)<br>            listChar.reverse()<br>            lst.append(listChar)<br>    for ele in lst:<br>        str=str+&quot; &quot;+&quot;&quot;.join(ele)<br>    print(f&quot;The encoded message is: {str}&quot;)<br>    <br>elif Encode_Decode.lower()==&#39;decode&#39;:<br>    for words in listText:<br>        if(len(words))&lt;3:<br>            listChar = list(words)<br>            listChar.reverse()<br>            lst.append(listChar)<br>        else:<br>            listChar = list(words)<br>            listChar=listChar[3:-3]<br>            listChar.insert(0,listChar[-1])<br>            listChar=listChar[:-1]<br>            lst.append(listChar)<br>    for ele in lst:<br>        str=str+&quot; &quot;+&quot;&quot;.join(ele)<br>    print(f&quot;The decoded message is: {str}&quot;)  <br>else:<br>    print(&quot;You have entered wrong option for message encryption. Please Choose Encode/Decode&quot;)
def Encoding():<br>    et = input(&quot;Enter the word you want to encrypt: &quot;)<br>  <br>    temp = []<br>    for i in range(len(et)):<br>        temp.append(et[i])<br>  <br>  <br>    t = temp[0]<br>    if len(et) &gt; 3:<br>        temp.append(t)<br>        temp[0] = &#39;fat&#39;<br>        temp.append(&#39;xyz&#39;)<br>        my_string = &#39;&#39;.join(temp)<br>        print(my_string)<br>    else:<br>        temp.reverse()<br>        my_string = &#39;&#39;.join(temp)<br>        print(my_string)<br><br><br>def Decoding():<br>    dt = input(&quot;Enter the word you want to decrypt: &quot;)<br>  <br>    temp2 = []<br>    for i in range(len(dt)):<br>        temp2.append(dt[i])<br>  <br>  <br>    if len(dt) &lt; 3:<br>        temp2.reverse()<br>        my_string = &#39;&#39;.join(temp2)<br>        print(my_string)<br>    else:<br>        del temp2[0:3]<br>        del temp2[-3:]<br>  <br>        e = temp2[-1]<br>        temp2.insert(0, e)<br>        del temp2[-1]<br>  <br>        my_string2 = &#39;&#39;.join(temp2)<br>        print(my_string2)  <br>      <br><br><br>Choice = input(&quot;Do you want to Encode or Decode: &quot;)<br><br>if Choice == &quot;Encode&quot;:<br>    print(&quot;Starting Encoding....&quot;)<br>    Encoding()<br>elif Choice == &quot;Decode&quot;:<br>    print(&quot;Starting Decode.....&quot;)<br>    Decoding()<br>else:<br>    print(&quot;Invalid Input&quot;)
import random<br>import string<br><br>def encode(word):<br>    if(len(word)&gt;3):<br>        temp_string = word[1:len(word)] + word[0]<br><br>        available_letters = string.ascii_lowercase<br>        # Generate a random string of three letters<br>        random_string = &#39;&#39;.join(random.choice(available_letters) for _ in range(3))<br>        random_string1 = &#39;&#39;.join(random.choice(available_letters) for _ in range(3))<br>        encoded_string = random_string + temp_string + random_string1<br><br>        print(&quot;The Final Encoded string is &quot;, encoded_string)<br>        return encoded_string<br>    else:<br>        encoded_string = &#39;&#39;.join(reversed(word))<br>        print(&quot;The Final Encoded string is &quot;,encoded_string)<br>        return encoded_string<br><br><br>def decode(to_decode):<br>    if(len(to_decode)&gt;3):<br>        temp = to_decode[3:len(to_decode)-3]<br>        decoded_string = temp[len(temp)-1] + temp[:len(temp)-1]<br>        print(&quot;The Final Decoded string is &quot;, decoded_string)<br><br>    else:<br>        decoded_string = &#39;&#39;.join(reversed(to_decode))<br>        print(&quot;The Final Decoded string is &quot;, decoded_string)<br><br><br><br>print(&quot;Enter the string to be Encoded&quot;)<br><br>word = input()<br><br>to_decode = encode(word) <a href=""http://www.youtube.com/results?search_query=%23calling"">#calling</a> the function to encode<br><br>decode(to_decode)  <a href=""http://www.youtube.com/results?search_query=%23calling"">#calling</a> the function to decode
import random<br>import string<br><br>text = list(input(&quot;Enter the message: &quot;))<br>choose = int(<br>  input(&quot;Enter 1 to Encode the message and enter 2 to decode the message&quot;))<br><br><br>def encode(message):<br>  if len(message) &gt;= 3:<br>    message.append(message[0])<br>    # message.remove(message[0])<br>    message = message[1:] + [<br>      random.choice(string.ascii_letters) for _ in range(3)<br>    ]<br>    message = [random.choice(string.ascii_letters) for _ in range(3)] + message<br>  else:<br>    message.reverse()<br>  return &#39;&#39;.join(message)<br><br><br>def decode(message):<br>  if len(message) &lt; 3:<br>    message.reverse()<br>  else:<br>    message = message[3:-3]<br>    message.insert(0, message[-1])<br>    message.pop()<br>  return &#39;&#39;.join(message)<br><br><br>if choose == 1:<br>  encoded_msg = encode(text)<br>  print(&quot;Encoded message:&quot;, encoded_msg)<br>elif choose == 2:<br>  decoded_msg = decode(text)<br>  print(&quot;Decoded message:&quot;, decoded_msg)<br>else:<br>  print(&quot;Invalid choice&quot;)
def encryption():<br>    a=str(input(&quot;Enter the message to encrypt:&quot;))<br>    # print(a.split())<br>    b=a.split()<br>    for i,x in enumerate(b):<br>        word=b[i]<br><br>        if len(word)&gt;2:<br>            word1=word[0:2]<br>            word2=word[2:]+word1<br>            print(&quot;be&quot;+word2 , end=&quot; &quot;)<br>        else:<br>            word1=word[0]<br>            word2=word[1:]+word1<br>            print(&quot;be&quot;+word2 , end=&quot; &quot;)<br>    <br>            <br>            <br>def decryption():<br>    a=str(input(&quot;Enter the message to decrypt:&quot;))<br>    # print(a.split())<br>    b=a.split()<br>    for i,x in enumerate(b):<br>        word=b[i]<br><br>        if len(word)&gt;4:<br>            word1=word[len(word)-2:len(word)]<br>            word2=word1+word[2:len(word)-2]<br>            print(word2 , end=&quot; &quot;)<br>        else:<br>            word1=word[len(word)-1:len(word)]<br>            word2=word1+word[2:len(word)-1]<br>            print(word2 , end=&quot; &quot;)<br><br>print(&quot;Secret Language Of Shimla:\n&quot; )<br>import time<br>time.sleep(1.5)<br>A=int(input(&quot;You want to (1) Encrypt a message or  (2) Decrypt a message (1 or 2) : &quot;))<br>if A==1:<br>    encryption()<br>elif A==2:<br>    decryption()<br>else:<br>    print(&quot;Please answer with 1 or 2 only&quot;)"
