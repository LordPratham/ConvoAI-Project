num = int(input(&quot;Enter the number: &quot;))<br>factors = []<br><br>for x in range(1, num+1):<br>    if num % x == 0:<br>        factors.append(x)<br>        <br>        <br># A prime number is a number which has only 2 factors one itself and second 1<br># print(len(factors))<br>if len(factors) == 2:<br>    print(f&quot;Congratulations {num} is a prime number, with factors only {factors}&quot;)<br><br>else:<br>    print(f&quot;Aaah! sorry {num} is not a prime number because it has factors {factors}&quot;)
