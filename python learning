class Library:<br>  def __init__(self,num_books,books):<br>    self.num_books = num_books<br>    self.books = books<br>    <br>  def check(self):<br>      if(self.num_books == len(self.books)):<br>        print(&quot;both are equal&quot;)<br>      else:<br>        print(&quot;both are not equal&quot;)<br><br>  # def add(self,bookname):<br>  #   self.num_books += 1<br>  #   self.books.append(bookname)<br>  #   print(&#39;book added successfully&#39;)<br><br>  <a href=""http://www.youtube.com/results?search_query=%23to"">#to</a> add more than one book<br>  def add_books(self):<br>    num = int(input(&#39;number of books you want to add: &#39;))<br>    for i in range(num):<br>      self.num_books += 1<br>      book_title = input(f&#39;enter the title of book {i+1}:&#39;)<br>      self.books.append(book_title)<br>      print(f&#39;book added successfully{book_title}&#39;)<br>    <br><br>  def showBooks(self):<br>    print(f&#39;the total Number of books {self.num_books} and books are:&#39;)<br>    for book in self.books:<br>      print(book)<br>    <br>  <br><br>obj = Library(0,[])<br>obj.check()<br><a href=""http://www.youtube.com/results?search_query=%23bookname"">#bookname</a> = input(&#39;enter the name of book&#39;)<br>obj.add_books()<br>obj.showBooks()
