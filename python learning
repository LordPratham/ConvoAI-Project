o=input(&#39;enter:first number&#39;)<br>m=input(&#39;enter:sign&#39;)<br>l=input(&#39;enter:second number&#39;)<br><br><br>def math(a, sign, b):<br>    if sign == &#39;+&#39;:<br>        c = int(a) + int(b)<br>        print(c)<br>    if sign == &#39;-&#39;:<br>        z = int(a) - int(b)<br>        print(z)<br>    if sign == &#39;/&#39;:<br>        g = int(a) / int(b)<br>        print(g)<br>    if sign == &#39;//&#39;:<br>        h = int(a) // int(b)<br>        print(h)<br>    if sign == &#39;%&#39;:<br>        m = int(a) % int(b)<br>        print(m)<br>    if sign == &#39;*&#39;:<br>        q = int(a) * int(b)<br>        print(q)<br>    if sign == &#39;**&#39;:<br>        e = int(a) ** int(b)<br>        print(e)<br>try:<br>    math(o,m,l)<br>except:<br>    print(&#39;bye&#39;)
