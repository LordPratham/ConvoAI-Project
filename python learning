import random<br><br>def get_question():<br>    questions = [<br>        {<br>            &quot;question&quot;: &quot;Which planet is known as the Red Planet?&quot;,<br>            &quot;options&quot;: [&quot;Mars&quot;, &quot;Venus&quot;, &quot;Jupiter&quot;, &quot;Saturn&quot;],<br>            &quot;answer&quot;: &quot;Mars&quot;<br>        },<br>        {<br>            &quot;question&quot;: &quot;Who painted the Mona Lisa?&quot;,<br>            &quot;options&quot;: [&quot;Leonardo da Vinci&quot;, &quot;Pablo Picasso&quot;, &quot;Vincent van Gogh&quot;, &quot;Claude Monet&quot;],<br>            &quot;answer&quot;: &quot;Leonardo da Vinci&quot;<br>        },<br>    ]<br>    return random.choice(questions)<br><br>def ask_question(question, options):<br><br>    print(question)<br>    for i, option in enumerate(options):<br>        print(f&quot;{i+1}. {option}&quot;)<br><br>def play_game():<br>    print(&quot;Welcome to &#39;Kaun Banega Crorepati&#39;!&quot;)<br>    print(&quot;Let&#39;s play!&quot;)<br><br>    question = get_question()<br>    question_text = question[&quot;question&quot;]<br>    options = question[&quot;options&quot;]<br>    correct_answer = question[&quot;answer&quot;]<br><br>    ask_question(question_text, options)<br><br>    user_choice = int(input(&quot;Enter your answer (1-4): &quot;))<br><br>    if options[user_choice - 1] == correct_answer:<br>        print(&quot;Congratulations! You have won 1 crore rupees!&quot;)<br>    else:<br>        print(&quot;Oops! That&#39;s incorrect. Better luck next time!&quot;)<br><br>play_game()
