import random as r<br>import string as s<br><br>def Coding(y):<br><br>    print(&quot;\n\nCoding of the string is here:\n&quot;)<br><br>    l=y.split(&quot; &quot;) <br><br>    <a href=""http://www.youtube.com/results?search_query=%23print"">#print</a>(l)<br><br>    empty=[]<br><br>    for i in l:<br>    <br>       if(len(i)&gt;2):<br>        <br>            n=i[1:len(i)].__add__(i[0])<br><br>            ran1=&quot;&quot;.join(r.choice(s.ascii_letters) for i in range(3))<br><br>            ran2=&quot;&quot;.join(r.choice(s.ascii_letters) for i in range(3))<br><br>            j=ran1+n+ran2<br><br>            # print(n,end=&quot; &quot;)<br><br>            empty.append(j)<br>       else:<br>            if(len(i)==2):<br>                # print(i[1]+i[0],end=&quot; &quot;)<br><br>                 empty.append(i[1]+i[0])<br>            else:<br>                # print(i,end=&quot; &quot;)<br><br>                empty.append(i)<br><br>    return &quot; &quot;.join(empty)        <br><br><br>def Decoding(z):<br><br>    print(&quot;\n\nDecoding of the string is here:\n&quot;)<br>    <br><br>    empty=[]<br><br>    l=z.split(&quot; &quot;)<br><br>    <a href=""http://www.youtube.com/results?search_query=%23print"">#print</a>(l)<br><br>    for i in l:<br>        <a href=""http://www.youtube.com/results?search_query=%23print"">#print</a>(i)<br><br>        if(len(i)&lt;3):<br><br>            if(len(i)==2):<br>                <a href=""http://www.youtube.com/results?search_query=%23print"">#print</a>(i[1]+i[0],end=&quot; &quot;)<br><br>                empty.append(i[1]+i[0])<br>            else:<br>                <a href=""http://www.youtube.com/results?search_query=%23print"">#print</a>(i,end=&quot; &quot;)<br><br>                empty.append(i) <br>        else:<br>            <br>            j=i.replace(i[2],i[-4])<br>            <br>            <a href=""http://www.youtube.com/results?search_query=%23print"">#print</a>(j)<br>        <br>            empty.append(j[2:-4])<br><br>            <a href=""http://www.youtube.com/results?search_query=%23print"">#print</a>(,end=&quot; &quot;)<br>            <br><br>    return &quot; &quot;.join(empty)<br><br><br>y=input(&quot;Enter the string you want for coding:&quot;)<br><br>z=Coding(y)<br><br>print(z)<br><br>print(Decoding(z))
