from array import *<br><br>arr = array(&#39;i&#39;,[12,6,3,2,7,9])<br>temp = 0<br><br>print(&quot;displaying oraginal arry&quot;)<br><br>for a in range(0,len(arr)):<br>    print(arr[a],end=&#39; &#39;)<br><br>for i in range(0,len(arr)):<br>    for j in range(i+1,len(arr)):<br>        if arr[i] &gt; arr[j]:<br>            temp = arr[i]<br>            arr[i] = arr[j]<br>            arr[j] = temp<br><br>print()<br>print(&#39;Assending Oder&#39;)<br>for s in range(0,len(arr)):<br>    print(arr[s],end=&quot; &quot;)
