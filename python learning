def add(*a):<br>    var=0<br>    for i in a:<br>        if type(i) is list:<br>            for j in i:<br>                var=var+j<br>            break<br>        var=var+i<br>    print(&#39;SUM of the Values: &#39;,var)<br><br>add(12,5,2,[1,5,51,6,123,1019])
