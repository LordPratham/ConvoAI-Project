class Library:<br>    <br>  def __init__(self):<br>    self.books = []<br><br>  def addBooks(self, book):<br>    self.books.append(book)<br><br><br>  def printAllBooks(self):<br>    print(f&quot;The Library has {len(self.books)} books :-&quot;)<br>    for book in self.books:<br>        print(book)<br>            <br><br>lib1 = Library()<br>lib1.addBooks(&quot;Deep Work&quot;)<br>lib1.addBooks(&quot;Ego Is The Enemy&quot;)<br>lib1.addBooks(&quot;Doglapan&quot;)<br>lib1.addBooks(&quot;Good Vibes, Good Life&quot;)<br>lib1.printAllBooks()
