import random<br>import string<br><br>def generateRandChar(Counter):<br>    randWord = &quot;&quot;<br>    for randomIter in range(Counter):<br>        randWord += random.choice(string.ascii_letters)<br>    return randWord<br><br>textInput = input(&quot;Enter Your Text : &quot;) # Input<br>splittedText = textInput.split(&quot; &quot;) # List<br>tempWordlistSecretCode = list() # Temporary List<br>for word in splittedText:<br>    splittedWord = [*word]<br>    poppedLetter = splittedWord.pop(0)<br>    splittedWord.append(poppedLetter)<br><br>    randChars1 = generateRandChar(3)<br>    randChars2 = generateRandChar(3)<br>    splittedWord.append(randChars1)<br>    splittedWord.insert(0,randChars2)<br><br>    joinedWord = &quot;&quot;.join(splittedWord)<br>    tempWordlistSecretCode.append(joinedWord)<br>print(f&quot;Secret Code Generated Below \n&gt;&gt;&gt; {&#39; &#39;.join(tempWordlistSecretCode)}&quot;)<br>inp = input(&quot;Do You wanna Decompile your Secret code? [Y/N]] : &quot;).capitalize()<br>tempWordlistDecompiledCode = list()<br>if inp == &quot;Y&quot;:<br>    for word in tempWordlistSecretCode:<br>        splittedWord = [*word]<br>        removedRandChar = splittedWord[3:-3]<br>        poppedLetter = removedRandChar.pop()<br>        removedRandChar.insert(0,poppedLetter)<br>        joinedWord = &quot;&quot;.join(removedRandChar)<br>        tempWordlistDecompiledCode.append(joinedWord)<br>print(f&quot;Decompiled Secret Code Below \n&gt;&gt;&gt; {&#39; &#39;.join(tempWordlistDecompiledCode)}&quot;)
