import tkinter as tk<br>from tkinter import messagebox<br><br>class KBCGameGUI:<br>    def __init__(self, master):<br>        self.master = master<br>        self.master.title(&quot;KBC Game&quot;)<br>        self.current_question_index = 0<br>        self.score = 0<br><br>        self.questions = [<br>            [&quot;Which language was used to create Facebook&quot;, &quot;Java&quot;, &quot;Python&quot;, &quot;HTML&quot;, &quot;Php&quot;, &quot;None&quot;, 4],<br>            <br>            [&quot;Which is biggest planet&quot;,&quot;Earth&quot;,&quot;Sturn&quot;,&quot;Pluto&quot;,&quot;Mars&quot;,&quot;Jupyter&quot;,5],<br>    <br>            [&quot;Which is hotest planet in our solar system&quot;,&quot;Earth&quot;,&quot;Mercury&quot;,&quot;Pluto&quot;,&quot;Mars&quot;,&quot;Jupyter&quot;,2],<br><br>            [&quot;Who is the founder of Neurolink Inc&quot;,&quot;Elone Musk&quot;,&quot;Warren Buffet&quot;,&quot;Mark Zukerberg&quot;,&quot;Satoshi Nakamoto&quot;,&quot;Stevejobs&quot;,1],<br><br>            [&quot;Which is inventor of crypto currency Bitcoin&quot;,&quot;Satoshi Nakamoto&quot;,&quot;Elien Wang&quot;,&quot;Binance&quot;,&quot;Hamlet Steve&quot;,&quot;James&quot;,1],<br><br>            [&quot;What is formula of Sodium chloride&quot;,&quot;NACL&quot;,&quot;CH3&quot;,&quot;H20&quot;,&quot;CHE3COOH&quot;,&quot;H2SO4&quot;,1],<br><br>            [&quot;Which Crypto Currency has hightest value&quot;,&quot;Etherium&quot;,&quot;BitCoin&quot;,&quot;XRP&quot;,&quot;Solana&quot;,&quot;PEPE&quot;,2],<br><br>            [&quot;Which is richest man on earth&quot;,&quot;Jeff Besos&quot;,&quot;Bill Gates&quot;,&quot;Warren Buffet&quot;,&quot;Hamlet Steve&quot;,&quot;Elone Musk&quot;,5],<br><br>            [&quot;Which won cricket worldcup 2023&quot;,&quot;Pakistan&quot;,&quot;Australia&quot;,&quot;India&quot;,&quot;Sirilanka&quot;,&quot;Bangladesh&quot;,5],<br>            # ... Add more questions<br>        ]<br><br>        self.levels = [2000, 3000, 4000, 5000, 6000, 7000, 8000, 9000, 10000]<br><br>        self.create_widgets()<br><br>    def create_widgets(self):<br>        self.master.configure(bg=&#39;<a href=""http://www.youtube.com/results?search_query=%23e6f7ff"">#e6f7ff</a>&#39;)  # Set background color<br><br>        self.question_label = tk.Label(<br>            self.master, text=&quot;&quot;, font=(&quot;Helvetica&quot;, 14), wraplength=400, justify=&quot;center&quot;, bg=&#39;<a href=""http://www.youtube.com/results?search_query=%23e6f7ff"">#e6f7ff</a>&#39;)  # Set question label color<br>        self.question_label.pack(pady=20)<br><br>        self.option_buttons = []<br>        for i in range(1, 6):<br>            button = tk.Button(<br>                self.master, text=&quot;&quot;, command=lambda i=i: self.check_answer(i), bg=&#39;<a href=""http://www.youtube.com/results?search_query=%2380c7e6"">#80c7e6</a>&#39;)  # Set button color<br>            button.pack(pady=5)<br>            self.option_buttons.append(button)<br><br>        self.next_question_button = tk.Button(<br>            self.master, text=&quot;Next Question&quot;, command=self.next_question, bg=&#39;<a href=""http://www.youtube.com/results?search_query=%2399ffcc"">#99ffcc</a>&#39;)  # Set button color<br>        self.next_question_button.pack(pady=20)<br><br>        self.update_question()<br><br>    def update_question(self):<br>        question = self.questions[self.current_question_index]<br>        self.question_label.config(<br>            text=f&quot;{question[0]}\n\n1: {question[1]}\n2: {question[2]}\n3: {question[3]}\n4: {question[4]}\n5: {question[5]}&quot;)<br><br>        for i in range(5):<br>            self.option_buttons[i].config(text=question[i + 1])<br><br>    def check_answer(self, answer):<br>        correct_answer = self.questions[self.current_question_index][-1]<br><br>        if answer == correct_answer:<br>            messagebox.showinfo(&quot;Correct!&quot;, f&quot;Congratulations! You&#39;ve won Rs {self.levels[self.current_question_index]}&quot;)<br>            self.score += self.levels[self.current_question_index]<br>        else:<br>            messagebox.showinfo(<br>                &quot;Incorrect!&quot;, f&quot;Sorry, the correct answer was option {correct_answer}. Your final score is Rs {self.score}&quot;)<br>            self.reset_game()<br><br>    def next_question(self):<br>        self.current_question_index += 1<br><br>        if self.current_question_index &lt; len(self.questions):<br>            self.update_question()<br>        else:<br>            messagebox.showinfo(&quot;Game Over&quot;, f&quot;Congratulations! You&#39;ve completed the game. Your final score is Rs {self.score}&quot;)<br>            self.reset_game()<br><br>    def reset_game(self):<br>        self.current_question_index = 0<br>        self.score = 0<br>        self.update_question()<br><br><br>if <i>_name_</i> == &quot;__main__&quot;:<br>    root = tk.Tk()<br>    root.geometry(&quot;500x500&quot;)  # Set window size<br>    game_gui = KBCGameGUI(root)<br>    root.mainloop()
questions = [<br>    <br>    [&quot;Which language was used to create Facebook&quot;,&quot;Java&quot;,&quot;Python&quot;,&quot;HTML&quot;,&quot;Php&quot;,&quot;None&quot;,4],<br>    <br>    [&quot;Which is biggest planet&quot;,&quot;Earth&quot;,&quot;Sturn&quot;,&quot;Pluto&quot;,&quot;Mars&quot;,&quot;Jupyter&quot;,5],<br>    <br>    [&quot;Which is hotest planet in our solar system&quot;,&quot;Earth&quot;,&quot;Mercury&quot;,&quot;Pluto&quot;,&quot;Mars&quot;,&quot;Jupyter&quot;,2],<br>    <br>    [&quot;Who is the founder of Neurolink Inc&quot;,&quot;Elone Musk&quot;,&quot;Warren Buffet&quot;,&quot;Mark Zukerberg&quot;,&quot;Satoshi Nakamoto&quot;,&quot;Stevejobs&quot;,1],<br>    <br>    [&quot;Which is inventor of crypto currency Bitcoin&quot;,&quot;Satoshi Nakamoto&quot;,&quot;Elien Wang&quot;,&quot;Binance&quot;,&quot;Hamlet Steve&quot;,&quot;James&quot;,1],<br>    <br>    [&quot;What is formula of Sodium chloride&quot;,&quot;NACL&quot;,&quot;CH3&quot;,&quot;H20&quot;,&quot;CHE3COOH&quot;,&quot;H2SO4&quot;,1],<br>    <br>    [&quot;Which Crypto Currency has hightest value&quot;,&quot;Etherium&quot;,&quot;BitCoin&quot;,&quot;XRP&quot;,&quot;Solana&quot;,&quot;PEPE&quot;,2],<br>    <br>    [&quot;Which is richest man on earth&quot;,&quot;Jeff Besos&quot;,&quot;Bill Gates&quot;,&quot;Warren Buffet&quot;,&quot;Hamlet Steve&quot;,&quot;Elone Musk&quot;,5],<br>    <br>    [&quot;Which won cricket worldcup 2023&quot;,&quot;Pakistan&quot;,&quot;Australia&quot;,&quot;India&quot;,&quot;Sirilanka&quot;,&quot;Bangladesh&quot;,5],<br>]<br>levels = [2000,3000,4000,5000,6000,7000,8000,9000,10000]<br>for i in range (0,len(questions)):<br>    question = questions[i]<br>    <a href=""http://www.youtube.com/results?search_query=%23print"">#print</a> (question[-1])<br>    print (f&quot;\n\nQuestion for Rs{levels[i]}&quot;)<br>    print (f&quot;{question[0]}    1:{question[1]}, 2:{question[2]}, 3:{question[3]}, 4:{question[4]}, 5:{question[5]}&quot; )<br>    Answer = int(input(&quot;Enter your answer between (1 - 5) &quot;))<br>    if(Answer == question[len(question)-1]):<br>        print (f&quot;Congratulations you have won RS, {levels[i]} &quot;)<br>    else:<br>        print(&quot;Wrong Answer you have lost the game!&quot;)<br>        break
