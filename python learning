import random<br>z=(input(&quot;Enter your name: &quot;))<br><br>print(f&#39;&#39;&#39; Lets play <br>      Snake Gun Water GAme<br>            AI vs {z} &#39;&#39;&#39;)<br>print(&quot;1.Snake&quot;)<br>print(&quot;2.Gun&quot;)<br>print(&quot;3.Water&quot;)<br>numb_game=3<br><br>ai_win=0<br>player_win=0<br>ties=0<br>for i in range(numb_game):<br>    x=random.randint(1,3)<br><br>    try:<br>        y=int(input(&quot;Enter your choice: &quot;))<br>        if (y&lt;1 and y&gt;3):<br>            raise ValueError(&quot;Please correct number&quot;)<br>    except ValueError:<br>        print(&quot;Invaild number.&quot;)<br>    else:<br>        print(f&#39;AI is {x}&#39;)<br>        print(f&#39;{z} are {y}&#39;)<br>    if x==y:<br>        print(&quot;TIE&quot;)<br>        ties=ties+1<br>    elif (x==1 and y==2) or (x==2 and y==3) or (x==3 and y==1):<br>        print(&quot;AI win&quot;)<br>        ai_win+=1<br>    else:<br>        print(f&quot;{z} win&quot;)<br>        player_win+=1<br>print(&quot;Game over result is: &quot;)<br>print(f&quot;AI result is:{ai_win} &quot;)<br>print(f&quot;{z} result is:{player_win}&quot;)<br>print(&quot;tie&quot;,ties)<br>if ai_win&gt;player_win:<br>    print(&quot;AI IS THE WINNNER&quot;)<br>elif ai_win&lt;player_win:<br>    print(f&quot;{z} IS THE WINNER&quot;)<br>else:<br>    print(&quot;Match is tie&quot;)
