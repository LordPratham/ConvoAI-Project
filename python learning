class Library:<br>    Books={}<br>    User=eval(input(&quot;Enter Items in Key:Values pairs: &quot;))<br>    <br>        <br>    Updater=Books.update(User)<br>    <br>    A=Books.keys()<br>    B=Books.values()<br><br><br>Roshan=Library<br>print(Roshan.Books)<br>print(&quot;Your input Keys:&quot;)<br>print(Roshan.A)<br>print(&quot;Your input Values:&quot;)<br>print(Roshan.B)
