class Person:<br><br>  def __init__(self, name):<br>    <a href=""http://self.name/"">self.name</a> = name<br><br>  def __str__(self):<br>    return f&quot;The name is {self.name}&quot;<br><br><br>class First_child(Person):<br><br>  def __init__(self, kid_name):<br>    print(<br>      f&#39;The Child name is {self.kid_name} and the dad name is {super().name}&#39;)<br><br><br>class Second_child(Person, First_child):<br><br>  def __init__(self, kid_name):<br>    print(<br>      f&#39;The Child name is {self.kid_name} , the dad name is {super().name} and the Brother name is {super().kid_name}&#39;<br>    )<br><br><br>parent = Person(&quot;Daddy&quot;)<br>child_one = First_child(&quot;Rohit&quot;)<br>child_two = Second_child(&quot;Mohit&quot;)
