import os<br><br>def organize(type):<br>    listOfType = [file for file in os.listdir() if file.endswith(type)]<br><br>    for index, item in enumerate(listOfType, start=1):<br>        print(item)<br>        os.rename(item, str(index) + type)<br><br>organize(&#39;.png&#39;)<br><br># Done
