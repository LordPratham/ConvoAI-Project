def fibonacci(n):<br>    if n&lt;= 0:<br>        return[]<br>    elif n== 1:<br>        return[0]<br>    elif n== 2:<br>        return[0, 1]<br>    else:<br>        fib= [0, 1]<br>        for i in range(2,n):<br>            fib.append(fib[i-1]+fib[i-2])<br>        return fib
