import random <br>b=&#39;S&#39;,&#39;W&#39;,&#39;G&#39;<br>a=random.choices(b)<br><br><br>for i in range(10):<br><br> c=list(input(&quot;enter a char S/W/G \n&quot;))  <br><br> if(c==a):<br>    print(&#39;draw&#39;)<br> elif(c==&#39;S&#39; and a==&#39;W&#39;):<br>    print(&#39;you win&#39;)<br> elif(c==&#39;S&#39; and a==&#39;G&#39;):<br>    print(&#39;you win&#39;)<br> elif(c==&#39;W&#39; and a==&#39;G&#39;):<br>    print(&#39;you loose&#39;)<br> elif(c==&#39;W&#39; and a==&#39;S&#39;):<br>    print(&#39;you win&#39;)<br> elif(c==&#39;G&#39; and a==&#39;W&#39;):<br>    print(&#39;you loose&#39;)<br> else:<br>    print(&#39;you win&#39;)<br>    <br> print(&#39;COMPUTER SELECTED&#39;,a)<br><br><br> x=input(print(&#39;Wanna play more Y/N&#39;))<br> if(x==&#39;Y&#39;):<br>  continue<br> else:<br>    break
import random<br>def result(w1,w2):<br>  arr_of_res=[[&#39;draw&#39;,&#39;lose&#39;,&#39;win&#39;],[&#39;win&#39;,&#39;draw&#39;,&#39;lose&#39;],[&#39;lose&#39;,&#39;win&#39;,&#39;draw&#39;]]<br>  for i in range(3):<br>    for j in range(3):<br>      if(i==w2 and j==w1):<br>        res=arr_of_res[i][j]<br>  return res<br>  <br>def game(player1):<br>  moves=[&#39;w&#39;,&#39;g&#39;,&#39;s&#39;]<br>  player2=random.choice(moves)<br>  print(f&quot;Opposition&#39;s Move: {player2}\n&quot;)<br>  for index,w in enumerate(moves):<br>    if(player1==w):<br>      w1=index<br>    if(player2==w):<br>      w2=index<br>  print(result(w1,w2))<br>  <br>if __name__==&quot;__main__&quot;:<br><br>  print((&quot;Welcome to Snake,Water,Gun game\n&quot;).center(50))<br>  print((&quot;rules:\tsnake- &#39;s&#39;\n\twater- &#39;w&#39;\n\tgun- &#39;g&#39;&quot;).center(50))<br>  while(True):<br>    for i in range(3):<br>      you=input(&quot;\nYour Move: &quot;)<br>      game(you)<br>    c=int(input(&quot;\ndo you want to play more? if yes press 1 else press 0\n&quot;))<br>    if(c!=1):<br>      print((&quot;__Game Over__&quot;).center(50))<br>      break
