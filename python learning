import random<br>def RandCode():<br>    codeLetters = &quot;qwertyuiopasdfghjklzxcvbnm&quot;<br>    code = &#39;&#39;<br>    for i in range(3):<br>        code = code + random.choice(codeLetters)<br>    return code<br>        <br>def dataEncoding(data):<br>    encode = &quot; &quot;<br>    for word in data.split(&quot; &quot;):<br>        if len(word) &gt; 3:<br>            temp = word[0]<br>            word = word[1:]<br>            word = word + temp<br>            encode = encode + RandCode() + word + RandCode() + &quot; &quot;<br>        else:<br>            encode = encode + word[::-1] + &quot; &quot;<br>    else:<br>        print(encode.strip())<br><br>data = &quot;Hello gourav ohh hii what is going on sachin nothing special bro are you going to college now&quot;<br><br>print(&quot;Data encoding\n&quot;)<br>dataEncoding(data)
