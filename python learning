n1=int(input(&quot;Enter a number&quot;))<br>n2=int(input(&quot;Enter another number&quot;))<br><br>calc=int(input(&quot;1:Add\n2:subtract\n3:multiply\n4:divide\n&quot;))<br>if(calc==1):<br>    print(n1+n2)<br>elif(calc==2):<br>    print(n1-n2)<br>elif(calc==3):<br>    print(n1*n2)<br>elif(calc==4):<br>    print(n1/n2)<br>else:<br>    print(&quot;error&quot;)
