def Bubble_sort(n):<br>    for i in range(len(n)-1,0,-1):<br>        for j in range(i):<br>            if n[j]&gt;n[j+1]:<br>                n[j],n[j+1]=n[j+1],n[j]<br>    print(n)<br>c=int(input(&quot;Enter the number of elements in list:&quot;))<br>n=[]<br>for k in range(c):<br>    d=int(input(&quot;Enter an value:&quot;))<br>    n.append(d)<br>print(n)<br>print(&quot;The sorted list is&quot;,Bubble_sort(n))<br><br><br><br>&quot;small modification&quot;
