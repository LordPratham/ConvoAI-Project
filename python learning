import random<br>lst = [&#39;s&#39;,&#39;w&#39;,&#39;g&#39;]<br>while True:<br>  choice = input(&quot;s for snake, w fo water, g for gun:&quot;)<br>  if choice not in (&#39;s&#39;,&#39;w&#39;,&#39;g&#39;):<br>    raise ValueError(&quot;Value should be either &#39;s&#39; or &#39;w&#39; or &#39;g&#39;&quot;)<br>  cC = random.choice(lst)<br>  if choice == cC:<br>    print(&quot;It&#39;s a draw&quot;)<br>    continue<br>  else:<br>    match choice:<br>      case &#39;s&#39; if cC == &#39;w&#39;:<br>        print(f&quot;You won, computer chose {cC}&quot;)<br>      case &#39;w&#39; if cC == &#39;g&#39;:<br>        print(f&quot;You won, computer chose {cC}&quot;)<br>      case &#39;g&#39; if cC == &#39;s&#39;:<br>          print(f&quot;You won, computer chose {cC}&quot;)<br>      case _:<br>        print(f&quot;You lost, computer chose {cC}&quot;)<br>    break
import random<br>a = int(input(&quot;0 for Snake, 1 for water and 2 for Gun&quot;))<br>n = int(random.randint(0,2))<br>if(a==0 and n==1):<br>    print(&quot;You Won&quot;)<br>elif(a == 1 and n == 2):<br>    print(&quot;You Won&quot;)<br>elif(a==2 and n==0):<br>    print(&quot;You Won&quot;)<br>elif(a == 0 and n==1):<br>    print(&quot;You Won&quot;)<br><a href=""http://www.youtube.com/results?search_query=%23lose"">#lose</a> condition-------------------------------------------------------------------<br>if(a==1 and n==0):<br>    print(&quot;You Lose&quot;)<br>elif(a == 2 and n == 1):<br>    print(&quot;You Lose&quot;)<br>elif(a==0 and n==2):<br>    print(&quot;You Lose&quot;)<br>elif(a == 1 and n==0):<br>    print(&quot;You Lose&quot;)<br><br>## Draw conditiion----------------------------------------------------------------<br>elif(n==1 and a ==1):<br>    print(&quot;It&#39;s a draw&quot;)<br>elif(a==1 and n ==1):<br>    print(&quot;It&#39;s a draw&quot;)<br>elif(n==2 and a ==2):<br>    print(&quot;It&#39;s a draw&quot;)<br>elif(a==2 and n ==2):<br>    print(&quot;It&#39;s a draw&quot;)<br>elif(a==0 and n ==0):<br>    print(&quot;It&#39;s a draw&quot;)<br>elif(n==0 and a ==0):<br>    print(&quot;It&#39;s a draw&quot;)<br>print(f&quot;You:{a}&quot;)<br>print(f&quot;Computer:{n}&quot;)
import random<br><br>def check(user, rand_value):<br>    if user==rand_value:<br>        return &quot;Match Drawn !&quot;<br><br>    elif ((user==0 and rand_value==1) or (user==1 and rand_value==2) or (user==2 and rand_value==0)):<br>        return &quot;You Won !&quot;<br>    <br>    else:<br>        return &quot;Sorry You Lose !&quot;<br><br>input_value = int(input(&quot;Please Enter \n0 For SNAKE \n1 For WATER \n2 For GUN : &quot;))<br>com = random.randint(0,2)<br><br>game = check(input_value, com)<br>print(f&quot;{game} Cause You Entered Value : {input_value} and Computer Value Is : {com}&quot;)<br><br># UC<br># 00 Draw 22 Drawn 11 Draw<br># 01 win 12 win 20 win<br># 02 loss 10 loss 21 loss
import random<br><br>def playerPlays():<br>    return input(&quot;Choose between rock, paper, and scissors: &quot;)<br><br>def computerPlays():<br>    choices = [&quot;rock&quot;, &quot;paper&quot;, &quot;scissors&quot;]<br>    return random.choice(choices)<br><br>def getGameResult(player_choice, computer_choice):<br>    rules = {<br>        &quot;rock&quot;: {&quot;rock&quot;: &quot;It&#39;s a draw&quot;, &quot;paper&quot;: &quot;You lose&quot;, &quot;scissors&quot;: &quot;You win&quot;},<br>        &quot;paper&quot;: {&quot;rock&quot;: &quot;You win&quot;, &quot;paper&quot;: &quot;It&#39;s a draw&quot;, &quot;scissors&quot;: &quot;You lose&quot;},<br>        &quot;scissors&quot;: {&quot;rock&quot;: &quot;You lose&quot;, &quot;paper&quot;: &quot;You win&quot;, &quot;scissors&quot;: &quot;It&#39;s a draw&quot;}<br>    }<br>    return rules[player_choice][computer_choice]<br><br>while True:<br>    player_choice = playerPlays()<br>    computer_choice = computerPlays()<br>    print(f&quot;Computer plays: {computer_choice}.&quot;)<br><br>    game_play = getGameResult(player_choice, computer_choice)<br>    print(game_play)<br><br>    if &quot;lose&quot; in game_play:<br>        print(f&quot;You lost to the computer. Thanks for playing, {name}!&quot;)<br>        break
