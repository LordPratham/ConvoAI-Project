import random<br>questions=[&quot;president of India&quot;,&quot;Indian cricket caption&quot;,&quot;full form of AIR&quot;,&quot;prime minister of india&quot;,&quot;National Anthum&quot;,&quot;national bird&quot;,&quot;national flower&quot;,&quot;hattori&#39;s brother&quot;,&quot;nobita&#39;s crush&quot;,&quot;Giyan&#39;s sister&quot;]<br>answers=[&quot;dropadi murmur&quot;,&quot;rohit sharma&quot;,&quot;All India Radio&quot;,&quot;narendra modi&quot;,&quot;Jan Gan Man&quot;,&quot;peacock&quot;,&quot;Lotus&quot;,&quot;sinzo&quot;,&quot;sizuka&quot;,&quot;Jaiko&quot;]<br>l = len(questions)<br>count = 0<br>i=0<br><br>while(i&lt;=l):<br>    print(&quot;questions:&quot;,i+1)<br>    print(questions[i])<br>    option = random.sample(answers,3)<br>    option.append(answers[i])<br>    random.shuffle(option)<br>    print(&quot;a.&quot;,option[0])<br>    print(&quot;b.&quot;,option[1])<br>    print(&quot;c.&quot;,option[2])<br>    print(&quot;d.&quot;,option[3])<br>    ch = input(&quot;enter your choice:&quot;)<br>    if(ch==&quot;a&quot;):<br>        if(questions.index(questions[i])==answers.index(option[0])):<br>            print(&quot;----------congratulations, you win---------&quot;)<br>            count = count + 1000<br>            i = i+1<br>        else:<br>            print(&quot;you lose&quot;)<br>            break<br>    elif(ch==&quot;b&quot;):<br>        if(questions.index(questions[i])==answers.index(option[1])):<br>            print(&quot;----------congratulations, you win---------&quot;)<br>            count = count + 1000<br>            i = i+1<br>        else:<br>            print(&quot;you lose&quot;)<br>            break    <br>    elif(ch==&quot;c&quot;):<br>        if(questions.index(questions[i])==answers.index(option[2])):<br>            print(&quot;----------congratulations, you win---------&quot;)<br>            count = count + 1000<br>            i = i+1<br>        else:<br>            print(&quot;you lose&quot;)<br>            break   <br>    elif(ch==&quot;d&quot;):<br>        if(questions.index(questions[i])==answers.index(option[3])):<br>            print(&quot;----------congratulations, you win---------&quot;)<br>            count = count + 1000<br>            i = i+1<br>        else:<br>            print(&quot;you lose&quot;)<br>            break   <br>    else:<br>        print(&quot;error&quot;)<br>        break<br>print(&quot;you win&quot;,count,&quot;rupees.&quot;)
