def smart_div(func):<br><br>    def inner(a,b):<br>        if a&lt;b:<br>            a,b = b,a<br>        return func(a,b)<br>    return inner<br><br>@smart_div<br>def div(a,b):<br>    <br>    print(a/b)<br><br>div(2,4)
