from PyPDF2 import PdfMerger<br>from os import listdir<br><br><br>merger = PdfMerger()<br>pdf = listdir()<br># print(pdf.index(&#39;project37.py&#39;))<br>pdf.pop(2)<br>for i in pdf:<br>    merger.append(i)<br>merger.write(&#39;NewMerged.pdf&#39;)<br>merger.close()
from PyPDF2 import PdfMerger<br><br># mk a list<br>pdfs1 = [&quot;1.pdf&quot;, &quot;2.pdf&quot;, &quot;3.pdf&quot;, &quot;4.pdf&quot;, &quot;5.pdf&quot;]<br># mk an instance<br>merger = PdfMerger()<br>for pdf in pdfs1:<br>    merger.append(pdf)<br><br>merger.write(&quot;merged.pdf&quot;)<br>merger.close()
ANSWER<br><br><br><br><br># Creating a pdf files merger<br>from PyPDF2 import PdfWriter<br>import os<br><br>filePath = input(&quot;Enter The file path where is your PDFs : &quot;)<br><br>merger = PdfWriter()<br>files = os.listdir(filePath)<br>for file in files:<br>    merger.append(f&quot;{filePath}/{file}&quot;)<br><br>merger.write(&quot;finalMerged.pdf&quot;)<br>merger.close()
