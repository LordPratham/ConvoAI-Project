class Library:<br>    def __init__(self):<br>        self.books = []<br><br>    def print_all_books(self):<br>        if self.books:<br>            print(&quot;Books in the library:&quot;)<br>            for book in self.books:<br>                print(book)<br>        else:<br>            print(&quot;The library is empty.&quot;)<br><br>    def add_book(self, book):<br>        self.books.append(book)<br>        print(f&quot;Book &#39;{book}&#39; added to the library.&quot;)<br><br>    def get_number_of_books(self):<br>        return len(self.books)<br><br><br># Example usage<br>library = Library()<br><br>library.print_all_books()  # The library is empty.<br><br>library.add_book(&quot;THE LORD OF THE RINGS&quot;)<br>library.add_book(&quot;LIFE OF PI&quot;)<br>library.add_book(&quot;PABLO ESCOBAR&quot;)<br><br>library.print_all_books()<br># Books in the library:<br># Book 1<br># Book 2<br># Book 3<br><br>num_books = library.get_number_of_books()<br>print(&quot;Number of books:&quot;, num_books)  # Number of books: 3
