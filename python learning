def my_addition(a,b):<br>  return(a+b)<br>  <br>def my_subtraction(a,b):<br>  return(a-b)<br><br>def my_multiplication(a,b):<br>  return(a*b)<br><br>def my_division(a,b):<br>  return(a/b)<br>  <br># def storeAnswer():<br>    <br>first = int(input(&quot;Enter the first number&quot;))<br>while_condition = True<br><br>while(while_condition):<br>    <br>    operation = input(&quot;select the operation&quot;)<br>    if(operation== &quot;exit&quot;):<br>        while_condition = False<br>        # break<br>    else:    <br>        second = int(input(&quot;Enter the second number&quot;))<br>        match operation:<br>            case &#39;+&#39;:<br>                first = c = my_addition(first,second)<br>                print(c)<br>                <br>            case &#39;-&#39;:<br>                first = c = my_subtraction(first,second)<br>                print(c)<br><br>            case &#39;*&#39;:<br>                first = c = my_multiplication(first,second)<br>                print(c)<br><br>            case &#39;/&#39;:<br>                first = c = my_division(first,second)<br>                print(c)
