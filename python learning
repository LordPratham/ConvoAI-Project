def fib(n):<br>    a = 0<br>    b = 1<br>    if n &lt;= 0:<br>        print(&#39;{} is an invalid term for fibonacci range&#39;.format(n))<br>    elif n == 1:<br>        print(a)<br>    else:<br>        print(a)<br>        print(b)<br>        for i in range(2, n):<br>            c = a + b<br>            a = b<br>            b = c<br>            print(c)<br>            if a + b &gt; 100:<br>                print(&#39;This function\&#39;s result set can only handle fibonacci numbers up to {}&#39;.format(c))<br>                break;
