class Library:<br>    def books(self, no):<br>        lst2 = []<br>        lst1 = []<br>        for i in range(no):<br>            book_name = (input(&quot;enter name of books: &quot;))<br>            stream = (input(&quot;enter book type: &quot;))<br>            if stream == &quot;science&quot;:<br>                lst1.append(book_name)<br>            elif stream == &quot;literature&quot;:<br>                lst2.append(book_name)<br>            else:<br>                print(&quot;Invalid book type! Please enter &#39;science&#39; or &#39;literature&#39;.&quot;)<br>                continue<br>        print(&quot;science BOOKS ARE :&quot; ,lst1)<br>        print(&quot;literature BOOKS ARE :&quot; ,lst2)<br>        return lst1, lst2<br><br><br>class ComparisonLibrary(Library):                   <br><br>    def compare_books(self, no, lst1, lst2):<br>        if no == len(lst1) + len(lst2):<br>            print(&quot;books added same as books entered initially to be added &quot;)<br>        else:<br>            print(&quot;missing books!!!&quot;)<br><br><br>no = int(input(&quot;enter the n.o of books u wanna enter:&quot;))<br>Library_items = ComparisonLibrary()<br>lst1, lst2 = Library_items.books(no)<br>Library_items.compare_books(no,lst1,lst2)
