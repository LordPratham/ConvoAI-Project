from plyer import notification<br>from threading import Timer<br><br><br><br>def reminder():<br>    notification.notify(title=&quot;Reminder&quot;, message=&quot;DRINK WATER BRO..!!&quot;, timeout=10)<br><br>aa=0<br>ab= 24 <a href=""http://www.youtube.com/results?search_query=%23for"">#for</a> 1 day == 24<br><br>while (aa&lt;ab):<br>    timer = Timer(3600, reminder)<br>    timer.start()<br>    aa += 1
