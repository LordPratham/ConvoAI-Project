from pypdf import PdfMerger<br><br># Create an instance of PdfFileMerger<br>merger = PdfMerger()<br><br># Add PDF files to be merged<br>pdf_files = [&quot;download.pdf&quot;, &quot;Assignment1.pdf&quot;]<br>for file in pdf_files:<br>    merger.append(file)<br><br># Specify the output file name<br>output = &quot;merged1.pdf&quot;<br><br># Merge the PDF files<br>merger.write(output)<br><br># Close the merger object<br>merger.close()<br><br>print(&quot;PDF files merged successfully!&quot;)
