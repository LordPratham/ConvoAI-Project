a = int(input(&quot;Enter the first number: &quot;))<br>b = int(input(&quot;Enter the Second number: &quot;))<br><br>x = int(input(&quot;&quot;&quot;1. Enter 1 for add<br>2. Enter 2 for Sub<br>3. Enter 3 for Div<br>4. Enter 4 for Mul\n&quot;&quot;&quot;))<br><br>match x:<br>    case 1:<br>        c = a + b<br>        print (&quot;Add is: &quot;, c)<br>    case 2:<br>        c = a - b<br>        print (&quot;Sub is: &quot;, c)<br>    case 3:<br>        c = a / b<br>        print (&quot;Div is: &quot;, c)<br>    case 4:<br>        c = a * b<br>        print (&quot;Mul is: &quot;, c)<br>    case _:<br>        print(&quot;Enter the correct option&quot;)
