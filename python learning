import random<br><br>def choice(x):<br>    if x &lt; 33:<br>        return 0  # one for snake<br>    if 33 &lt; x &lt; 66:<br>        return 1  # one for water<br>    if 66 &lt; x:<br>        return 2  # one for snake<br><br>decision_matrix = [[0, 1, -1], [-1, 0, 1], [1, -1, 0]]<br><br>while True:<br>    x = random.randint(0, 100)<br>    computer_choice = choice(x)<br>    your_choice = int(input(&quot;Enter 0 for &#39;Snake&#39;, 1 for &#39;Water&#39;, 2 for &#39;Gun&#39; : &quot;))<br>    result = decision_matrix[computer_choice][your_choice] # type: ignore<br>    print(f&quot;computer choice is {computer_choice} and your choice is {your_choice}&quot;)<br>    if result == 0:<br>        print(&quot;Game Draw&quot;)<br>    elif result == 1:<br>        print(&quot;You Win&quot;)<br>        break<br>    else:<br>        print(&quot;You Lose&quot;)<br>        break
import random<br><br>def play_game():<br>    choices = [&quot;snake&quot;, &quot;water&quot;, &quot;gun&quot;]<br>    user_score = 0<br>    comp_score = 0<br>    num_rounds = int(input(&quot;Enter the number of rounds you want to play: &quot;))<br>    <br>    for i in range(num_rounds):<br>        user_choice = input(&quot;Enter your choice (snake, water, gun): &quot;).lower()<br>        comp_choice = random.choice(choices)<br>        print(f&quot;Round {i+1}: You chose {user_choice}, computer chose {comp_choice}&quot;)<br><br>        if user_choice == &quot;snake&quot;:<br>            if comp_choice == &quot;water&quot;:<br>                print(&quot;You win! Snake drank water.&quot;)<br>                user_score += 1<br>            elif comp_choice == &quot;gun&quot;:<br>                print(&quot;You lose! Gun killed snake.&quot;)<br>                comp_score += 1<br>            else:<br>                print(&quot;It&#39;s a draw!&quot;)<br><br>        elif user_choice == &quot;water&quot;:<br>            if comp_choice == &quot;snake&quot;:<br>                print(&quot;You lose! Snake drank water.&quot;)<br>                comp_score += 1<br>            elif comp_choice == &quot;gun&quot;:<br>                print(&quot;You win! Gun drowned in water.&quot;)<br>                user_score += 1<br>            else:<br>                print(&quot;It&#39;s a draw!&quot;)<br><br>        elif user_choice == &quot;gun&quot;:<br>            if comp_choice == &quot;snake&quot;:<br>                print(&quot;You win! Gun killed snake.&quot;)<br>                user_score += 1<br>            elif comp_choice == &quot;water&quot;:<br>                print(&quot;You lose! Gun drowned in water.&quot;)<br>                comp_score += 1<br>            else:<br>                print(&quot;It&#39;s a draw!&quot;)<br><br>        else:<br>            print(&quot;Invalid input! You lose this round.&quot;)<br>            comp_score += 1<br>    <br>    print(&quot;\nGame over!&quot;)<br>    if user_score &gt; comp_score:<br>        print(f&quot;You won {user_score} rounds and the computer won {comp_score} rounds.&quot;)<br>    elif user_score &lt; comp_score:<br>        print(f&quot;The computer won {comp_score} rounds and you won {user_score} rounds.&quot;)<br>    else:<br>        print(f&quot;It&#39;s a draw! Both you and the computer won {user_score} rounds.&quot;)<br>      <br>play_game()
