def fibonacci(n):<br>    if (n==0):<br>        return 0<br>    elif (n==1):<br>        return 1<br>    else:<br>        return fibonacci(n-1)+fibonacci(n-2)<br>print(fibonacci(9))
def fab(n):  <a href=""http://www.youtube.com/results?search_query=%23fab"">#fab</a>=name of formula<br>    if n == 0:<br>        return 0    <a href=""http://www.youtube.com/results?search_query=%23taking"">#taking</a> default Fibonacci numbers <br>    if n == 1:<br>        return 1<br>    else:<br>        return fab(n - 1) + fab(n - 2)    <a href=""http://www.youtube.com/results?search_query=%23using"">#using</a> recursion<br><br>a = int(input(&quot;Enter the number you want: &quot;))  # Convert input to integer<br>print(fab(a))
def fib(n):<br>  if n == 0:<br>    print(0)<br>  elif n == 1:<br>    print(1)<br>  else:<br>    print((n-1)+(n-2))
def fib(n):<br>    if (n==0):<br>        return 0<br>    if (n==1):<br>        return 1<br>    else:<br>        return fib(n-1) + fib(n-2)<br><br>n = int(input(&quot;Please enter the desired fibonacci index number: &quot;))<br><br><br>print(fib(n))
def fibonacci(n):<br>    if n == 0:<br>        return 0<br>    elif n == 1:<br>        return 1<br>    else:<br>        return(fibonacci(n-1) + fibonacci(n-2))
def fibonacci(num):<br>    if (num == 0 or num == 1):<br>        return num<br>    else:<br>        return fibonacci(num - 2) + fibonacci(num - 1)<br><br><br>for i in range(11):<br>    print((fibonacci(i)), end=&quot; &quot;)
def fibonacci(num):<br>    if (num == 0 or num == 1):<br>        return num<br>    else:<br>        return fibonacci(num-2) + fibonacci(num - 1)<br><br><br>n = int(input(&quot;Enter the number: &quot;))<br>print(fibonacci(n))
