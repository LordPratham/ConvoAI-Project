def binary_search(nums, num):<br>    begin_index = 0<br>    end_index = len(nums) - 1<br><br>    while begin_index &lt;= end_index:<br>        <a href=""http://www.youtube.com/results?search_query=%23import"">#import</a> pdb;pdb.set_trace<br>        mid_point = (begin_index + (end_index - begin_index)//2)<br>        mid_point_value = nums[mid_point]<br>        if mid_point_value == num:<br>            return mid_point<br>        elif num &lt; mid_point_value:<br>            end_index = mid_point - 1<br>        else:<br>            num &gt; mid_point_value<br>            begin_index = mid_point + 1<br>    return None<br><br>nums = [2,4,5,6,7,8,9,10]<br>num = 6<br><br>print(binary_search(nums, num))
