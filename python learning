My code:<br>import random<br><br>class Secret_Language:<br>    <br>    def __init__(self):<br>        self.code = &quot;&quot;<br>        self.alphabet = &quot;abcdefghijklmnopqrstuvwxyz&quot;<br>        self.ask = &quot;&quot;<br>     <br>    def ask_code_or_decode(self):<br>        # Ask the user whether they want to code or decode<br>        ask = str(input(&quot;\nDo you wish to code or decode: &quot;))<br>        if ask == &quot;code&quot;:<br>            self.code = str(input(&quot;\nEnter Sentence or Word to Code: &quot;))<br>            # Split the input into words<br>            self.code = self.code.split()<br>            self.code_language()<br>        elif ask == &quot;decode&quot;:<br>            self.decode = str(input(&quot;\nEnter Sentence or word to Decode: &quot;))<br>            # Split the input into words<br>            self.decode = self.decode.split()<br>            self.decode_language()<br>        <br>    def code_language(self):<br>        modified_codes = []  # Create a list to store the modified codes<br>        for word in self.code:<br>            code = word<br>            alphabet = 0<br>            if len(code) &gt;= 3:<br>                # Appending first letter at the end<br>                modified_code = code[:0] + code[1:] + code[0]<br>                # Reversing code to append 3 random letters at the start<br>                modified_code = modified_code[::-1]<br>                # Producing the final code<br>                for i in range(3):<br>                    # Generate a random letter each time<br>                    random_alphabet = random.choice(self.alphabet)<br>                    # Appending random letters<br>                    modified_code = modified_code + random_alphabet<br>                    # Reversing code to append 3 random letters at the end<br>                modified_code = modified_code[::-1]<br>                for i in range(3):<br>                    # Generate a random letter each time<br>                    random_alphabet = random.choice(self.alphabet)<br>                    # Appending random letters<br>                    modified_code = modified_code + random_alphabet<br>            else:<br>                # If the word is too short, reverse it<br>                modified_code = code[::-1]<br>            # Store the modified code<br>            modified_codes.append(modified_code)<br>        # Return the modified codes<br>        print(&quot;\nConverted to Secret Code: &quot; ,&quot; &quot;.join(modified_codes)) <br>        <br>    def decode_language(self):<br>        # Create a list to store the modified codes<br>        modified_codes = []<br>        for word in self.decode:<br>            code = word<br>            if len(code) &gt;= 3:<br>                # Remove the first letter and the 3rd letter<br>                modified_code = code[:0] + code[3:]<br>                modified_code = modified_code[::-1]<br>                # Remove the last letter and the 4th from the end<br>                modified_code = modified_code[:0] + modified_code[3:]<br>                # Appending last letter at the start<br>                modified_code = modified_code[:0] + modified_code[1:] + modified_code[0]<br>                modified_code = modified_code[::-1]<br>            else:<br>                # If the word is too short, reverse it<br>                modified_code = code[::-1]<br>            # Store the modified code<br>            modified_codes.append(modified_code)<br>        # Return the modified codes<br>        print(&quot;\nDecoded from Secret Code: &quot; ,&quot; &quot;.join(modified_codes) )<br>         <br>x = Secret_Language()<br>x.ask_code_or_decode()
# code by gouranga-ls<br>import random<br>def help():<br>    print(&quot;\n&quot;)<br>    print(&quot;1. text to hex\n&quot;)<br>    print(&quot;2. hex to text\n&quot;)<br>    print(&quot;0. exit\n&quot;)<br><br>def reev(x):<br>  return x[::-1]<br><br>def addmehex(name):<br>    word_list = [&quot;a&quot;, &quot;b&quot;, &quot;c&quot;, &quot;i&quot;, &quot;e&quot;, &quot;f&quot;, &quot;g&quot;, &quot;w&quot;, &quot;k&quot;, &quot;n&quot;]<br>    random_words1 = random.sample(word_list, 3)<br>    random_words2 = random.sample(word_list, 3)<br>    nn1 = random_words1[0], random_words1[1], random_words1[2]<br>    nn2 = random_words2[0], random_words2[1], random_words2[2]<br>    nn1_str = &quot;&quot;.join(nn1)<br>    nn2_str = &quot;&quot;.join(nn2)<br>    print(&quot;Your text hex : &quot;,nn1_str, name, nn2_str,sep=&quot;&quot; )<br>    mainfun()<br><br>def t_hex(cov):<br>    name = reev(str(cov))<br>    print(f&quot;Your name is {cov}&quot;)<br>    addmehex(name)<br><br>def mainfun():<br>    help()<br>    x = input(&quot;choose option: &quot;)<br>    if (int(x) == 1):<br>        print(&quot;text to hex select\n&quot;)<br>        text = input(&quot;type your text: &quot;)<br>        if(len(text) &gt;= 5 ):<br>            t_hex(text)<br>        else:<br>            print(&quot;too sorted text min 5:word in text !!&quot;)<br>            mainfun()<br>    elif(int(x) == 2):<br>        print(&quot;hex to text select&quot;)<br>        text = input(&quot;type your text: &quot;)<br>        if(len(text) &gt;= 5 ):<br>            new_text = text[3:-3]<br>            reev_text = reev(new_text)<br>            print(f&quot;Your hex code: {reev_text}&quot;)<br>            mainfun()<br>        else:<br>            print(&quot;too sorted text min 5:word in text !!&quot;)<br>            mainfun()<br>    else:<br>        print(&quot;Your are exit !&quot;)<br>mainfun()
