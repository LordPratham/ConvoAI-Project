import random<br><br>Win=[]<br>choices=[&quot;Snake&quot;, &quot;Water&quot;, &quot;Gun&quot;]<br>scene=[[&quot;DRAW&quot;,&quot;WIN&quot;,&quot;LOOSE&quot;],[&quot;LOOSE&quot;,&quot;DRAW&quot;,&quot;WIN&quot;],[&quot;WIN&quot;,&quot;LOOSE&quot;,&quot;DRAW&quot;]]<br>points={&quot;S&quot;:0,&quot;W&quot;:1,&quot;G&quot;:2}<br><br>player1=input(&quot;Choose one from (Snake , Gun , Water) : &quot;)<br><br>player2=random.choice(choices)<br>print(f&quot;Player 2 selected :: {player2}&quot;)<br><br>print(&quot;You : &quot;, scene[points[player1[0]]][points[player2[0]]])
import random<br><br>def get_user_choice():<br>    print(&quot;Enter your choice: Snake (S), Water (W), Gun (G)&quot;)<br>    user_choice = input().upper()<br>    if user_choice not in [&#39;S&#39;, &#39;W&#39;, &#39;G&#39;]:<br>        print(&quot;Invalid choice. Please enter S for Snake, W for Water, or G for Gun.&quot;)<br>        return get_user_choice()<br>    return user_choice<br><br>def get_computer_choice():<br>    return random.choice([&#39;S&#39;, &#39;W&#39;, &#39;G&#39;])<br><br>def determine_winner(player_choice, computer_choice):<br>    if player_choice == computer_choice:<br>        return &quot;Draw&quot;<br>    elif (player_choice == &#39;S&#39; and computer_choice == &#39;G&#39;) or \<br>         (player_choice == &#39;W&#39; and computer_choice == &#39;S&#39;) or \<br>         (player_choice == &#39;G&#39; and computer_choice == &#39;W&#39;):<br>        return &quot;You Win&quot;<br>    else:<br>        return &quot;Computer Wins&quot;<br><br>def snake_water_gun():<br>    user_choice = get_user_choice()<br>    computer_choice = get_computer_choice()<br>    print(f&quot;Your choice: {user_choice}&quot;)<br>    print(f&quot;Computer&#39;s choice: {computer_choice}&quot;)<br>    result = determine_winner(user_choice, computer_choice)<br>    print(f&quot;Result: {result}&quot;)<br><br>snake_water_gun()
