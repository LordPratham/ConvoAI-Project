import random<br>random_value = random.choice([&quot;Snake&quot;, &quot;Water&quot;, &quot;Gun&quot;])<br>input_value=input(&quot;Enter your weapon S,W,G\n&quot;).title()<br>if input_value in [&quot;Snake&quot;, &quot;Water&quot;, &quot;Gun&quot;]:<br>    print(&quot;Computer Select&quot;,random_value)<br>    if input_value == random_value:<br>        print(&quot;Match Draw&quot;)<br>    elif input_value == &quot;Gun&quot; and random_value == &quot;Snake&quot;:<br>        print(&quot;You Win&quot;)<br>    elif input_value==&quot;Water&quot; and random_value==&quot;Gun&quot;:<br>        print(&quot;You Win&quot;)<br>    elif input_value ==&quot;Snake&quot; and random_value==&quot;Water&quot;:<br>        print(&quot;You Win&quot;)<br>    else:<br>        print(&quot;Computer win&quot;)<br>else:<br>    print(&quot;Invalid syntax&quot;)
<a href=""http://www.youtube.com/results?search_query=%23challenge"">#Challenge</a> Acceted<br>import random<br><br>def WhoWon(playerInput, computerInput):<br>        if (playerInput == computerInput):<br>            return print(&quot;Its draw&quot;)<br><br>        elif (playerInput == 1 and  computerInput == 2):<br>            return print(&quot;You won&quot;)<br>            <br>        elif (playerInput == 3 and  computerInput ==1):<br>            return print(&quot;You won&quot;)<br><br>        elif (playerInput == 2 and  computerInput == 3):<br>            return print(&quot;You won&quot;)<br>        else:<br>             return isComputerWon(computerInput,playerInput)<br><br>def isComputerWon(computerInput, playerInput):<br>        if (computerInput == 1 and  playerInput == 2):<br>            return print(&quot;Computer won!&quot;)<br><br>        elif (computerInput == 3 and  playerInput ==1):<br>            return print(&quot;Computer won!&quot;)<br><br>        elif (computerInput == 2 and playerInput == 3):<br>            return print(&quot;Computer won!&quot;)<br>        <br><br><br><br>selection = [&#39;snake&#39;, &#39;water&#39;, &#39;gun&#39;]<br>a = True<br>playerScore = 0<br>computerScore = 0<br>print(&quot;Entre 1 for snake&quot;)<br>print(&quot;Entre 2 for water&quot;)<br>print(&quot;Entre 3 for gun&quot;)<br>while a:<br>    playerInput = int(input(&quot;Entre your input or entre 0 to exit: &quot;))<br><br>    try:<br>        if playerInput == 0:<br>            if(playerScore == computerScore):<br>                print(&quot;Its a draw game&quot;)<br>                break<br>            elif(playerScore&gt;computerScore):<br>                print(&quot;You won this game&quot;)<br>                print(f&quot;&quot;&quot;Scores are-:<br>    Your Score - {playerScore}<br>    Computer Score - {computerScore} &quot;&quot;&quot;)<br>                break<br>            else:<br>                print(&quot;Computer won this game&quot;)<br>                print(f&quot;&quot;&quot;Scores are-:<br>                    Your Score - {playerScore}<br>                    Computer Score - {computerScore} &quot;&quot;&quot;)<br>                break<br>        else:<br>            computerInput = random.randint(a=1,b=3)<br>            print(f&quot;You selected {selection[playerInput-1]}&quot;)<br>            print(f&quot;Computer selected {selection[computerInput-1]}&quot;)<br><br>            WhoWon(playerInput,computerInput)<br>            if (playerInput == 1 and  computerInput == 2):<br>                playerScore = playerScore+1<br>            <br>            elif (playerInput == 3 and  computerInput):<br>                playerScore = playerScore+1<br><br>            elif (playerInput == 2 and  computerInput == 3):<br>                playerScore = playerScore+1<br>        <br>            elif (computerInput == 1 and  playerInput == 2):<br>                computerScore = computerScore+1<br><br>            elif (computerInput == 3 and  playerInput ==1):<br>                computerScore = computerScore+1<br><br>            elif (computerInput == 2 and playerInput == 3):<br>                computerScore = computerScore+1<br>            else:<br>                computerScore+0<br>                playerScore+0<br>            <br>            print(f&quot;Your score - {playerScore}&quot;)<br>            print(f&quot;Computer score - {computerScore}&quot;)<br>            print(&quot;\n&quot;)<br>    except:<br>        print(&quot;Invalid input\n&quot;)<br>        userInput = int(input(&quot;Entre 1 to try again or Entre any key to exit&quot;))<br>        if userInput == 1:<br>            a = True<br>        else:<br>            a = False
# code by gouranga-ls<br>import random<br><br>pv = &quot;&quot;<br>def main():<br>    global pv<br>    def cvn(num):<br>        if num == 1:<br>            return &quot;snake&quot;<br>        elif num == 2:<br>            return &quot;water&quot;<br>        elif num == 3:<br>            return &quot;gun&quot;<br>        else :<br>            return 0<br>    def bot():<br>        swg = [&quot;1&quot;,&quot;2&quot;,&quot;3&quot;]<br>        r1 = random.sample(swg,1)<br>        botc = cvn(int(r1[0]))<br>        print(f&quot;Bot choose :- {botc}&quot;)<br>        return int(r1[0])<br>    def pvb(pc,bc):<br>        if pc == bc:<br>            print(&quot;  The Match :- Draw&quot;)<br>            main()<br>        elif (pc == 1 and bc == 2) or (pc == 2 and bc == 3) or (pc == 3 and bc == 1):<br>            print(&quot;  The Match :- You Win&quot;)<br>            main()<br>        elif pc == 1 or bc == 3 :<br>            print(&quot;  The Match :- You lose&quot;)<br>            main()<br>            <br>    print(&quot;  1. Snake&quot;)<br>    print(&quot;  2. Water&quot;)<br>    print(&quot;  3. Gun&quot;)<br>    print(&quot;  0. Ouit&quot;)<br>    p = int(input(&quot;choose your option :- &quot;))<br>    if p == 1 :<br>        pv = &quot;Snake&quot;<br>        print(f&quot;Your choose :- {pv}&quot;)<br>        bcon = bot()<br>        pvb(p,bcon)<br>    elif p == 2:<br>        pv = &quot;Water&quot;<br>        print(f&quot;Your choose :- {pv}&quot;)<br>        bcon = bot()<br>    elif p == 3:<br>        pv = &quot;Gun&quot;<br>        print(f&quot;Your choose :- {pv}&quot;)<br>        bcon = bot()<br>    elif p==0:<br>        print(&quot;You Quit Game&quot;)<br>    else:<br>        print(f&quot;Type 1 , 2 OR 3 You type :- {p}&quot;)<br>        main()<br>main()
