import random<br>for i in range(1000):<br><br> user=int(input(f&#39;press{0} for snake , press{1} for gun, press {2} for water&#39;))<br> numbers = [0, 1, 2]<br> random_number = random.choice(numbers)<br> if(random_number==0  and user==1):<br>    print(&#39;  YOU WIN BECAUSE YOU TOOK  GUNğŸ”«  AND I TOOK A SANKE ğŸ&#39;)<br> elif (random_number==1 and user==0):<br>    print(&#39;YOU LOSSE BECAUSE YOU TOOK  SNAKEğŸ AND I TOOK A GUNğŸ”«&#39;) <br> elif(random_number ==user ):<br>    print(&#39;THE MATCH IS DRAW BECAUSE YOU BOTH TOOK SAME  THING&#39;)<br> elif(random_number==1 and user==2):<br>    print(&#39;YOU WIN BECAUSE YOU TOOK A WATERğŸŒŠ AND I TOOK A GUNğŸ”«&#39;)<br> elif(random_number==2 and user==1):<br>    print(&#39;YOU LOSSE BECAUSE YOU TOOK GUNğŸ”«   AND  I TOOK A WATERğŸŒŠ &#39;)<br> elif(random_number==2  and user==0):<br>    print(&#39; YOU WIN BECAUSE YOU TOOK A SNAKEğŸ AND I TOOK A WATERğŸŒŠ&#39;)<br> else:<br>    print(&#39;YOU LOSSE BECAUSE YOU TOOK A WATERğŸŒŠ  AND  I TOOK A SNAKEğŸ &#39;)
