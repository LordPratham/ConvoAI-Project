Num = 10<br>for i in range(1, 10+1):<br>    print (&quot;10 X &quot;, i, &quot;= &quot;, Num*i)
# Sample list of dictionaries containing information about students<br>students = [<br>    {&quot;name&quot;: &quot;Alice&quot;, &quot;grade&quot;: 85},<br>    {&quot;name&quot;: &quot;Bob&quot;, &quot;grade&quot;: 92},<br>    {&quot;name&quot;: &quot;Charlie&quot;, &quot;grade&quot;: 78},<br>    {&quot;name&quot;: &quot;David&quot;, &quot;grade&quot;: 95},<br>]<br><br># Lists to store high-achieving and low-achieving students<br>high_achievers = []<br>low_achievers = []<br><br># Iterate over the list of students<br>for student in students:<br>    name = student[&quot;name&quot;]<br>    grade = student[&quot;grade&quot;]<br><br>    # Check if the student is a high-achiever or a low-achiever<br>    if grade &gt;= 90:<br>        high_achievers.append(name)<br>    elif grade &lt; 80:<br>        low_achievers.append(name)<br><br># Print the results<br>print(&quot;High Achievers:&quot;)<br>for student in high_achievers:<br>    print(student)<br><br>print(&quot;\nLow Achievers:&quot;)<br>for student in low_achievers:<br>    print(student)
