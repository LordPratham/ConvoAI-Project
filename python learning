def fib(n):<br>    a = 0<br>    b = 1<br>    if ( n == 1):<br>        print(a)<br>    if(n &lt; 0):<br>        print(&quot;invalid count number, Try Again ...!!&quot;)<br>    else:<br>        print(a, end=&#39; &#39;)<br>        print(b, end=&#39; &#39;)<br>        for i in range(2, n):<br>            c = a + b<br>            a = b<br>            b = c<br>            if(c &lt; 100):<br>                print(c, end=&#39; &#39;)<br>fib(100)
