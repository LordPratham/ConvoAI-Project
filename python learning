import openpyxl<br>import multiprocessing<br><br># Function to split data and save to an output file<br>def process_data(input_file, output_file, sheet_name):<br>    workbook = openpyxl.load_workbook(input_file)<br>    sheet = workbook[sheet_name]<br>    <br>    output_workbook = openpyxl.Workbook()<br>    output_sheet = output_workbook.active<br>    <br>    for row in sheet.iter_rows(min_row=2, values_only=True):  # Assuming your data starts from the second row<br>        first_item = row[0].split(&#39;_&#39;)[0]<br>        <br>        if first_item.isdigit():<br>            output_file_index = int(first_item) % 4<br>        else:<br>            output_file_index = 0<br>        <br>        if output_files[output_file_index] == output_file:<br>            output_sheet.append(row)<br>    <br>    output_workbook.save(output_file)<br>    output_workbook.close()  # Close the output workbook<br>    workbook.close()<br><br>if <i>_name_</i> == &quot;__main__&quot;:<br>    input_file = &#39;your_input_file.xlsx&#39;<br>    output_files = [&#39;output_file_1.xlsx&#39;, &#39;output_file_2.xlsx&#39;, &#39;output_file_3.xlsx&#39;, &#39;output_file_4.xlsx&#39;]<br>    sheet_name = &#39;Sheet1&#39;  # Change to the name of your sheet<br>    <br>    # Create a pool of processes to process the data in parallel<br>    pool = multiprocessing.Pool(processes=4)  # You can adjust the number of processes<br>    <br>    # Run the process_data function in parallel for each output file<br>    results = [pool.apply_async(process_data, (input_file, output_files[i], sheet_name)) for i in range(4)]<br>    <br>    # Wait for all processes to complete<br>    for result in results:<br>        result.get()<br>    <br>    pool.close()<br>    pool.join()
Nice<br>)
