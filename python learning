# WELCOME TO &quot;GUESS THE FRUIT&quot;<br><br>print(&#39;Welcome to  \&#39;GUESS THE FRUIT\&#39; game&#39; )<br><br>print(&#39;choose any one --- apple , banana , mango&#39;)<br><br>import random<br><br>you = input(&#39;your turn &#39;)                   <a href=""http://www.youtube.com/results?search_query=%23guess"">#guess</a><br><br><br>def gameWin(comp, you):<br>    if comp == you:<br>        return None<br><br>    elif comp == &#39;apple&#39;:<br>        if you==&#39;apple&#39;:<br>            return True<br>        elif you==&#39;banana&#39;or&#39;mango&#39;:<br>            return False<br>    <br>    elif comp == &#39;banana&#39;:<br>        if you==&#39;banana&#39;:<br>            return True<br>        elif you==&#39;apple&#39; or &#39;mango&#39;:<br>            return False<br>    <br>    <br>    elif comp == &#39;mango&#39;:<br>        if you==&#39;mango&#39;:<br>            return True<br>        elif you==&#39;apple&#39; or &#39;banana&#39;:<br>            return False<br><br><br>num = random.randint(1 , 4)<br>if num == 1:<br>    comp = &#39;bpple&#39;<br>elif num == 2:<br>    comp = &#39;banana&#39;<br>elif num == 3 :<br>    comp=&#39;mango&#39;<br><br><br>a = gameWin(comp,you)<br><br>print(f&#39;computer choose &#39;, {comp})             <a href=""http://www.youtube.com/results?search_query=%23printing"">#printing</a> choises<br>print(f&#39;you choose&#39;, {you})<br><br>if a == None :                      <a href=""http://www.youtube.com/results?search_query=%23last"">#last</a> decision<br>    print(&#39;game TIE&#39;)<br>elif a == True:<br>    print(&#39;YOU WON&#39;)                   <br>elif a== False :<br>    print(&#39;YOU loss&#39;)       <br><br>                              # the end
