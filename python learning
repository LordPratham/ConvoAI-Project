class Animal:<br>    def __init__(self, name, breed, color, legs):<br>        <a href=""http://self.name/"">self.name</a> = name<br>        self.breed = breed<br>        self.color = color<br>        self.legs = legs<br><br>    def info(self):<br>        print(f&quot;The name of the animal is {self.name}. It&#39;s a {self.color} coloured {self.breed} with {self.legs} legs&quot;)<br><br>class Cat(Animal):<br>    def __init__(self, name, breed, color, legs, make_sound):<br>        self.make_sound = make_sound<br>        super().__init__(name, breed, color, legs)<br><br><br>    def cat_info(self):<br>        print(f&quot;The name of the animal is {self.name}. It&#39;s a {self.color}\ncoloured {self.breed} with {self.legs} legs and its makes {self.make_sound} sound&quot;)<br><br><br><br>a1 = Animal(&quot;Cow&quot;, &quot;Cow_Breed&quot;, &quot;Black &amp; White&quot;, 4)<br>print(a1.info())<br><br>c1 = Cat(&quot;Cat&quot;, &quot;Stray&quot;, &quot;Ginger&quot;, 4, &quot;meow&quot;)<br>c1.cat_info()
