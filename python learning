Took help from google and then discovered the random module. Could not figure out the reversing part so took help from google for that too. Here is the final output. <br>import random<br>def code():<br>    userInput = input(&quot;Enter the word: &quot;)<br>    randomCharacters = [&quot;q&quot;,&quot;w&quot;,&quot;e&quot;,&quot;r&quot;,&quot;t&quot;,&quot;y&quot;,&quot;u&quot;,&quot;i&quot;,&quot;o&quot;,&quot;p&quot;,&quot;l&quot;,&quot;k&quot;,&quot;h&quot;,&quot;g&quot;,&quot;f&quot;,&quot;d&quot;,&quot;s&quot;,&quot;a&quot;,&quot;z&quot;,&quot;x&quot;,&quot;c&quot;,&quot;v&quot;,&quot;b&quot;,&quot;n&quot;,&quot;m&quot;]<br>    startRandomCharacters = &quot;&quot;<br>    endRandomCharacters = &quot;&quot;<br>    if len(userInput)&gt;=3:<br>        modifiedInput = userInput[1:]+userInput[0]<br>        for i in range(0,3):<br>            startRandomCharacters += random.choice(randomCharacters)<br>            endRandomCharacters += random.choice(randomCharacters)<br>        print(f&quot;Your final codeword is {startRandomCharacters+modifiedInput+endRandomCharacters}&quot;)<br>    else:<br>        print(f&quot;The output is {userInput[::-1]}&quot;)<br> <br>def decode():<br>    userInput = input(&quot;Enter the codeword: &quot;)<br>    if len(userInput)&lt;3:<br>       print(f&quot;The output is {userInput[::-1]}&quot;)<br>    else:<br>        randomCharactersRemoved = userInput[3:-3]<br>        lastCharacter = randomCharactersRemoved[-1]<br>        print(f&quot;The decoded word is {lastCharacter+randomCharactersRemoved[:-1]}&quot;)<br>       <br><br>askUser = input(&quot;Do you want to code or decode?&quot;)<br>if askUser.lower()==&quot;code&quot;:<br>    code()<br>elif askUser.lower()==&quot;decode&quot;:<br>    decode()<br>else:<br>    print(&quot;Enter a valid value.&quot;)
import string<br>import random<br>inp = input(&quot;Enter a sentence: &quot;)<br>sentence = inp.split()<br># print(sentence)<br>n = 3<br><br>for i in range(len(sentence)):<br>    word = sentence[i]<br>    # print(&quot;Word:&quot;, word)<br>    letters = list(word)<br>    # print(&quot;Letters:&quot;, letters)<br><br>    if len(letters) == 2:<br>        # Reverse the letters<br>        temp = letters[1] + letters[0]<br>        # print(&quot;Reversed:&quot;, temp)<br>        print( str(temp))<br><br>    if len(letters) &gt;= 3:<br>        temp2 = letters[0]<br>        del letters[0]<br>        name2 = &quot;&quot;.join(letters) + temp2<br>        rand = &quot;&quot;.join(random.choices(string.ascii_lowercase, k=n))<br>        print(str(rand) + name2 + str(rand))
import random<br><br># for generating random characters<br>def generat_characters(charset=&quot;abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789&quot;):<br>    rc = &quot;&quot;<br>    for i in range(6):<br>        rc += random.choice(charset)<br>    return rc<br><br>def code(word):<br>    rc = generat_characters(charset=&quot;abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789&quot;)<br>    if len(word) &lt;= 2:<br>        return (word[::-1])<br>    else:<br>        # print(rc)<br>        return (rc[0:3] + word[1:] + word[0] + rc[3:6])<br><br><br>def decode(word):<br>    if len(word) &lt;= 2:<br>        return (word[::-1])<br>    else:<br>        return (word[-4] + word[3:-4])<br><br>c = (input(&quot;Do u want to code or decode: &quot;))<br>print(&#39;&#39;&#39;NOTE: If your message consist more than one word then it should end with &#39;.&#39;,&#39;?&#39;or&#39;!&#39;.&#39;&#39;&#39;)<br>x = c.lower()<br>match x:<br>    case &quot;code&quot;:<br>        msg = input(&quot;Enter the message you want code: &quot;)<br>        word = &quot;&quot;<br>        nc = &quot;&quot;<br>        if msg.isspace() == True:<br>            for i in range(len(msg)):<br>                if (msg[i]==&quot; &quot; or msg[i]==&quot;.&quot; or msg[i]==&quot;?&quot; or msg[i]==&quot;!&quot; or msg[i]==&quot;,&quot;):<br>                    coded = code(word)<br>                    nc += (coded + msg[i])<br>                    word = &quot;&quot;<br>                else:<br>                    word += msg[i]<br>        else:<br>            nc = code(msg)<br>        print(nc)<br>    case &quot;decode&quot;:<br>        msg = input(&quot;Enter the message you want decode: &quot;)<br>        word = &quot;&quot;<br>        nc = &quot;&quot;<br>        if msg.isspace() == True:<br>            for i in range(len(msg)):<br>                if (msg[i]==&quot; &quot; or msg[i]==&quot;.&quot; or msg[i]==&quot;?&quot; or msg[i]==&quot;!&quot; or msg[i]==&quot;,&quot;):<br>                    decoded = decode(word)<br>                    nc += (decoded + msg[i])<br>                    word = &quot;&quot;<br>                else:<br>                    word += msg[i]<br>        else:<br>            nc = decode(msg)<br>        print(nc)<br>    case _ :<br>        print(&quot;Invalid input&quot;)
import random<br>import string<br><br>def decoding(sentence):<br>            <br>            words=sentence.split()<br>            final=&quot;&quot;<br><br>            # using loop to check each word of sentence<br>            for i in range(0,len(words)):<br>                <a href=""http://www.youtube.com/results?search_query=%23if"">#if</a> word&lt;=3, then reversing it <br>                if len(words[i])&lt;=3:<br>                    revised=words[i][::-1]<br>                <a href=""http://www.youtube.com/results?search_query=%23if"">#if</a> word&gt;3, then removing three characters from start and end ,and<br>                # removing last char and putting at first<br>                if len(words[i])&gt;3:<br>                    removingRandoms=words[i][3:len(words[i])-3:1]<br>                    words[i]=removingRandoms<br>                    firstWord=words[i][len(words[i])-1]<br>                    removingLast=words[i][0:len(words[i])-1]<br>                    revised=firstWord+removingLast<br>                final=final+revised+&quot; &quot;<br>            print(final) <br><br># asking choice<br>print(&quot;Which Function you want to perform? &quot;)<br>print(&quot;1: Decoding      2:Encoding&quot;)<br>choice=input()<br><br># handling exception<br>if choice.isdigit:<br><br>    # if user chooses decoding<br>    if int(choice)==1:<br>        print(&quot;Enter a Sentence to Decode&quot;)<br>        sentence=input()<br>        decoding(sentence)          <br><br>    <a href=""http://www.youtube.com/results?search_query=%23if"">#if</a> user choose encoding <br>    elif int(choice)==2:<br>        revised=&quot;&quot;<br>        print(&quot;Enter a Sentence to Encode&quot;)<br>        sentence=input()<br>        words=sentence.split()<br>        final=&quot;&quot;<br>        <br>        # using loop to check each word of sentence<br>        for i in range(0,len(words)):<br>            random_char=&quot;&quot;<br>            # generating 3 random characters<br>            for _ in range(3):<br>                random_char = random_char+random.choice(string.ascii_lowercase)<br><br>            <a href=""http://www.youtube.com/results?search_query=%23if"">#if</a> word&lt;=3, then reversing it <br>            if len(words[i])&lt;=3:<br>                revised=words[i][::-1]<br><br>            <a href=""http://www.youtube.com/results?search_query=%23if"">#if</a> word&gt;3, then removing first letter and placing it at end and<br>            # adding 3 random characters at start and end <br>            if len(words[i])&gt;3:<br>                first=&quot;&quot;<br>                first=words[i][0]<br>                removingFirst=words[i][1:]<br>                revised=removingFirst+first<br>                revised=f&quot;{random_char}{revised}{random_char}&quot;<br>            final=final+revised+&quot; &quot;<br>        print(final)<br>        print(&quot;Do you want to Decode it? &quot;)<br>        choice=input()<br>        if choice.lower()==&quot;yes&quot;:<br>            decoding(final)<br>        else: print(&quot;Ending Program&quot;)            <br>    else:<br>        print(&quot;Digit is not Valid&quot;)        <br>else:<br>    print(&quot;Invalid Input!!&quot;)
