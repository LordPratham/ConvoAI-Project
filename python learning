# SNAKE WATER GUN<br><br>import random<br><br>snake, water, gun = 0, 1, 2<br><br><br><br>c_score = 0<br>p_score = 0<br><br>while True:<br>    computer = random.randint(0,2)<br>    player = input(&quot;Enter your choice: &quot;)<br><br>    if player == &quot;stop&quot;:<br>        print(&quot;Game Over!&quot;)<br>        break<br><br>    if computer == 0 and player == &quot;0&quot;:<br>        print(&quot;It&#39;s a draw!&quot;)<br><br>    elif computer == 0 and player == &quot;1&quot;:<br>        print(&quot;Computer Won!&quot;)<br>        c_score +=1<br><br>    elif computer == 0 and player == &quot;2&quot;:<br>        print(&quot;Player Won&quot;)<br>        p_score += 1<br><br>    elif computer == 1 and player == &quot;0&quot;:<br>        print(&quot;Player Won!&quot;)<br>        p_score += 1<br><br>    elif computer == 1 and player == &quot;1&quot;:<br>        print(&quot;It&#39;s a draw!&quot;)<br><br>    elif computer == 1 and player == &quot;2&quot;:<br>        print(&quot;Computer Won!&quot;)<br>        c_score +=1<br><br>    elif computer == 2 and player == &quot;0&quot;:<br>        print(&quot;Computer Won!&quot;)<br>        c_score +=1<br><br>    elif computer == 2 and player == &quot;1&quot;:<br>        print(&quot;Player Won!&quot;)<br>        p_score += 1<br>            <br>    elif computer == 2 and player == &quot;2&quot;:<br>        print(&quot;It&#39;s a draw!&quot;)<br>    else:<br>           print(&quot;Enter only 0, 1 or 2.&quot;)<br>    <br>   <br><br>print(f&quot;The scores of Computer is {c_score} and Player is {p_score}.&quot;)<br><br>if c_score&gt;p_score:<br>    print(&quot;Computer won this match.&quot;)<br>else:<br>    print(&quot;Player won this match&quot;)
