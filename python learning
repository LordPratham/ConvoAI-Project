def sort(list):<br><br>    for i in range(len(list)-1):<br>        min = i<br>        for j in range(i,len(list)-1):<br>            if list[j] &lt; list[min]:<br>                min =j<br>        temp = list[i]<br>        list[i]= list[min]<br>        list[min]= temp<br>        print(list)<br><br><br><br><br><br>list = [5,3,8,7,2,6]<br><br>sort(list)
