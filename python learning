def calculategmean(a,b):<br>    mean = (a*b)/(a+b)<br>    print(mean) <br><br>def Comparision(a,b):<br>    if(a&gt;b):<br>        print(a,&quot;is Bigger than&quot;,b)<br>    elif(b&gt;a):<br>        print(b,&quot; is Bigger than&quot;,a)<br>    else:<br>        print(&quot;A and B is Equals to&quot;)<br>a = 9<br>b = 8<br>calculategmean(a,b)<br>Comparision(a,b)<br><br>c = 80<br>d = 90<br>calculategmean(c,d)<br>Comparision(c,d)
