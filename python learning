import random<br><br>print(&quot;Welcome to Abdullah Gorar&#39;s secret code language\n&quot;)<br><br><br>def generate_random_characters(char_length):<br>    alpha_num = &quot;ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmopqrstuvwxyz0123456789&quot;<br>    random_str = &quot;&quot;<br>    for i in range(char_length):<br>        random_str += random.choice(alpha_num)<br>    return random_str<br><br>def encode(message):<br>    splited_message = message.split(&quot; &quot;)<br>    encoded_message = []<br>    for word in splited_message:<br>        if len(word) &lt; 3:<br>            encoded_message.append(word[::-1])<br>        else:<br>            temp_word = &quot;&quot;<br>            temp_word += generate_random_characters(3)<br>            temp_word += word[1:]<br>            temp_word += word[0]<br>            temp_word += generate_random_characters(3)<br>            encoded_message.append(temp_word)<br>    return &quot; &quot;.join(encoded_message)<br><br>def decode(encoded_message):<br>    splited_message = encoded_message.split(&quot; &quot;)<br>    decoded_message = []<br>    for word in splited_message:<br>        if len(word) &lt; 3:<br>            decoded_message.append(word[::-1])<br>        else:<br>            removed_random_chars = word[3:-3]<br>            temp_word = &quot;&quot;<br>            temp_word += removed_random_chars[-1]<br>            temp_word += removed_random_chars[0:-1]<br>            decoded_message.append(temp_word)<br>    return &quot; &quot;.join(decoded_message)<br><br>encode_decode_options = input(&quot;1: To encode a message \t 2: To decode a message: &quot;)<br><br>if encode_decode_options == &quot;1&quot;:<br>    message = input(&quot;Enter your message: &quot;)<br>    print(encode(message))<br>elif encode_decode_options == &quot;2&quot;:<br>    message = input(&quot;Enter your message: &quot;)<br>    print(decode(message))<br>else:<br>    raise ValueError(&quot;You did not pass the correct value please try running program again with corrct value.&quot;)
