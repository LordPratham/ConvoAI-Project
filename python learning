=1<br>                 rewrd+=5000<br>                 life-=1<br>                 time.sleep(2)<br>                 print(&quot;Your reward : &quot;,rewrd, &quot; and your life: &quot;,life,&quot;\n\n&quot;)<br>                 <br>            elif(life!=0):<br>                 time.sleep(2)<br>                 print(&quot;\nIncorrect Answer! Alternate question using life\n\n&quot;)<br>        <br>            <br>                 print(a_q[1],&quot;\n\n&quot;)<br>                 an=input(&quot;Enter the correct option: &quot;)<br>                 if (an==a_a[1]):<br>                      time.sleep(2)<br>                      print(&quot;\nCorrect Answer!&quot;)<br>                      n+=1<br>                      rewrd+=5000<br>                      life-=1<br>                      time.sleep(2)<br>                      print(&quot;Your reward : &quot;,rewrd, &quot; and your life: &quot;,life,&quot;\n\n&quot;)<br>                 else:<br>                     time.sleep(2)<br>                     print(&quot;Bhai IT chodh do!&quot;)<br>                     time.sleep(2)<br>                     exit(0)<br>        else:<br>            time.sleep(2)<br>            print(&quot;Bhai IT chodh do!&quot;)<br>            time.sleep(2)<br>            exit(0)
