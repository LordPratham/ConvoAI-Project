def fibonacci (num):<br>    if num==0:<br>        return 0<br>    elif num==1:<br>        return 1<br>    else:<br>        return fibonacci(num-1)+fibonacci(num-2)<br><br>print(fibonacci(6))
def fibonacci(number):<br>  if (number == 0 or number == 1):<br>    return number<br>  else:<br>    return fibonacci(number-1) + fibonacci(number-2)
