def ask_question(question, answer, prize_money):<br>    user_response = input(question + &quot; &quot;)<br>    if user_response.lower() == answer.lower():<br>        print(&quot;Correct! You earned {} RS. 1000&quot;.format(prize_money))<br>        return True<br>    else:<br>        print(&quot;Incorrect! The correct answer was: {}&quot;.format(answer))<br>        return False<br><br>if <i>_name_</i> == &quot;__main__&quot;:<br>    questions_and_answers = [<br>        (&quot;Q1: What were the two major superpowers during the Cold War?&quot;, &quot;US and Soviet Union&quot;, 1000),<br>        (&quot;Q2: What was the capital of Poland during the Cold War?&quot;, &quot;Warsaw&quot;, 1000),<br>        (&quot;Q3: Which famous speech started with &#39;I have a dream&#39;?&quot;, &quot;Martin Luther King Jr.&#39;s speech&quot;, 1000),<br>        (&quot;Q4: What is the largest planet in our solar system?&quot;, &quot;Jupiter&quot;, 1000),<br>    ]<br><br>    total_prize_money = 0<br><br>    for question, answer, prize_money in questions_and_answers:<br>        if ask_question(question, answer, prize_money):<br>            total_prize_money += prize_money<br><br>    print(&quot;\nYou earned a total of {} RS.&quot;.format(total_prize_money))
