import random as r<br><br>l=[&quot;snake&quot;,&quot;water&quot;,&quot;gun&quot;]<br><br>player=input(&quot;What you want to choose select these:snake,water,gun\nyour selection is:&quot;)<br><br>computer=r.choice(l)<br><br>print(&quot;Computer chooses the &quot;,computer)<br><br>print(&quot;\nAnd the winner is : &quot;)<br><br>if(player==computer):<br>    print(&quot;The game is draw&quot;)<br>elif((player==&quot;snake&quot; and computer==&quot;water&quot;)or(player==&quot;water&quot; and computer==&quot;gun&quot;)or(player==&quot;gun&quot; and computer==&quot;snake&quot;)):<br>    print(&quot;Player win the game&quot;)<br>else:<br>    print(&quot;Computer win the game&quot;)
import random<br>choices=[-1,0,1]<br>ref=[0,&#39;Rock&#39;,&#39;Paper&#39;,&#39;Scissors&#39;]<br>com_ch=int(random.choice(choices))<br>user_ch=int(input(&#39;Enter -1 for Rock, 0 for Paper &amp; 1 for Scissors &#39;))<br><br>print(&#39;Computer chose &#39;, ref[com_ch+2])<br>print(&#39;You Chose &#39;, ref[user_ch+2])<br>if com_ch == user_ch:<br>    print(&quot;Draw&quot;)<br>elif com_ch==0 and user_ch==1:<br>    print(&#39;You Win&#39;)<br>elif com_ch==1 and user_ch==0:<br>    print(&#39;You Loose&#39;)<br>elif com_ch==1 and user_ch==-1:<br>    print(&#39;You Win&#39;)<br>elif com_ch==-1 and user_ch==1:<br>    print(&#39;You Loose&#39;)<br>elif com_ch==-1 and user_ch==0:<br>    print(&#39;You Win&#39;)<br>elif com_ch==0 and user_ch==-1:<br>    print(&#39;You Loose&#39;)<br>else:<br>    print(&#39;Invalid Input&#39;)
import random<br>computerPlayer = [&#39;rock&#39;,&#39;paper&#39;,&#39;scissors&#39;]<br>player1 = [&#39;rock&#39;,&#39;paper&#39;,&#39;scissors&#39;]<br><br>computer=random.choice(computerPlayer)<br>player=random.choice(player1)<br><br>i=computerPlayer.index(computer)<br>j=player1.index(player)<br><br>print(&#39;computer&#39;,computerPlayer[i])<br>print(&#39;player&#39;,player1[j])<br>if i==j:<br>    print(&quot;tie&quot;)<br>elif (i == 0 and j == 2) or (i == 1 and j == 0) or (i == 2 and j == 1):<br>    print(&#39;Computer wins!&#39;)<br>else:<br>    print(&#39;player wins&#39;)
