entername = int(input(&quot;Enter how many names&quot;))<br>namelist = []<br>countname = 0<br><br><br>def namecounter(n):<br>    for i in range(n):<br>        name = input(&quot;Enter name(s)&quot;)<br>        namelist.append(name)<br>    for j in namelist:<br>        count = len(j)<br><br>        if count &gt;= 5:<br>            globals()[&#39;countname&#39;] += 1<br><br>    print(namelist, &quot;{} names has five or more letters&quot;.format(countname))<br><br><br>namecounter(entername)
lst1 = [ ]<br><br>def len1(lst1):<br>    name = 0<br>    for j in lst1:<br>        if len(j) &gt;= 5:<br>            name += 1<br>    return(name)<br><br><br>name = len1(lst1)<br><br>print(name)<br><br><br>another way:<br><br>lst1 = [ ]<br><br>def len1(lst1):<br>    i = 0<br>    for j in lst1:<br>        if len(j) &gt;= 5:<br>            i += 1<br>    print(i)<br><br>len1(lst1)
def count(length):<br>    counting=0<br>    for x in (length):<br>        if len(x)&gt;5:<br>            counting+=1<br>    return counting<br>user_input=input(&quot;please enter a names using space&quot;)<br>names_as_strings= user_input.split()<br>length= [str for str in names_as_strings]<br>counting= count(length)<br>print(&quot;counting:)<br>print(length)
