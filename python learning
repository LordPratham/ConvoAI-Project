def clues(list):<br>    for i in list:<br>        print(f&quot;{i}&quot;)<br>def questions(question):<br>    print(&quot;Here is your question:&quot;)<br>    print(question)<br>question_clues = {<br>    &#39;question1&#39;:&quot;What is 8 + 6?&quot;,<br>    &#39;clues1&#39;:[12,14,16,18],<br>    &#39;answer1&#39;:14,<br>    &#39;question2&#39;:&quot;If you have 5 apples and you give 3 to your friend, how many apples do you have left?&quot;,<br>    &#39;clues2&#39;:[2,4,3,5],<br>    &#39;answer2&#39;:2,<br>}<br># for checking answers.<br>def check_answers(answer,c_answer):<br>    if answer == c_answer:<br>        return True<br>    else:<br>        return False<br>money = 0<br># main loop<br>while True:<br>    user = input(&quot;Enter &#39;y&#39; to play the or enter &#39;n&#39; to quit :&quot;)<br>    if user.lower() == &#39;y&#39;:<br>        questions(question_clues[&#39;question1&#39;])<br>        clues(question_clues[&#39;clues1&#39;])<br>        answer = int(input(&quot;Enter Your answer :&quot;))<br>        if check_answers(answer,question_clues[&#39;answer1&#39;])== True:<br>            money += 100000<br>            print(&quot;Congratulations! correct answers.&quot;)<br>            user_input = input(&quot;Enter &#39;y&#39; if you want to continue answering questions or enter &#39;n&#39; if you want to quit the game: &quot;)<br>            if user_input == &#39;y&#39;:<br>                questions(question_clues[&#39;question2&#39;])<br>                clues(question_clues[&#39;clues2&#39;])<br>                answer = int(input(&quot;Enter Your answer :&quot;))<br>                if check_answers(answer, question_clues[&#39;answer2&#39;]) == True:<br>                    money *= money<br>                    print(&quot;Congratulations! correct answers.&quot;)<br>                    print(f&quot;Here is your earned money :{money}$&quot;)<br>                    print(&quot;Thanks for playing the game.&quot;)<br>                    break<br>                else:<br>                    print(&quot;Wrong answer better luck next time .&quot;)<br>                    break<br>            elif user_input == &#39;n&#39;:<br>                print(&quot;You choose to quit.&quot;)<br>                print(f&quot;Here is your money {money}$&quot;)<br>                break<br>        else:<br>            print(&quot;Wrong answer better luck next time .&quot;)<br>    elif user.lower() == &#39;n&#39;:<br>        print(&quot;You choose to quit.&quot;)<br>        print(f&quot;Total earned money : {money}$&quot;)<br>        break
