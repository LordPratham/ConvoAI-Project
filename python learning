<a href=""http://www.youtube.com/results?search_query=%23100daysofcode"">#100daysofcode</a><br><br><br>class Library:<br>    def __init__(self):<br>        self.no_of_books = 10<br>        self.books = [&#39;munshi premchand&#39;, &#39;Why i killed gandhi&#39;, &#39;Anandwan&#39;, &#39;The C programing laungage&#39;,<br>                      &#39;past and history of linux&#39;, &#39;machine learning&#39;, &#39;hitler&#39;, &#39;the brief history of time&#39;, &#39;coder&#39;, &#39;wings of fire&#39;]<br><br><br>class SecLibrary(Library):<br>    def __init__(self):<br>        Library.__init__(self)  # Initialize the parent class<br>        self.added_books = []<br><br>    def booklistinfo(self):<br>        for i in range(self.no_of_books):<br>            print(f&#39;book {i + 1} : {self.books[i]}&#39;)<br><br>    def addbook(self):<br>        print(&#39;Enter name of your book:&#39;)<br>        namebook = input()<br>        self.added_books.append(namebook)<br>        self.no_of_books += 1<br>        self.books.append(namebook)<br><br>    def removebook(self):<br>        print(&#39;Enter index of the book to remove:&#39;)<br>        indexbook = int(input())<br>        if 0 &lt;= indexbook &lt; len(self.books):<br>            removed_book = self.books.pop(indexbook)<br>            print(f&#39;&quot;{removed_book}&quot; has been removed from the library.&#39;)<br>            self.no_of_books -= 1<br>        else:<br>            print(&#39;Invalid index. Book removal failed.&#39;)<br><br><br>sec_lib = SecLibrary()<br>sec_lib.booklistinfo()<br>sec_lib.addbook()<br>sec_lib.booklistinfo()<br><br>sec_lib.removebook()<br>sec_lib.booklistinfo()
class Library:<br>    book_container = []<br>    def __init__(self):<br>        self.no_of_books = 0<br><br>    <br>    def add_book(self,books):<br>        if type(books) == list:<br>            for words in books:<br>                Library.book_container.append(words)<br><br>        else:<br>            Library.book_container.append(books)<br><br>    def show_library(self):<br>        if len(Library.book_container) &lt; 1:<br>            print(&#39;Your Library does not have any books to show&#39;)<br><br>        else:<br>            print(&#39;Your Library contains following books&#39;)<br>            for words in Library.book_container:<br>                print(f&#39;--&gt; {words}&#39;)<br>    <br><br>    def remove_book(self,book_name):<br>        Library.book_container.remove(book_name)<br><br>    <br>    def books_number(self):<br>        self.no_of_books = len(Library.book_container)<br>        print(self.no_of_books)<br><br><br>books_list = [&#39;Python&#39;,&#39;Java&#39;,&#39;C++&#39;]<br><br><br>user = Library() # --&gt; Library object<br>user.show_library() # --&gt; Prints (Your Library does not have any books to show)<br><br>user.add_book(books_list) # --&gt; add-book method<br>user.show_library() # --&gt; Show all the books in the library<br><br>user.remove_book(&#39;Python&#39;) # --&gt; Removes a book from the library<br>user.show_library() # --&gt; Show books after removing<br><br>user.books_number() # --&gt; Prints How many books in your library
