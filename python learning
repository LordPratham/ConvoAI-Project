def sort(nums):<br>    for i in range(len(nums)-1,0,-1):<br>        for j in range (i):<br>            if nums[j]&gt;nums[j+1]:<br>                nums[j],nums[j+1]=nums[j+1],nums[j]<br><br>nums = [5,3,8,6,7,12]<br>sort(nums)<br><br>print(nums)
