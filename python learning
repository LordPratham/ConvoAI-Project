using dictionary kaun banega karodpathi:<br># questions<br>questions=[&quot;What is the capital of INDIA?&quot;,&quot;What is the capital of USA?&quot;,&quot;What is the capital of JAPAN?&quot;,&quot;What is the capital of PAKISTAN?&quot;,&quot;What is the capital of CHINA?&quot;]<br># answers=[&quot;NEW DELHI&quot;,&quot;WASHINGTON DC&quot;,&quot;TOKYO&quot;,&quot;ISTANBUL&quot;,&quot;BEIJING&quot;]<br>answers={&quot;D&quot;:&quot;NEW DELHI&quot;,&quot;B&quot;:&quot;WASHINGTON DC&quot;,&quot;C&quot;:&quot;TOKYO&quot;,&quot;A&quot;:&quot;ISTANBUL&quot;,&quot;E&quot;:&quot;BEIJING&quot;}<br><br>print(&quot;Hello!Nice to meet you.Shall we start the game?&quot;)<br>sn=input(&quot;YES OR NO.TELL ME BUDDY:&quot;)<br>if sn.lower()==&#39;yes&#39;:<br>  print(&quot;WELCOME&quot;)<br>  COUNT=0<br>  for i in range (0,len(questions)):<br>    print(questions[i])<br>    print(&quot;<a href=""http://a.istanbul/"">A.ISTANBUL</a>&quot;, &quot;B.WASHINGTON DC&quot;, &quot;<a href=""http://c.tokyo/"">C.TOKYO</a>&quot;, &quot;<a href=""http://d.new/"">D.NEW</a> DELHI&quot;, &quot;E.BEIJING&quot;, end=&quot; &quot;)<br>    option=input(&quot;Enter the option:&quot;)<br>    if (option.upper()==(list(answers.keys())[i])):<br>      print(&quot;YOU ARE CORRECT&quot;)<br>      COUNT+=((i+2)*1000)<br>      print(f&quot;YOU HAVE WON ${COUNT}&quot;)<br>    else:<br>      print(&quot;SORRY,INCORRECT ANSWER&quot;)<br>      print(f&quot;YOUR TOTAL WINNING AMOUNT IS ${COUNT}&quot;)<br>      break<br>else:<br>  print(&quot;OK!HAVE A BAD DAY&quot;)
