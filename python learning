import sys<br><br>sys.set_int_max_str_digits(999999999)<br><br><br>def fibonacci(n):<br>    &#39;&#39;&#39;Take number n as interger, returns its factorial&#39;&#39;&#39;<br>    try:<br>        if n == 0:<br>            return 0<br>        elif n == 1:<br>            return 1<br>        else:<br>            return fibonacci(n-1) + fibonacci(n-2)<br>    except:<br>        print(&#39;Keep it real&#39;)<br>    <br>n = int(input(&#39;Enter a number: &#39;))<br><br>print(fibonacci(n)) # type: ignore<br><br># example:<br># fib(5) = fib(4) + fib(3)<br># fib(4) = fib(3) + fib(2)<br># fib(3) = fib(2) + fib(1)<br># fib(2) = fib(1) + fib(0)<br><br># fib(5) = fib(3) + fib(2) + fib(2) + fib(1)<br># fib(5) = fib(2) + fib(1) + fib(1) + fib(0) + fib(1) + fib(0) + 1<br># fib(5) = fib(1) + fib(0) + 1 + 1 + 0 + 1 + 0 + 1<br># fib(5) = 1 + 0 + 1 + 1 + 0 + 1 + 0 + 1<br># fib(5) = 5
def fibonacci_sq(x):<br>    n1, n2 = 0, 1<br>    sum = 0<br>    for i in range (0, x):<br>        sum = n1 + n2<br>        print(&quot;Fibonacci Sequence: &quot;,sum,sep=&quot;&quot;)<br>        n2 = n1<br>        n1 = sum<br>prg_again = &quot;y&quot;<br>while True:<br>    if prg_again == &quot;y&quot;:<br>        x = int(input(&quot;Enter a number: &quot;))<br>        fibonacci_sq(x)<br>    else:<br>        break<br>    prg_again = input(&quot;Play again: &quot;).lower()
