def fibo(n):<br>    num1 = 0<br>    num2 = 1<br>    if n &lt; 0:<br>        print(&#39;Invalid input&#39;)<br>    elif n == 0:<br>        print(num1)<br>    elif n == 1:<br>        print(num2)<br>    else:<br>        for i in range(n):<br>            num2 = num1 + num2<br>            num1 = num2 - num1<br>            if num2 &gt; n:<br>                print(num1)<br>                break<br><br>fibo(100)
