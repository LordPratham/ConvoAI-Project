code language:<br><br>import random<br>import msvcrt<br>def inp():<br>    a = input(&#39;enter string\n&#39;)<br>    b = &#39;&#39;<br>    l = []<br>    i = 0<br>    while (i &lt; len(a)):<br>        if (a[i] == &#39; &#39;):<br>            while (a[i] == &#39; &#39;):<br>                i = i + 1<br>            l = l + [b]<br>            b = &#39;&#39;<br>        b = b + a[i]<br>        i = i + 1<br>    return(l + [b])<br>def rand():<br>    return chr(random.randrange(97,123))<br>def main():<br>    match(int(input(&#39;enter:\n1 to code\n2 to decode\n&#39;))):<br>        case 1:<br>            l=inp()<br>            for i in range(len(l)):<br>                if(len(l[i])&lt;3):<br><br>                    for j in range(int(len(l[i])/2)):<br>                        # l[i] = l[i][j+1:] + l[i][:j+1]<br>                        l[i] = l[i][:j] + l[i][len(l[i])-j-1] + l[i][j+1:len(l[i])-j-1] + l[i][j] + l[i][len(l[i])-j:]<br><br>                        # new_string = original_string[:index1] + char2 + original_string[index1 + 1:index2] + char1 + original_string[index2 + 1:]<br>                else:<br>                    s1=rand()+rand()+rand()<br>                    s2=rand()+rand()+rand()<br>                    l[i]=s1+l[i][1:]+l[i][0]+s2<br>            print(*l)<br>            main()<br>        case 2:<br>            l=inp()<br>            for i in range(len(l)):<br>                if(len(l[i])&lt;3):<br><br>                    for j in range(int(len(l[i])/2)):<br>                        # l[i] = l[i][j+1:] + l[i][:j+1]<br>                        l[i] = l[i][:j] + l[i][len(l[i])-j-1] + l[i][j+1:len(l[i])-j-1] + l[i][j] + l[i][len(l[i])-j:]<br><br>                        # new_string = original_string[:index1] + char2 + original_string[index1 + 1:index2] + char1 + original_string[index2 + 1:]<br>                else:<br>                    l[i]=l[-4]+l[3:-4]<br>            print(*l)<br>            main()<br>        case _:<br>            print(&#39;wrong input&#39;)<br>            main()<br>main()
