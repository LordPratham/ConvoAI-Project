print(&#39;&#39;&#39;<br>+ ADD<br>- SUBTRACT<br>* MULTIPLY<br>/ DIVIDE<br>&#39;&#39;&#39;)<br>num1=int(input(&quot;Enter Your First Number:- &quot;))<br>num2=int(input(&quot;Enter Your Second Number:- &quot;))<br>opr=input(&quot;Enter Your Opr(+,-,*,/):- &quot;)<br>if opr==&quot;+&quot;:<br>    print(num1+num2)<br>    print(&quot;Thanks You&quot;)<br>elif opr==&quot;-&quot;:<br>    print(num1-num2)<br>    print(&quot;Thanks You&quot;)<br>elif opr==&quot;*&quot;:<br>    print(num1*num2)<br>    print(&quot;Thanks You&quot;)<br>elif opr==&quot;/&quot;:<br>    print(num1/num2)<br>    print(&quot;Thanks You&quot;)<br>else:<br>    print(&quot;Invalid Opr&quot;)<br>    print(&quot;Dont Play With Us&quot;)
