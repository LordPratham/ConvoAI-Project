class library:<br>    books_list=[&#39;agni&#39;,&#39;vayu&#39;,&#39;dharti&#39;,&#39;jal&#39;,&#39;bhumi&#39;]<br>    no_of_books=len(books_list)<br><br>class Newlibrary(library):<br>    def dispaly_Books(self):<br>        for i in range(len(self.books_list)):<br>            print(&quot;Book Name:&quot;,self.books_list[i])<br>    def add_Books(self):<br>        book_name=input(&quot;enter the name of book you want to add&quot;)<br>        self.books_list.append(book_name)<br>        print(self.books_list)<br>    def Number_Of_Books(self):<br>        print(&quot;number of books in your library currently is &quot; , len(self.books_list))<br><br><br><br><br>a=Newlibrary()<br>a.dispaly_Books()<br>a.add_Books()<br>a.Number_Of_Books()
