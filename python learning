import random<br><br>first_prompt=input(&#39;You Wanna Encode or Decode: &#39;).lower()<br>if first_prompt==&#39;encode&#39;:<br>    x=input(&#39;Enter the Secret Message: &#39;)    <br>    first_=x[0]<br>    y=x.replace(x[0],&#39;&#39;) + first_<br>    z=str()<br>    AA=[&#39;A&#39;,&#39;B&#39;,&#39;C&#39;,&#39;D&#39;,&#39;F&#39;,&#39;X&#39;,&#39;Y&#39;,&#39;Z&#39;,&#39;1&#39;,&#39;2&#39;,&#39;3&#39;,&#39;4&#39;,&#39;5&#39;,&#39;6&#39;,&#39;7&#39;,&#39;8&#39;,&#39;9&#39;]<br>    if len(x)&gt;=3:<br>        for i in range(0,3):<br>           y = y + str(random.choice(AA)).strip(&quot;&#39;&quot;)<br>           y = str(random.choice(AA)).strip(&quot;&#39;&quot;) + y<br>    else:<br>        y=y<br>    print(f&#39;The Encoded Message goes as: {y}&#39;)<br>elif first_prompt==&#39;decode&#39;:<br>    x=input(&#39;Enter the Encoded Message: &#39;)<br>    last_x=x[len(x)-4]<br>    z=x.replace(x[0:3],&#39;&#39;).replace(x[len(x)-3:len(x)],&#39;&#39;)<br>    y=last_x + z.replace(x[len(x)-4],&#39;&#39;)<br>    print(f&#39;The Decoded Message goes as: {y}&#39;)<br>else:<br>    raise SyntaxError(&#39;Invalid Syntax&#39;)
