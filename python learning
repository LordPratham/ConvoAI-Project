import random<br>print(&quot;welcome to snake ğŸ, water ğŸ’¦, gun ğŸ”« game &quot;)<br>print(&quot;&quot;&quot; choose: <br>0 for snake<br>1 for water<br>2 for gun&quot;&quot;&quot;)<br>print(&quot;&quot;&quot;overview of game:<br>snake wins against the water<br>water wins against the gun<br>gun wins against the snake&quot;&quot;&quot;)<br>a=int(input(&quot;enter your choice:&quot;))<br>b=random.randint(0,2)<br>print(b)<br>def win(a,b):<br>  if(a&lt;3):<br>    if(a==b):<br>     print(&quot;it&#39;s a draw&quot;)<br>    elif(b&gt;a):<br>     print(&quot;you won the game&quot;)<br>    elif(a==2 and b==0):<br>     print(&quot;you won the game&quot;)<br>    else:<br>     print(&quot;you lose the game&quot;)<br>  else:<br>    print(&quot;you entered wrong option&quot;)<br>win(a,b)<br>I used simple if else
# &lt;&lt;-------  Sri Ganesha -  7crore -------&gt;&gt;<br># &lt;&lt;-------  Snake Water Gun Game-------&gt;&gt;<br>import random<br><br>########## Function #################<br><br>def start_game(dict):<br>  dict= {&#39;SS&#39;:&#39;D&#39;, &#39;SW&#39;:&#39;W&#39;, &#39;SG&#39;:&#39;L&#39;, &#39;WS&#39;:&#39;L&#39;, &#39;WW&#39;:&#39;D&#39;, &#39;WG&#39;:&#39;W&#39;, &#39;GS&#39;:&#39;W&#39;, &#39;GW&#39;:&#39;L&#39;, &#39;GG&#39;:&#39;D&#39; }<br>  UR_Choice=input(&#39;\nEnter your choice\n S for Snake\n W for Water\n G for Gun :- &#39;)<br>  Sys_Choice=random.choice([&#39;SNAKE&#39;,&#39;GUN&#39;,&#39;WATER&#39;])<br>  print(f&#39;Sys_Choice is {Sys_Choice}&#39;)<br>  Sys_Choice1= &#39;S&#39; if Sys_Choice==&#39;SNAKE&#39; else &#39;G&#39; if Sys_Choice==&#39;GUN&#39; else &#39;W&#39;<br>  # print(dict[UR_Choice+Sys_Choice1])77<br>  return(dict[UR_Choice+Sys_Choice1])<br>    <br>########## Script #################<br><br>print(&#39;\nwelcome to &lt;&lt;------- Snake Water Gun -------&gt;&gt;&#39;)<br>player_name=input(&#39;\nEnter player name &#39;)<br><br>dict= {&#39;SS&#39;:&#39;D&#39;, &#39;SW&#39;:&#39;W&#39;, &#39;SG&#39;:&#39;L&#39;, &#39;WS&#39;:&#39;L&#39;, &#39;WW&#39;:&#39;D&#39;, &#39;WG&#39;:&#39;W&#39;, &#39;GS&#39;:&#39;W&#39;, &#39;GW&#39;:&#39;L&#39;, &#39;GG&#39;:&#39;D&#39; }<br><br>RN=1<br>TP=0<br><br>f=open(&#39;myfile.txt&#39;)<br>High_Score=f.readlines()[-1]<br># print(High_Score)<br><br>Play=&#39;1&#39; # yes want to play<br>while (Play==&#39;1&#39;):<br>  print(&#39;\nLets Start Game&#39;)<br>  print(f&#39;\nRound No {RN}&#39;)<br>  result=start_game(dict)<br>  if result==&#39;W&#39;:<br>    print(&#39;\nHurreyyy.. you won the Game.. Congrats!!!!&#39;)<br>    TP+=2<br>    Play=input(&#39;\nDo you want to play another game? \n1:Yes \n0:No  &#39;)<br><br>  elif result==&#39;D&#39;:<br>    print(&#39;\nOhhh.. Game Draw.. No problem- you are still in Game!!!!&#39;)<br>    TP+=1<br>    Play=input(&#39;\nDo you want to play another game? \n1:Yes \n0:No &#39;)<br>    <br>  elif result==&#39;L&#39;:<br>    print(&#39;\nSorry.. you last Game .. No problem- better luck next time!!!! &#39;)<br>    print(f&#39;\n your total points {TP}&#39;)<br>    break<br>  RN+=1<br><br>HS=High_Score.split(&#39;-&#39;)<br>print(int(HS[1]))<br><br>if TP &gt; int(HS[1]):<br>  print(&#39;\nyou are the highest scorer till now... Cheers!!!!&#39;)<br>  with open(&#39;myfile.txt&#39;,&#39;a&#39;) as g:<br>    g.writelines(f&#39;\n{player_name} -{TP}&#39; )<br>else:<br>  print(f&#39;\nhighest score till now is {High_Score}&#39;)
import random<br><br>def get_player_choice():<br>    choice = input(&quot;Enter &#39;s&#39; for snake, &#39;w&#39; for water, &#39;g&#39; for gun: &quot;)<br>    return choice<br><br>def get_computer_choice():<br>    choices = [&#39;s&#39;, &#39;w&#39;, &#39;g&#39;]<br>    choice = random.choice(choices)<br>    return choice<br><br>def play_game(player_choice, computer_choice):<br>    if player_choice == computer_choice:<br>        print(&quot;Tie!&quot;)<br>    elif (player_choice == &#39;s&#39; and computer_choice == &#39;w&#39;) or (player_choice == &#39;w&#39; and computer_choice == &#39;g&#39;) or (player_choice == &#39;g&#39; and computer_choice == &#39;s&#39;):<br>        print(&quot;Congratulations! You win.&quot;)<br>    else:<br>        print(&quot;Sorry, you lose.&quot;)<br><br>def play_again():<br>    play_again = input(&quot;Do you want to play again? (y/n): &quot;)<br>    if play_again == &#39;y&#39;:<br>        return True<br>    else:<br>        return False<br><br># Main game loop<br>while True:<br>    player_choice = get_player_choice()<br>    computer_choice = get_computer_choice()<br>    print(f&quot;You chose {player_choice}. The computer chose {computer_choice}.&quot;)<br>    play_game(player_choice, computer_choice)<br>    if not play_again():<br>        break<br><br>print(&quot;Thanks for playing!&quot;)
# import random to generate ranfom integers among 0,1 and 2<br>import random<br>print(&quot;Lets play Snake,Water,Gun\n&quot;)<br><br># random integer from 0 to 2<br>computer = random.randint(0, 2)<br>player = int(input(&quot;Type 0 for snake, Type 1 for water and Type 2 for gun :- &quot;))<br><br># function that tests who win<br>def test(computer, player): <br>  print(f&quot;computer : {computer}&quot;)     <br>  print(f&quot;you : {player}&quot;)<br>  if computer == player:<br>    print(f&quot;Draw&quot;)<br>  elif computer == 1 and player ==0:<br>    print(&quot;You win&quot;)<br>  elif computer == 2 and player == 0:<br>    print(&quot;You lose&quot;)<br>  elif computer == 0 and player == 1:<br>    print(&quot;You lose&quot;)    <br>  elif computer == 2 and player == 1:<br>    print(&quot;You win&quot;)   <br>  elif computer == 0 and player == 2:<br>    print(&quot;You win&quot;)<br>  elif computer == 1 and player == 2:<br>    print(&quot;You lose&quot;)  <br>  else:<br>    player = int(input(&quot;Please, type 0 for snake, type 1 for water and type 2 for gun :- &quot;))<br>    test(computer, player)<br> <br># calling function   <br>test(computer, player)
