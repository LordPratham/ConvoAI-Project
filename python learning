SNAKE WATER GUN WITH ERROR HANDLING<br><br><br>def get_valid_input(player_name):<br>    while True:<br>        try:<br>            val = int(input(f&quot;{player_name} choose a value from Snake=0 or Water=1 or Gun=2: &quot;))<br>            if val not in [0, 1, 2]:<br>                raise ValueError(&quot;Invalid input. Please enter 0, 1, or 2.&quot;)<br>            return val<br>        except ValueError as e:<br>            print(f&quot;Error: {e}&quot;)<br><br>def func(matrix, items, plays, p1, p2):<br><br>    p1c = 0<br>    p2c = 0<br>    play = plays<br><br>    while play:<br>        valp1 = get_valid_input(p1)<br>        valp2 = get_valid_input(p2)<br><br>        if matrix[valp1][valp2] == &#39;D&#39;:<br>            print(&quot;It&#39;s a draw&quot;)<br>            p2c += 1<br>            p1c += 1<br>        elif matrix[valp1][valp2] == &#39;W&#39;:<br>            print(f&quot;{p1} won this round with {items[valp1]} by {items[valp2]} &quot;)<br>            p1c += 1<br>        else:<br>            print(f&quot;{p2} won this round with {items[valp2]} by {items[valp1]} &quot;)<br>            p2c += 1<br><br>        play -= 1<br><br>    if p2c &gt; p1c:<br>        print(f&quot;{p2} wins the game by {p2c - p1c} points in a point total of {plays}&quot;)<br>    elif p2c &lt; p1c:<br>        print(f&quot;{p1} wins the game by {p1c - p2c} points in a point total of {plays}&quot;)<br>    else:<br>        print(f&quot;It&#39;s a draw for both {p1} and {p2}&quot;)<br><br><br>matrix = [[&#39;D&#39;, &#39;W&#39;, &#39;L&#39;],<br>          [&#39;L&#39;, &#39;D&#39;, &#39;W&#39;],<br>          [&#39;W&#39;, &#39;L&#39;, &#39;D&#39;]]<br><br>items = [&#39;Snake&#39;, &#39;Water&#39;, &#39;Gun&#39;]<br>p1 = input(&quot;Enter player one name: &quot;)<br>p2 = input(&quot;Enter player two name: &quot;)<br>plays = int(input(&quot;Enter the total rounds to be played by p1 and p2: &quot;))<br>func(matrix, items, plays, p1, p2)
