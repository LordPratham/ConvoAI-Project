import random<br><br>def play_game(player_choice):<br>    choices = [&quot;Snake&quot;, &quot;Water&quot;, &quot;Gun&quot;]<br>    computer_choice = random.choice(choices)<br>    <br>    print(f&quot;Your choice: {player_choice}&quot;)<br>    print(f&quot;Computer&#39;s choice: {computer_choice}&quot;)<br>    <br>    if player_choice == computer_choice:<br>        return &quot;It&#39;s a tie!&quot;<br>    elif player_choice == &quot;Snake&quot;:<br>        if computer_choice == &quot;Water&quot;:<br>            return &quot;You win! Snake drinks water.&quot;<br>        else:<br>            return &quot;You lose! Gun kills the snake.&quot;<br>    elif player_choice == &quot;Water&quot;:<br>        if computer_choice == &quot;Gun&quot;:<br>            return &quot;You win! Water puts out the gun.&quot;<br>        else:<br>            return &quot;You lose! Snake drinks water.&quot;<br>    elif player_choice == &quot;Gun&quot;:<br>        if computer_choice == &quot;Snake&quot;:<br>            return &quot;You win! Gun kills the snake.&quot;<br>        else:<br>            return &quot;You lose! Water puts out the gun.&quot;<br>    else:<br>        return &quot;Invalid choice. Please choose Snake, Water, or Gun.&quot;<br><br>if <i>_name_</i> == &quot;__main__&quot;:<br>    print(&quot;Welcome to Snake, Water, Gun Game!&quot;)<br>    player_choice = input(&quot;Enter your choice (Snake/Water/Gun): &quot;).capitalize()<br>    <br>    result = play_game(player_choice)<br>    print(result)
import random<br><br>def check(comp,user):<br>    if comp == user:<br>        return 0<br>        <br>    if (comp == 0 and user == 1):<br>        return -1<br>        <br>    if (comp == 1 and user == 2):<br>        return -1<br>        <br>    if (comp == 2 and user == 0):<br>        return -1<br>    <br>    return 1<br>        <br>comp = random.randint(0,2)<br>user = int(input(&quot;0 for Snake,1 for Water,2 for Gun : &quot;))<br>        <br>score = check(comp,user)<br>print(&quot;You:&quot;,user)<br>print(&quot;Computer:&quot;,comp)<br><br><br>if (score == 0):<br>    print(&quot;Its a draw&quot;)<br>elif (score == -1):<br>    print(&quot;You lose&quot;)<br>else:<br>    print(&quot;You Won&quot;)
def game():<br>    import random<br>    l=[&quot;SNAKE&quot;,&quot;WATER&quot;,&quot;GUN&quot;]<br>    computer_choice=random.choice(l)<br><br>    player=0<br>    computer=0<br>    play_again=0<br>    play_again=True<br>    for i in range(3):<br>        player_choice=input(&quot;enter any one snake or water or gun &quot;)<br>        player_choice=player_choice.upper().strip()<br>        if (player_choice==&quot;SNAKE&quot; and computer_choice==&quot;WATER&quot;):<br>            print(&quot;SNAKE IS THE WINNER , YOU ARE THE WIINER&quot;)<br>            player=player+1<br>        elif (player_choice==&quot;SNAKE&quot; and computer_choice==&quot;SNAKE&quot;):<br>            print(&quot;THIS GAME IS DRAW&quot;)<br>        elif (player_choice==&quot;SNAKE&quot; and computer_choice==&quot;GUN&quot;):<br>            print(&quot;GUN IS THE WINNER , COMPUTER IS THE WIINER&quot;)<br>            computer=computer+1<br>        elif (player_choice==&quot;WATER&quot; and computer_choice==&quot;GUN&quot;):<br>            print(&quot;WATER IS THE WINNER ,YOU ARE THE WIINER&quot;)<br>            player=player+1<br>        elif (player_choice==&quot;WATER&quot; and computer_choice==&quot;WATER&quot;):<br>            print(&quot;THIS GAME IS DRAW&quot;)<br><br>        elif (player_choice==&quot;WATER&quot; and computer_choice==&quot;SNAKE&quot;):<br>            print(&quot;SNAKE IS THE WINNER ,COMPUTER IS THE WIINER&quot;)<br>            computer=computer+1<br><br>        elif (player_choice==&quot;GUN&quot; and computer_choice==&quot;WATER&quot;):<br>            print(&quot;WATER IS THE WINNER ,COMPUTER IS THE WIINER&quot;)<br>            computer=computer+1<br><br>        elif (player_choice==&quot;GUN&quot; and computer_choice==&quot;GUN&quot;):<br>            print(&quot;THIS GAME IS DRAW&quot;)<br><br>        elif (player_choice==&quot;GUN&quot; and computer_choice==&quot;SNAKE&quot;):<br>            print(&quot;GUN IS THE WINNER ,YOU ARE THE WIINER&quot;)<br>            player=player+1<br><br>        else :<br>            raise ValueError(&quot;you enter the wrong input&quot;)<br><br>    if(player&gt;computer):<br>        print(f&quot;YOU ARE THE WINNER WITH THIS SCORE {player}&quot;)<br>    elif (computer&gt;player):<br>        print(f&quot;COMPUTER THE WINNER WITH THIS SCORE {computer}&quot;)<br>    else:<br>        print(f&quot;THIS GAME IS DRAW, COMPUTER={computer} AND PLAYER={player}&quot;)<br>    play_again=int(input(print(&quot;do you want to quit=1 or play=0 &quot;)))<br>    if (play_again==0):<br>        game()<br>    elif (play_again==1):<br>        print (&quot;THANKYOU&quot;)<br>    else:<br>        raise ValueError(&quot;invaild input&quot;)<br><br>game()
