from numpy import array,matrix<br><br>arr1 = array([<br>    [4, 5, 9, 5],<br>    [-8, 5, 4, 8],<br>    [0, 7, 5, 8]<br>    ])<br><br>arr2 = array([<br>    [7, 6, 8],<br>    [-4, 1, 9],<br>    [8, -5, 4],<br>    [9, 0, 4],<br>    ])<br><br>arr = []<br><br>for irow in range(len(arr1)):<br>    for jcol in range(len(arr2[0])):<br>        sum = 0<br>        for icol in range(len(arr1[0])):<br>            sum += arr1[irow][icol] * arr2[icol][jcol]<br>        arr.append(sum)<br><br>arr = matrix(arr).reshape(len(arr1), len(arr2[0]))<br>print(&#39;Using loop:&#39;)<br>print(arr)<br><br>print(&#39;Using builtin function:&#39;)<br>arr1 = matrix(arr1)<br>arr2 = matrix(arr2)<br>print(arr1 * arr2)
import numpy as np<br>m1=np.matrix([[1,2,3],[4,5,6]])<br>m2=np.matrix([[7,8],[9,10],[11,12]])<br>lst=[]<br><br>if(int(m1.shape[1]) == int(m2.shape[0])):<br>    print(&quot;logic can be started&quot;)<br>    for i in np.arange(0,int(m1.shape[0])):<br>        for k in np.arange(0,int(m2.shape[1])):<br>            val=0<br>            for j in np.arange(0, int(m2.shape[0])):<br>                val += int(m1[i, j] * m2[j, k])<br>            lst.append(val)<br>else:<br>    print(&quot;Not fitted to do matrix multiplication&quot;)<br>matrixmul= np.matrix(lst)<br>matrixmul_reshape=matrixmul.reshape(int(m1.shape[0]),int(m2.shape[1]))<br>print(matrixmul_reshape)<br><br>m3=m1*m2<br>print(m3)
