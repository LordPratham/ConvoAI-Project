def enc():<br>    global new_str<br>    global new_list<br>    import random <br>    l=[&#39;a&#39;,&#39;b&#39;,&#39;c&#39;,&#39;d&#39;,&#39;e&#39;,&#39;f&#39;]<br><br>    word=input(&quot;enter the string =&quot;)<br>    new_word=word.split()<br>    print(new_word)<br>    new_list=[]<br>    for i in new_word:<br>        new_str=i[1:len(i)]<br>        new_str=new_str+i[0]     ## code runs till replacing the first letter to last letter <br>        a=random.randrange(0,6)<br>        b=random.randrange(0,6)<br>        c=random.randrange(0,6)<br>        new_str=l[c]+l[a]+l[b]+new_str<br>        new_list.append(new_str)<br><br>    print(&quot;the encrypted msg is here =&quot;)<br>    for d in new_list:<br>        print(&quot;\t\t\t&quot;,d,end=&quot;\n&quot;)<br>    print(&quot;&quot;)<br>    <br><br>def dec():                                                   ## decoding the sentence <br>    global org<br>    dec=input(&quot;enter the msg to decode = &quot;)<br>    new_list2=dec.split()<br>    print(&#39;the decoded msg is here =&#39;)<br>    for i in new_list2:<br>        org1=i[3:len(i)-1]<br>        org=i[-1]+org1<br>        print(&quot;\t\t\t&quot;,org)<br>    print(&quot;&quot;)<br><br>def write():                                                               ## writting the encryption here in a file <br>    file=open(r&quot;F:\python vscode files\secret encryptions.txt&quot;,&quot;w&quot;)        <br>    for l in new_list:<br>        file.write(l)<br>        file.write(&quot; &quot;)   <br>    file.close()<br>    print(&quot;&quot;)<br># front end <br>if __name__==&quot;__main__&quot;:<br>    while True:<br>        print(&quot;+&quot;,&quot;-&quot;*37,&quot;+&quot;)<br>        print(&quot;|\t1. Encryption\t\t\t|&quot;)<br>        print(&quot;|\t2. Decoding\t\t\t|&quot;)<br>        print(&quot;|\t3. Copy the contents in file \t|&quot;)<br>        print(&quot;|\t4. Exit\t\t\t\t|&quot;)<br>        print(&quot;+&quot;,&quot;-&quot;*37,&quot;+&quot;)<br>        print(&quot; &quot;)<br>        ch=int(input(&quot;enter the choice =&quot;))<br>        if ch==1:<br>            enc()<br>            print(&quot;enter any other choice =&quot;)<br>        if ch==2:<br>            dec()<br>            print(&quot;enter any other choice =&quot;)<br>        if ch==4:<br>            break<br>        if ch==3:<br>            write()<br>            print(&quot;enter any other choice =&quot;)
