import random<br><br># Define a list of questions and their correct answers<br>questions = [<br>    {&quot;question&quot;: &quot;What is the capital of France?&quot;, &quot;options&quot;: [&quot;A. Paris&quot;, &quot;B. London&quot;, &quot;C. Berlin&quot;, &quot;D. Madrid&quot;], &quot;answer&quot;: &quot;A&quot;},<br>    {&quot;question&quot;: &quot;Which planet is known as the &#39;Red Planet&#39;?&quot;, &quot;options&quot;: [&quot;A. Venus&quot;, &quot;B. Mars&quot;, &quot;C. Jupiter&quot;, &quot;D. Saturn&quot;], &quot;answer&quot;: &quot;B&quot;},<br>    {&quot;question&quot;: &quot;Who wrote &#39;Romeo and Juliet&#39;?&quot;, &quot;options&quot;: [&quot;A. Charles Dickens&quot;, &quot;B. William Shakespeare&quot;, &quot;C. Mark Twain&quot;, &quot;D. Jane Austen&quot;], &quot;answer&quot;: &quot;B&quot;},<br>    {&quot;question&quot;: &quot;What is the largest mammal in the world?&quot;, &quot;options&quot;: [&quot;A. Elephant&quot;, &quot;B. Giraffe&quot;, &quot;C. Blue Whale&quot;, &quot;D. Lion&quot;], &quot;answer&quot;: &quot;C&quot;},<br>    {&quot;question&quot;: &quot;What is the chemical symbol for gold?&quot;, &quot;options&quot;: [&quot;A. Go&quot;, &quot;B. Gd&quot;, &quot;C. Au&quot;, &quot;D. Ag&quot;], &quot;answer&quot;: &quot;C&quot;},<br>]<br><br># Initialize variables for game<br>total_questions = len(questions)<br>current_question = 0<br>total_prize = 0<br><br># Shuffle the questions to make them random<br>random.shuffle(questions)<br><br># Main game loop<br>while current_question &lt; total_questions:<br>    question_data = questions[current_question]<br>    question = question_data[&quot;question&quot;]<br>    options = question_data[&quot;options&quot;]<br>    correct_answer = question_data[&quot;answer&quot;]<br>    <br>    # Display the question and options<br>    print(f&quot;\nQuestion {current_question + 1}: {question}&quot;)<br>    for option in options:<br>        print(option)<br>    <br>    # Get user&#39;s answer<br>    user_answer = input(&quot;Enter your choice (A, B, C, or D): &quot;).strip().upper()<br>    <br>    # Check if the user&#39;s answer is correct<br>    if user_answer == correct_answer:<br>        print(&quot;Correct answer! You won some money.&quot;)<br>        total_prize += 10000  # You can adjust the prize money as per your game rules.<br>        current_question += 1<br>    else:<br>        print(&quot;Sorry, that&#39;s incorrect. Game over!&quot;)<br>        break<br><br># Display the total prize won by the player<br>print(f&quot;\nCongratulations! You won a total prize of ${total_prize}&quot;)
