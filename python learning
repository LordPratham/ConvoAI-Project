def CheckNames(names):<br>    results = []<br>    greaterThanfive = 0<br>    lessThanFive = 0<br><br>    for name in names:<br>        if len(name) &gt; 5:<br>            results.append(name[0:1].upper() + name[1:])<br>            greaterThanfive += 1<br>        else:<br>            lessThanFive += 1<br><br>    return results, greaterThanfive, lessThanFive<br><br><br>inputNames = []<br><br>for i in range(10):<br>    inputNames.append(input(&#39;Please enter user name {} &gt;&gt; &#39;.format(i + 1)))<br><br>resNames, longNames, shortNames = CheckNames(inputNames)<br>print(&quot;Names are &gt;&gt; {} and longCounter = {} and shortCounter = {} &quot;.format(resNames, longNames, shortNames))
