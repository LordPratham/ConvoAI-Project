from numpy import*<br><br>m = int(input(&#39;1st Matrix:- Enter the no. of rows: &#39;))<br>n = int(input(&#39;1st Matrix:- Enter the no. of columns: &#39;))<br>x = matrix([0 for i in range(m*n)])<br>for i in range(m*n):<br>    x[0,i] = int(input(&quot;Enter the element for Matrix 1: &quot;))<br><br>r = int(input(&#39;2nd Matrix:- Enter the no. of rows: &#39;))<br>p = int(input(&#39;2nd Matrix:- Enter the no. of columns: &#39;))<br>y = matrix([0 for i in range(r*p)])<br>for i in range(r*p):<br>    y[0,i] = int(input(&quot;Enter the element for Matrix 2: &quot;))<br><br>m1 = x.reshape(m,n)<br>m2 = y.reshape(r,p)<br>m3 = m1 * m2<br>print(&quot;First Matrix is below:- \n&quot;, m1, &quot;\n&quot;, &quot;Second Matrix is below:- \n&quot;, m2, &quot;\n&quot;, &quot;Multiplication of Matrix:- \n&quot;, m3)
