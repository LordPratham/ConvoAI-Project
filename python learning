KBC = [&quot;welcome to KBC&quot;]<br>question = [&quot;what is captial city of india&quot;]<br>option1 = [&quot; hyderabad &quot;]<br>option2 = [&quot;spain&quot;]<br>option3 = [&quot;dehli&quot;]<br>option4 = [&quot;dubai&quot;]<br>options = [option1 , option2 ,option3, option4]<br>answer = option1<br>price = [1000]<br>lst = [i for i in options if i == answer and price]<br>print(lst)<br>print(price)<br>print(KBC)
