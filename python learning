import random<br><br><br>def user_choice():<br>  user_input = input(&quot;Select Rock,Paper,scissor:&quot;)<br>  if user_input in [&quot;rock&quot;, &quot;paper&quot;, &quot;scissor&quot;]:<br>    return user_input.lower()<br>  else:<br>    print(&quot;Invalid choice&quot;)<br><br><br>def computer_choice():<br>  computer_input = random.choice([&quot;rock&quot;, &quot;paper&quot;, &quot;scissor&quot;])<br>  return computer_input<br><br><br>def winner_determine(user_choice, computer_choice):<br>  if user_choice == computer_choice:<br>    return &quot;Draw&quot;<br>  elif (user_choice == &quot;rock&quot; and computer_choice == &quot;scissor&quot; or \<br>         user_choice == &quot;scissor&quot; and computer_choice == &quot;paper&quot; or\<br>         user_choice == &quot;paper&quot; and computer_choice == &quot;stone&quot;):<br>    return &quot;You Won&quot;<br><br>  else:<br>    return &quot;Computer Won&quot;<br><br><br>def play_game():<br>    user_choic = user_choice()<br>    computer_choic = computer_choice() <br>    print(f&quot;\nYou Choose: {user_choic}  Computer Choose: {computer_choic}&quot;)<br><br>    x=winner_determine(user_choic,computer_choic)<br>    print(x)<br><br><br>play_game()
