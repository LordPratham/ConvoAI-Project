import random<br>user_score = 0<br>comp_score = 0<br>for i in range(1,4):<br>  jk = (1, 2, 3)<br>  rounds = {1:&quot;first&quot;,2:&quot;second&quot;,3:&quot;third&quot;,4:&quot;fourth&quot;,5:&quot;fifth&quot;,6:&quot;sixth&quot;,7:&quot;seventh&quot;,8:&quot;eighth&quot;,9:&quot;ninth&quot;,10:&quot;tenth&quot;}<br>  pk = rounds.get(i)<br>  print(f&quot;{pk} ROUND STARTS&quot;)<br>  user = int(input(&quot;1 for snake\n2 for water\n3 for gun\n&quot;))<br>  if user == 1:<br>               print(&quot;you have selected snake&quot;)<br>  if user == 2:<br>               print(&quot;you have selected water&quot;)<br>  if user == 3:<br>             print(&quot;you have selected gun&quot;)<br>  comp = (random.choice(jk))<br>  if comp == 1:<br>               print(&quot;comp have selected snake&quot;)<br>  if comp == 2:<br>               print(&quot;comp have selected water&quot;)<br>  if comp == 3:<br>             print(&quot;comp have selected gun&quot;)<br>  if user == comp:<br>        print(&quot;MATCH DRAW&quot;)<br>        user_score = user_score<br>        comp_score = comp_score<br>  elif user == 1 and comp == 2:<br>          print(&quot;you won&quot;)<br>          user_score = user_score +1<br>          comp_score = comp_score -1<br>  elif user ==1 and comp ==3:<br>          print(&quot;comp won&quot;)<br>          comp_score == comp_score + 1<br>          user_score = user_score -1<br>  elif user == 2 and comp == 3:<br>          print(&quot;you won&quot;)<br>          user_score = user_score +1<br>          comp_score = comp_score -1<br>  elif user ==2 and comp ==1:<br>          print(&quot;comp won&quot;)<br>          comp_score == comp_score + 1<br>          user_score = user_score -1<br>  elif user == 3 and comp == 1:<br>          print(&quot;you won&quot;)<br>          user_score = user_score +1<br>          comp_score = comp_score -1<br>  elif user ==3 and comp ==2:<br>          print(&quot;comp won&quot;)<br>          comp_score == comp_score + 1<br>          user_score = user_score -1<br>  print(f&quot;AT END OF {pk} ROUND SCORE IS {user_score}\nCOMP SCORE IS {comp_score}&quot;)<br>  if i == 3:<br>    if comp_score &gt; user_score:<br>      print(&quot;       COMP HAS WON THE MATCH&quot;)<br>    else:<br>      print(&quot;       YOU HAVE WON THE MATCH&quot;)
