n=int(input())<br>m=int(input())<br>print(&quot;add of the two number: &quot;, n+m)<br>print(&quot;subtract of the two number: &quot;,n-m)<br>print(&quot;multiplication of the two number: &quot;,n*m)<br>print(&quot;division of the two number :&quot;,n/m)<br>print(&quot;modulus of the two number: &quot;,n%m)<br>print(&quot;folor division of two number :&quot;,n//m)
# Simple Calculator<br><br>a = int(input(&quot;Enter first number: &quot;))<br>b = int(input(&quot;Enter second number: &quot;))<br><br>print(f&quot;\nThe Addition of {a} + {b} =&quot;,a+b) # Addition<br>print(f&quot;The Subraction of {a} - {b} =&quot;,a-b) # Subraction<br>print(f&quot;The Addition of {a} * {b} =&quot;,a*b) # Multiplication<br>print(f&quot;The Division of {a} / {b} =&quot;,a/b) # Division<br>print(f&quot;The Modulus of {a} % {b} =&quot;,a%b) # Modulus<br>print(f&quot;The Floor Division of {a} // {b} =&quot;,a//b) # floor Division<br>print(f&quot;The Exponential of {a} ** {b} =&quot;,a**b) # Exponential
