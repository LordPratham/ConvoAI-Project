class Liberary:<br><br>    def __init__(self):<br>        self.no_of_books = 0<br>        self.books = []<br><br>    def display_books(self):<br>        if self.no_of_books==0:<br>            print(&quot;No books in the library. &quot;)<br>        else:<br>            print(&quot;Books in the liberary: &quot;)<br>            for book in self.books:<br>                print(book)<br><br>    def add_book(self,book_title):<br>        self.books.append(book_title)<br>        self.no_of_books+=1<br>        print(f&quot;Book &#39;{book_title}&#39; added to the liberary. &quot;)<br><br>    def get_no_of_books(self):<br>        return self.no_of_books
