from random import randint<br><br><br>print(&quot;Welcome to Stone paper scissors Game&quot;)<br><br><br>l=[&#39;rock&#39;,&#39;paper&#39;,&#39;scissors&#39;]<br>comp=l[randint(0,2)]<br><br>player_score=0<br>comp_score=0<br>rounds=int(input(&quot;Enter the number of rounds you wanna play: &quot;))<br>for i in range(0,rounds):<br>    player = input(&quot;Enter Rock, Paper or Scissors: &quot;).lower()        <br>    if(player==comp):<br>        print(&quot;Tie!&quot;)<br><br>    elif(player==&#39;rock&#39;):<br>        if(comp==&#39;scissors&#39;):<br>            print(f&quot;You Won! {player} smashes {comp}&quot;)<br>            player_score=player_score+1<br>            print(f&quot;Players Score is {player_score}&quot;)<br>                <br>        else:<br>            print(f&quot;You Lose! {comp} smashes {player}&quot;)<br>            comp_score=comp_score+1<br>            print(f&quot;Computers Score is {comp_score}&quot;)<br>                <br><br>    elif(player==&#39;scissors&#39;):<br>        if(comp==&#39;rock&#39;):<br>            print(f&quot;You Won! {player} smashes {comp}&quot;)<br>            player_score=player_score+1<br>            print(f&quot;Players Score is {player_score}&quot;)<br>                <br>                    <br>        else:<br>            print(f&quot;You Lose! {comp} smashes {player}&quot;)<br>            comp_score=comp_score+1<br>            print(f&quot;Computers Score is {comp_score}&quot;)<br>            <br><br>    else:<br>        if(comp==&#39;rock&#39;):<br>            print(f&quot;You Won! {player} smashes {comp}&quot;)<br>            player_score=player_score+1<br>            print(f&quot;Players Score is {player_score}&quot;)<br>                <br>                    <br>        else:<br>            print(f&quot;You Lose! {comp} smashes {player}&quot;)<br>            comp_score=comp_score+1<br>            print(f&quot;Computers Score is {comp_score}&quot;) <br><br>if(player_score&gt;comp_score):<br>    print(&quot;You&#39;ve Won&quot;)<br>    print(f&quot;Players score is:{player_score} &quot;)<br><br>elif(player_score==comp_score):<br>    print(&quot;Its a Draw&quot;)<br><br>else:<br>    print(&quot;You Lose&quot;)<br>    print(f&quot;Computer score is:{comp_score} &quot;)
# Exercise 5<br><br># Rock Paper Scissor<br><br>import random<br>flag = 0<br><br>dic = {0:&#39;Rock&#39;, 1:&#39;Paper&#39;, 2:&#39;Scissor&#39;}<br>i = 0<br>print(&quot;Rock : Paper : Scissor&quot;)<br>while(i &lt; 3):<br>    print(f&quot;~ Round {i} ~&quot;)<br>    <br>    user = str(input(&quot;You : &quot;))<br><br>    comp = str(random.choice([&#39;Rock&#39;, &#39;Paper&#39;, &#39;Scissor&#39;]))<br>    print(f&quot;Computer : {comp}&quot;) <br><br>    for key in dic:<br>        if dic[key] == user :<br>            x = key<br><br>        if dic[key] == comp:<br>            y = key<br>    if x != y : i += 1<br>    j = 0 <br>    match(x, y):<br>        case 0, 1 : print(&quot;ğŸ‘&quot;)   <br>        case 0, 2 : <br>            flag += 1 <br>            print(&quot;ğŸ‘&quot;)<br>        case 1, 0 : <br>            flag += 1 <br>            print(&quot;ğŸ‘&quot;)   <br>        case 1, 2 : print(&quot;ğŸ‘&quot;) <br>        case 2, 1 : <br>            flag += 1 <br>            print(&quot;ğŸ‘&quot;)<br>        case 2, 0 : print(&quot;ğŸ‘&quot;)<br>    <br>        <br>        <br><br>    print()   <br><br>print()<br>if flag == 2 :<br>    print(&quot;ğŸ‰ğŸ‰Winner&quot;)<br><br>else:<br>    print(&quot;ğŸ‘ğŸ‘Loser&quot;)
import random<br><br>def IncreaseScore():<br>    global score<br>    score += 1<br><br>def DecreaseScore():<br>    global score<br>    score -= 1<br><br>def Result(pair):<br>    dict = {1: [(&quot;Snake&quot;, &quot;Water&quot;), (&quot;Water&quot;, &quot;Gun&quot;), (&quot;Gun&quot;, &quot;Snake&quot;)], <br>            -1: [(&quot;Snake&quot;, &quot;Gun&quot;), (&quot;Water&quot;, &quot;Snake&quot;), (&quot;Gun&quot;, &quot;Water&quot;)],  <br>            0: [(&quot;Snake&quot;, &quot;Snake&quot;), (&quot;Water&quot;, &quot;Water&quot;), (&quot;Gun&quot;, &quot;Gun&quot;)]}<br><br>    for key,value in dict.items():<br>        if pair in value:<br>            res = key<br>            break<br>    <br>    if res == 1:<br>        print(&quot;\nYou have won the match.\n&quot;)<br>        IncreaseScore()<br>        con = input(&quot;Continue or not press (Y|N) &quot;)<br>        if con.lower() == &quot;y&quot;:<br>            Game()<br>        else:<br>            print(f&quot;Your score is  {score}&quot;)<br>            print(&quot;Thankyou&quot;)<br>    elif res == -1:<br>        print(&quot;\nYou lost the game.\n&quot;)<br>        DecreaseScore()<br>        con = input(&quot;Continue or not press (Y|N) &quot;)<br>        if con.lower() == &quot;y&quot;:<br>            Game()<br>        else:<br>            print(f&quot;Your score is  {score}&quot;)<br>            print(&quot;Thankyou&quot;)<br>    else:<br>        print(&quot;\nMatch draw\n&quot;)<br>        Game()<br>    <br>def Game():<br>    print(&quot;Choose anyone:&quot;)<br>    print(&quot;\tSnake\tWater\tGun\n\t  0\t  1\t 2&quot;)<br>    p = int(input(&quot;Enter your choice (0 1 2): &quot;))<br>    choice = [&quot;Snake&quot;, &quot;Water&quot;, &quot;Gun&quot;]<br><br>    player = choice[p]<br>    computer = choice[random.randrange(3)]<br>    print(&quot;Your choice:\t &quot;, player)<br>    print(&quot;Computer choice: &quot;, computer)<br>    <br>    pair = (player, computer)<br>    Result(pair)<br><br>if <i>_name_</i> == &quot;__main__&quot;:<br>    print(&quot;-&quot;*20, &quot;Welcome To SnakeWaterGun Game&quot;, &quot;-&quot;*20)<br>    score = 0<br>    Game()
import random<br><br><br>def Determine_winner(player_choice, computer_choice):<br>    if player_choice == computer_choice:<br>        return &#39;its a tie&#39;<br><br>    if player_choice == &#39;snake&#39;:<br>        if computer_choice == &#39;gun&#39;:<br>            return &#39;computer win&#39;<br>        elif computer_choice == &#39;water&#39;:<br>            return &#39;you win&#39;<br><br>    if player_choice == &#39;water&#39;:<br>        if computer_choice == &#39;gun&#39;:<br>            return &#39;computer win&#39;<br>        elif computer_choice == &#39;snake&#39;:<br>            return &#39;you win&#39;<br><br>    if player_choice == &#39;gun&#39;:<br>        if computer_choice == &#39;snake&#39;:<br>            return &#39;computer win&#39;<br>        elif computer_choice == &#39;water&#39;:<br>            return &#39;you win&#39;<br><br><br>def playgame():<br>    choice = (&quot;snake&quot;, &quot;gun&quot;, &quot;water&quot;)<br><br>    print(&#39;Hello Welcome to snake gunn water game&#39;)<br>    print(&#39;select your choice from 1.snake 2.water 3.gun&#39;)<br><br>    while True:<br>        player_choice = input(&#39;Enter your choice:&#39;)<br><br>        if player_choice not in choice:<br>            print(&#39;invalid choice&#39;)<br>            continue<br><br>        computer_choice = random.choice(choice)<br>        print(&#39;computer choice:&#39;, computer_choice)<br><br>        result = Determine_winner(player_choice, computer_choice)<br>        print(result)<br><br>        play_again = input(&#39;If you want to play again the enter yes or no: &#39;).lower()<br>        if play_again != &#39;yes&#39;:<br>            break<br>    print(&#39;Thanks for playing&#39;)<br><br><br>playgame()
game_start = True<br>game_item = [&#39;gun&#39;, &#39;water&#39;, &#39;snake&#39;]<br>result = bool()<br>def game_starting_point(game_item, result):<br>    global game_start<br>    while (game_start):  <br>        comp = random.choice(game_item)<br>        print(f&#39;computer Turn:{comp}&#39;)<br>        player = input(&#39;Enter your weapon==&gt;[Gun, Water, Snake] and &quot;q&quot; to quit the game\nPlayer Turn:&#39;)<br>    <br>        if (comp == &#39;snake&#39;):          <br>            if (player == &#39;water&#39;):<br>                result = False<br>            else:<br>                result = True<br>        if  (comp == &#39;water&#39;):<br>            if (player == &#39;snake&#39;):<br>                result = True<br>            else:<br>                result = False<br>        if (comp == &#39;gun&#39;):<br>            if (player == &#39;water&#39;):<br>                result = True<br>            else:<br>                result = False<br>        break<br>    if (player == &#39;q&#39;):<br>        print(&#39;Thankq for playing our GAME!&#39;)<br>        game_start = False<br>    def game_result():<br>            if comp == player:<br>                print(&#39;Your match is Tie!&#39;)<br>            elif result == True:<br>                print(&#39;You Won&#39;)<br>            else:<br>                if result == False:<br>                    print(&#39;You Lose&#39;) <br>            person = input(&quot;Do you like play again press &#39;n&#39; or &#39;y&#39;=&gt;&quot;)<br>            if (person == &#39;y&#39;):<br>                game_starting_point(game_item, result)<br>            else:<br>                print(&#39;Thankq for playing our GAME!&#39;)<br>                game_start = False<br>        <br>    game_result()<br><br>if __name__==&quot;__main__&quot;:<br>    game_starting_point(game_item, result)
