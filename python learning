def Play_KBC():<br>    Questions=[&#39;\n1. Who is the Prime Minister of India \n \n A. NITISH KUAMR \n B. MOMADH SHAMI \n C. NARENDRA MODI \n D. NERAV MODI&#39;,&#39;\n2. Who is the Chief Minister of Bihar. \n \n A. ADTIYA NATH YOGI \n B. LALU YADAV \n C. NITISH KUMAR \n D. DEEPAK KUAMR&#39;,&#39;\n3. What is Name of Python Developer.\n \n A. JAMES GOSLING \n B. TIM BERNERS-LEE \n C. GUDIO VAN ROSSUM \n D. DENNIS RITCHIE&#39;,&#39;\n4. What is Name of Java Developer.\n \n A. TIM BERNERS-LEE \n B. GUDIO VAN ROSSUM \n C. JAMES GOSLING \n D. DENNIS RITCHIE &#39;]<br>    Answers=[&quot;NARENDRA MODI&quot;,&quot;NITISH KUMAR&quot;,&quot;GUDIO VAN ROSSUM&quot;,&quot;JAMES GOSLING&quot;]<br><br><br>    Messages=str(&quot;\nEnter Your Answer :&quot;)<br>    Right=(&quot;Your Answr is Right\n&quot;)<br>    Wrong=(&quot;\nYour Answer is Wrong \n&quot;)<br>    Answer=(&quot;Right Answer is....&quot;)<br>    Q1=Questions[0]<br>    Q2=Questions[1]<br>    Q3=Questions[2]<br>    Q4=Questions[3]<br><br>    <a href=""http://www.youtube.com/results?search_query=%23right"">#Right</a> answer win price<br>    Q1_Price=int(0)<br>    Q2_Price=int(0)<br>    Q3_Price=int(0)<br>    Q4_Price=int(0)<br>    Congrats=(&quot;congratulations You are Won&quot;)<br><br>    <a href=""http://www.youtube.com/results?search_query=%23question_1"">#Question_1</a><br>    print(Q1)<br>    Message=input(Messages)<br>    if Message==Answers[0]:<br>        print(Right)<br>        Q1_Price=int(100000)<br>    else: <br>        print(Wrong)<br>        print(Answer,Answers[0])<br><br><br>    <a href=""http://www.youtube.com/results?search_query=%23question_2"">#Question_2</a><br>    print(Q2)<br>    Message=input(Messages)<br>    if Message==Answers[1]:<br>        print(Right)<br>        Q2_Price=int(200000)<br><br>    else:<br>        print(Wrong)<br>        print(Answer,Answers[1])<br><br>    <a href=""http://www.youtube.com/results?search_query=%23question"">#Question</a>-3<br>    print(Q3)<br>    Message=input(Messages)<br>    if Message==Answers[2]:<br>        print(Right)<br>        Q3_Price=int(400000)<br>        print(Congrats,Q1_Price+Q2_Price+Q3_Price)<br>    else:<br>        print(Wrong)<br>        print(Answer,Answers[2])<br><br>    # Question-4<br>    print(Q4)<br>    Message=input(Messages)<br>    if Message==Answers[3]:<br>        print(Right)<br>        Q4_Price=int(800000)<br>        print(Congrats,Q1_Price+Q2_Price+Q3_Price+Q4_Price)<br>    else:<br>        print(Wrong)<br>        print(Answer,Answers[3])<br>        print(Congrats,Q1_Price+Q2_Price+Q3_Price+Q4_Price)<br>Play_KBC()
