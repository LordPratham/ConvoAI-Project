import random<br>def randomizor(a):<br>  front=random.choices(&quot;abcdefghijklmnopqrstuvwxyz&quot;,k=3)<br>  back=random.choices(&quot;abcdefghijklmnopqrstuvwxyz&quot;,k=3)<br>  while front==back:<br>    back= random.choices(&quot;abcdefghijklmnopqrstuvwxyz&quot;,k=3)<br>  return &quot;&quot;.join(front)+a[1:]+a[0]+&quot;&quot;.join(back)<br>def encode():<br>    l=[]<br>    while True:<br>        a=input(&quot;Enter your message:&quot;)<br>        l1=a.split()<br>        for i in l1:<br>            if len(i)&lt;=2:<br>                i=i[::-1]<br>                l.append(i)<br>            else:<br>                i=randomizor(i)<br>                l.append(i)<br>        encoded_string=str.join(&quot; &quot;,l)<br>        print(&quot;Your encoded string is:&quot;,encoded_string,&quot;\nplease copy it and send&quot;)<br>        ch=input(&quot;Do you want to encode more:y/n&quot;)<br>        if ch not in &quot;Yy&quot;:<br>            break<br>def decode():<br>    a=input(&quot;Enter your encoded string:&quot;)<br>    l1=a.split()<br>    l=[]<br>    for i in l1:<br>        if len(i)&lt;=2:<br>            i=i[::-1]<br>            l.append(i)<br>        else:<br>            i=i[-4]+i[3:-4]<br>            l.append(i)<br>    decoded_string=str.join(&quot; &quot;,l)<br>    print(&quot;Your decoded string is:&quot;,decoded_string)<br>x=int(input(&quot;What you want to do\n Enter your choice:\n1.Encode\n2.Decode\n&quot;))<br>match x:<br>    case 1:<br>        encode()<br>    case 2:<br>        decode()
import string<br>import random<br><br><br><br>print()<br>txt=&#39;--CODE DECODE CENTER--&#39;<br>print(txt.center(50))<br>print()<br>print(&#39;Enter your Choice: &#39;)<br>print()<br>print(&#39;Press 1 for Encoding, 2 for Decoding and 3 to Quit&#39;)<br>a=int(input(&#39;&gt;&gt;&gt;&#39;))<br>N=3<br>b=0<br>c=0<br>x=0<br>x1=0<br>b1=0<br>s=&#39;&#39;<br><br>def Encode(n):<br>  <br>  <br>  l=n.split(&#39; &#39;)<br>  <br>  s=&#39;&#39;<br>  <br>  res = &#39;&#39;.join(random.choices(string.ascii_letters, k=N))<br>  res1 = &#39;&#39;.join(random.choices(string.ascii_letters, k=N))<br>  # print(l)<br>  for i in l:<br>    <br>    if len(i)&gt;=3:<br>      b=(i[1:]+i[0])<br>      x=str(res)+b+str(res1)<br>      s+=x+&#39; &#39;<br>    else:<br>      x1=(i[::-1])<br>      s+=x1+&#39; &#39;<br>  print(&#39;Your Encoded Message Is: &#39;,s)<br><br>def Decode(n):<br>  s=&#39;&#39;<br>  l=n.split(&#39; &#39;)<br>  c=0<br>  b=0<br>  for i in l:<br>    <br>    if len(i)&gt;=3:<br>      b=(i[-4]+i[3:-4])<br>      s+=b+&#39; &#39;<br>    else:<br>      b1=(i[::-1])<br>      s+=b1+&#39; &#39;<br>  print(&#39;Your Decoded message is: &#39;,s)<br><br><br><br><br>if a==1:<br>  <br>  print(&#39;Enter your message below to code it :&#39;)<br>  n=input()<br>  Encode(n)<br><br>if a==2:<br>  print(&#39;Enter your encoded message below to decode it :&#39;)<br>  n=input()<br>  Decode(n)<br><br>if a==3:<br>  print(&#39;Quitting.....&#39;)<br>  run=False<br><br>if a!=1 and a!=2 and a!=3:<br>  print(&#39;Invalid Input!!!&#39;)
import random<br>import string<br><br># User Input <br>str=input(&quot;Enter the word to encode/decode: &quot;)<br>code=int(input(&quot;Enter 1 to encode or enter 0 to decode: &quot;))<br><br># Function to Encode<br>def encode(str):<br>  if (len(str)&lt;3):<br>    return str[1::]+str[0]<br>    <br>  else:<br>     <br>    encoded= (&#39;&#39;.join(random.choices(string.ascii_lowercase, k=3)))+str[1::]+str[0]+(&#39;&#39;.join(random.choices(string.ascii_lowercase, k=3)))<br>    return encoded<br>     <br># Function to Decode<br>def decode(str):<br>  if (len(str)&lt;3):<br>    return str[1::]+str[0]<br><br>  else:<br>    l_rand=str[3:]<br>    r_rand=l_rand[:-3]<br>    decoded=r_rand[-1]+(r_rand.strip(r_rand[-1]))<br>    return decoded<br><br># Calling Methods<br><br>if(code==1):<br>  print(f&quot;The encodded word is: {encode(str)}&quot;)<br>if(code==0):<br>  print(f&quot;The decodded word is: {decode(str)}&quot;)
