# Name : Heet Mehta<br>import os<br>def clear(folder, extension):<br>    os.chdir(folder)<br>    i = 1<br>    for f in os.listdir(folder):<br>        if f.endswith(extension):<br>            os.rename(f, f&#39;{i}{extension}&#39;)<br>            i += 1<br><br>if <i>_name_</i> == &#39;__main__&#39;:<br>    clear(r&#39;C:\Users\admin\Desktop\testfol&#39;,&#39;.mp3&#39;)
