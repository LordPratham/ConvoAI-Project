import os<br><br><br>def clear_clutter(file_extension, file_path):<br>    files = os.listdir(file_path)<br>    user_extension_files = [file for file in files if file.endswith(file_extension)]<br>    for i, file in enumerate(user_extension_files):<br>        os.rename(f&#39;{file_path}/{file}&#39;, f&#39;{file_path}/{i}{file_extension}&#39;)<br><br><br>f_extension = input(&quot;Enter your file extension:   &quot;)<br>clear_clutter(f_extension, &#39;photos&#39;)<br>print(&quot;All files are rename Successfully &quot;)
