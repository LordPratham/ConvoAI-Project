import os<br><br><br>files = os.listdir(&quot;New folder&quot;)<br>i=1<br>                            <br>for file in files:<br>    if file.endswith(&quot;jpg&quot;):<br>        os.rename(fr&quot;C:\Users\lenovo\Desktop\python\New folder\{file}&quot;, fr&quot;C:\Users\lenovo\Desktop\python\New folder\{i}.jpg&quot;)<br>        i=i+1<br>    elif file.endswith(&quot;png&quot;):<br>        os.rename(fr&quot;C:\Users\lenovo\Desktop\python\New folder\{file}&quot;, fr&quot;C:\Users\lenovo\Desktop\python\New folder\{i}.png&quot;)<br>        i=i+1<br>    elif file.endswith(&quot;txt&quot;):<br>        os.rename(fr&quot;C:\Users\lenovo\Desktop\python\New folder\{file}&quot;, fr&quot;C:\Users\lenovo\Desktop\python\New folder\{i}.txt&quot;)<br>        i=i+1<br>    elif file.endswith(&quot;pdf&quot;):<br>        os.rename(fr&quot;C:\Users\lenovo\Desktop\python\New folder\{file}&quot;, fr&quot;C:\Users\lenovo\Desktop\python\New folder\{i}.pdf&quot;)<br>        i=i+1<br><br>print(files)
