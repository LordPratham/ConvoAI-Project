def fibonnaci(n):<br>    a = 0<br>    b = 1<br>    sequence = []<br><br>    while a &lt; n:<br>        sequence.append(a)<br>        c = a + b<br>        a = b<br>        b = c<br><br>    return sequence<br><br><br><br>print(fibonnaci(100))
