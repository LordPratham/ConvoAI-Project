import numpy as np<br><br>m1 = np.matrix(np.arange(1,11).reshape((2,5)))<br><br>m2 = np.matrix(np.arange(1,11).reshape((5,2)))<br><br>m3 = np.matrix(np.zeros(4).reshape((2,2)), int)<br><br>t1 = m1.shape<br>t2 = m2.shape<br><br><a href=""http://www.youtube.com/results?search_query=%23if"">#if</a> shape mismatch display error message<br>if( t1[0] !=  t2[1] and t1[1] != t2[0]):<br>    print(&quot;multiplication cant be done&quot;)<br>else:<br>    for i in range(t1[0]):<br>        for j in range(t2[1]):<br>            for k in range(t1[1]):<br>                m3[i,j] += m1[i, k] * m2[k, j]<br><br>print(m3)<br><br>print(m1 * m2)<br>op:<br>[[ 95 110]<br> [220 260]]<br>[[ 95 110]<br> [220 260]]
