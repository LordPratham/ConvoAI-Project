import random<br>name = str(input(&quot;Enter the Name of player &quot;))<br>print(f&quot;Welcome {name}&quot;)<br>won = 0<br>lose = 0<br>Draw = 0<br>while True:<br>    print(f&#39;&#39;&#39;Press 1 For Stone <br>    Press 2 For Scisssor <br>    Press 3 for paper<br>    Press 0 For Exit<br>    WIN = {won}    DRAW = {Draw}     LOSE = {lose}                                                                           Player Name --&gt; {name}<br>          &#39;&#39;&#39;)<br>    a=int(input())<br>    if a == 1:<br>        print(&quot;You-- Stone&quot;)<br>    elif a == 2:<br>        print(&quot;You-- Scisssor&quot;)<br>    elif a == 3:<br>        print(&quot;You-- Paper&quot;)<br>    if a == 0:<br>        if won &gt;0 or Draw &gt;0 or lose &gt;0:<br>            print(&quot;Thanks For Playing &quot;)<br>            exit()<br>        else:<br>            x=str(input(&quot;Are you Sure You want to Exit ? Y/N &quot;))<br>            if x == &#39;N&#39; or x == &#39;n&#39;:<br>                continue<br>            else:<br>                exit()<br><br>    if a == 1 or a==2 or a==3:<br>        computer = random.randint(1,3)<br>        if computer == 1:<br>            print(&quot;Computer-- Stone&quot;)<br>        elif computer == 2:<br>            print(&quot;Computer-- Scisssor&quot;)<br>        elif computer == 3:<br>            print(&quot;Computer-- Paper&quot;)<br><br>        if a == computer:<br>            print(&quot; Match Draw !! \n&quot;)<br>            print(&quot;------------------------------------------------------------------------------------------------------&quot;)<br>            Draw +=1<br>        elif a == 1 and computer == 2 or a==2 and computer ==3 or a==3 and computer == 1:<br>            print(&quot;Wow , Congratulations You Won The Game !! \n&quot;)<br>            print(&quot;------------------------------------------------------------------------------------------------------&quot;)<br>            won +=1<br>        elif a ==1 and computer == 3 or a == 2 and computer == 1 or a == 3 or computer ==2:<br>            print(&quot;Oops !! Better Luck Next Time \n&quot;)<br>            print(&quot;------------------------------------------------------------------------------------------------------&quot;)<br>            lose +=1<br><br>    else:<br>        print(&quot;Enter the Correct Input !! \n &quot;)<br>        print(&quot;------------------------------------------------------------------------------------------------------&quot;)<br>        continue<br><br><br>        <br>@Codewithharry <a href=""http://www.youtube.com/results?search_query=%23100daysofcode"">#100Daysofcode</a> <a href=""http://www.youtube.com/results?search_query=%23stonepaperscissor"">#Stonepaperscissor</a> <a href=""http://www.youtube.com/results?search_query=%23stone"">#stone</a> <a href=""http://www.youtube.com/results?search_query=%23paper"">#paper</a> <a href=""http://www.youtube.com/results?search_query=%23scissor"">#Scissor</a>
print(&quot;--------Welcome To Snake,Water and Gun Game----------&quot;)<br>score=0<br>for i in range(3):<br> import random<br> obj=[&quot;Snake&quot;,&quot;Water&quot;,&quot;Gun&quot;]<br> <br> random=random.choice(obj).lower()<br> <br> userInput=input(&quot;Enter Your Choice please:&quot;).lower()<br> if(userInput==&quot;snake&quot; and random==&quot;water&quot;):<br>     print(f&quot;The Computer Output is {random}&quot;)<br>     print(&quot;You Wins!&quot;)<br>     score+=1<br> elif(userInput==&quot;water&quot; and random==&quot;gun&quot;):<br>     print(f&quot;The Computer Output is {random}&quot;)<br>     print(&quot;You Wins!&quot;)<br>     score+=1<br> elif(userInput==&quot;gun&quot; and random==&quot;snake&quot;):<br>     print(f&quot;The Computer Output is {random}&quot;)<br>     print(&quot;You Wins!&quot;)<br>     score+=1<br> elif(userInput==random):<br>     print(&quot;||Draw||&quot;)<br> else:<br>     print(&quot;You Loss!!&quot;)<br>     print(f&quot;The Computer Output is {random}&quot;)<br>     score-=1<br>print(f&quot;Your Total Score is {score} out of 3 Note:One Marks deducted If Computer Wins&quot;)
import random<br><br><br>def get_player_choice():<br>    choice = input(&quot;Enter &#39;s&#39; for snake, &#39;w&#39; for water, &#39;g&#39; for gun: &quot;)<br>    if (choice==&#39;s&#39; or choice==&#39;S&#39;) or (choice==&#39;g&#39;or choice==&#39;G&#39;) or (choice==&#39;w&#39; or choice==&#39;W&#39;):<br>        return choice<br><br><br>def get_computer_choice():<br>    choices = [&#39;s&#39;, &#39;w&#39;, &#39;g&#39;]<br>    choice = random.choice(choices)<br>    return choice<br><br>def play_game(player_choice, computer_choice):<br>    if player_choice == computer_choice:<br>        print(&quot;Tie!&quot;)<br>    elif (player_choice == &#39;s&#39; and computer_choice == &#39;w&#39;) or (player_choice == &#39;w&#39; and computer_choice == &#39;g&#39;) or (player_choice == &#39;g&#39; and computer_choice == &#39;s&#39;):<br>        print(&quot;Congratulations! You win.&quot;)<br><br>    else:<br>        print(&quot;Sorry, you lose.&quot;)<br><br>def play_again():<br>    play_again = input(&quot;Do you want to play again? (y/n): &quot;)<br>    if play_again == &#39;y&#39; or play_again==&#39;Y&#39;:<br>        return True<br>    else:<br>        return False<br><br># Main game loop<br>while True:<br>    player_choice = get_player_choice()<br>    computer_choice = get_computer_choice()<br>    print(f&quot;You chose {player_choice}. The computer chose {computer_choice}.&quot;)<br>    play_game(player_choice, computer_choice)<br>    if not play_again():<br>        break<br><br><br>print(&quot;Thanks for playing!&quot;)
import random<br><br>options =[&#39;S&#39;, &#39;W&#39;, &#39;G&#39;]<br><br>print(&#39;please select what you want to choose from Snake(0) Water(1) gun(2) : &#39;  )<br><br>player = str(input(&#39;&#39;).upper())<br><br>computer = random.choices([&#39;S&#39;, &#39;W&#39;, &#39;G&#39;])<br><br>if(player == &#39;S&#39;  and computer == [&#39;S&#39;]):<br>    print(f&#39;draw the computer chose {computer} and you chosef {player}&#39;)<br><br>elif(player == &#39;S&#39; and computer == [&#39;W&#39;]):<br>    print(f&#39;win the computer chose {computer} and you chosef {player}&#39;)<br><br>elif(player == &#39;S&#39; and computer == [&#39;G&#39;]):<br>    print(f&#39;lose the computer chose {computer} and you chosef {player}&#39;)<br><br>elif(player == &#39;W&#39; and computer == [&#39;G&#39;]):<br>    print(f&#39;win the computer chose {computer} and you chosef {player}&#39;)<br><br>elif(player == &#39;W&#39; and computer == [&#39;S&#39;]):<br>    print(f&#39;lose the computer chose {computer} and you chosef {player}&#39;)<br><br>elif(player == &#39;W&#39; and computer == [&#39;W&#39;]):<br>    print(f&#39;draw the computer chose {computer} and you chosef {player}&#39;)<br><br>elif(player == &#39;G&#39; and computer == [&#39;G&#39;]):<br>    print(f&#39;draw the computer chose {computer} and you chosef {player}&#39;)<br><br>elif(player == &#39;G&#39; and computer == [&#39;W&#39;]):<br>    print(f&#39;lose the computer chose {computer} and you chosef {player}&#39;)<br><br>elif(player == &#39;G&#39; and computer == [&#39;S&#39;]):<br>    print(f&#39;winthe computer chose {computer} and you chosef {player}&#39;)<br><br>else:<br>    print(&quot;please choose correct value&quot;)
