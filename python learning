# Made a Simple Program Showing Operations<br><br>a = 12<br>b = 23<br>c = 34<br>d = 45<br><br>print(&quot;If we add 12 into 23 then we get&quot;, a+b)<br>print(&quot;If we subtract 23 from 12 then we get&quot;, b-a)<br>print(&quot;If we multiply 34 into 45 then we get&quot;, c*d)<br>print(&quot;If we divide 45 into 34 then we get&quot;, d/c)
solution
first_number = input(&quot;enter first number : &quot;)<br>operation = input(&quot;enter operation (+, -, /, x, r, sq) : &quot;)<br>second_number = input(&quot;enter second number : &quot;)<br>first_number = int(first_number)<br>second_number = int(second_number)<br>if operation == (&quot;+&quot;) :<br>    print(first_number + second_number)<br>elif operation == (&quot;-&quot;) :<br>    print(first_number - second_number)<br>elif operation == (&quot;/&quot;) :<br>    print(first_number / second_number)<br>elif operation == (&quot;x&quot;) :<br>    print(first_number * second_number)<br>elif operation == (&quot;%&quot;) :<br>    print(first_number % second_number)<br>elif operation == (&quot;sq&quot;) :<br>    print(first_number ** second_number)<br>else :<br>    print(&quot;invalid operation&quot;)<br>feedback = input(&quot;how it work? (good, bad, average) &quot;)<br>if feedback == (&quot;good&quot;) :<br>    print(&quot;Yeah Haider always develop good &quot;)<br>elif feedback == (&quot;bad&quot;) :<br>    print(&quot;sorry to hear that, you don&#39;t like it &quot;)<br>elif feedback == (&quot;average&quot;) :<br>    print(&quot;we will try to improve&quot;)<br><br>print(&quot;thanks for using &quot;)
