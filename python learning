a = input(&quot;Enter the first number : &quot;)<br>Operator = input(&quot;Enter the Operator ( + , - , * , / , % ): &quot;)<br>b = input(&quot;Enter the secondd number  : &quot;)<br>a = int(a)<br>b= int(b)<br>if Operator == &quot;+&quot;:<br>   print (a + b )<br>elif Operator == &quot;-&quot;:                                                   <br>       print (a - b )<br>elif Operator == &quot;/&quot;:                                                   <br>       print (a / b )<br>elif Operator == &quot;*&quot;:                                                   <br>       print (a * b )<br>elif Operator == &quot;%&quot;:                                                   <br>       print (a % b )<br>else  :<br>      print (&quot;Invalid operator&quot;)
