class User:<br>    def __init__(self,id,rating):<br>        self.id=id<br>        self.rating=rating<br>        <br>    def show_user(self):<br>        print(f&quot;User ID is {self.id} and Leet Code Rating is {self.rating}&quot;)    <br><br>class preference(User):    <br>      def show_prefer(self):<br>          print(f&quot;Job:{self.job}\nSelection:{self.selection}&quot;)  <br>          <br>class service(preference):<br>    def __init__(self,id,rating,job,selection,remote,hired):<br>        self.remote=remote<br>        self.hired=hired<br>        self.job=job<br>        self.selection=selection<br>        super().__init__(id,rating)<br>    <br>    def show_service(self):<br>        print(f&quot;Remote:{self.remote}\nHired:{self.hired}&quot;)<br>              <br>              <br>o1=service(&quot;407&quot;,&quot;2000&quot;,&quot;As Developer&quot;,&quot;In Google&quot;,&quot;Offline&quot;,&quot;Yes&quot;)         <br>o1.show_user()<br>o1.show_prefer()<br>o1.show_service()
