import random<br>choices=[-1,0,1]<br>ref=[0,&#39;Rock&#39;,&#39;Paper&#39;,&#39;Scissors&#39;]<br>com_ch=int(random.choice(choices))<br>user_ch=int(input(&#39;Enter -1 for Rock, 0 for Paper &amp; 1 for Scissors &#39;))<br><br>print(&#39;Computer chose &#39;, ref[com_ch+2])<br>print(&#39;You Chose &#39;, ref[user_ch+2])<br>if com_ch == user_ch:<br>    print(&quot;Draw&quot;)<br>elif com_ch==0 and user_ch==1:<br>    print(&#39;You Win&#39;)<br>elif com_ch==1 and user_ch==0:<br>    print(&#39;You Loose&#39;)<br>elif com_ch==1 and user_ch==-1:<br>    print(&#39;You Win&#39;)<br>elif com_ch==-1 and user_ch==1:<br>    print(&#39;You Loose&#39;)<br>elif com_ch==-1 and user_ch==0:<br>    print(&#39;You Win&#39;)<br>elif com_ch==0 and user_ch==-1:<br>    print(&#39;You Loose&#39;)<br>else:<br>    print(&#39;Invalid Input&#39;)
