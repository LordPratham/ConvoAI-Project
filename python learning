class library:<br>    def __init__(self,no_of_books,books):<br>        self.books=books<br>        self.no_of_books=no_of_books<br><br>    def add(self,n):<br>        self.books.append(n)<br><br>    def pri(self):<br>        for i in self.books:<br>          print(i)<br>    def length(self):<br>        print(len(self.books))<br><br>a=library(12,[&quot;how&quot;,&quot;when&quot;])<br>a.add(&quot;hello&quot;)<br>a.pri()<br>a.length()
