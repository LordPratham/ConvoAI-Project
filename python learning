 print(&quot;Correct  Answer!!&quot;)<br>        print(&quot;You won 100 Points&quot;)<br>        points=points+100<br>        print(&quot;Total Points= &quot;,points)<br>        continue<br>   <br>        <br>    elif(i==1 and ch==&quot;a&quot;):<br>        print(&quot;Correct  Answer!!&quot;)<br>        print(&quot;You won 100 Points&quot;)<br>        points=points+300<br>        print(&quot;Total Points= &quot;,points)<br>        continue<br>    <br>        <br>        <br>    elif(i==2 and ch==&quot;a&quot;):<br>        print(&quot;Correct  Answer!!&quot;)<br>        print(&quot;You won 100 Points&quot;)<br>        points=points+100<br>        print(&quot;Total Points= &quot;,points)<br>        continue<br>    <br>        <br>    elif(i==3 and ch==&quot;c&quot;):<br>        print(&quot;Correct  Answer!!&quot;)<br>        print(&quot;You won 100 Points&quot;)<br>        points=points+100<br>        print(&quot;Total Points= &quot;,points)<br>        continue<br>   <br>        <br>    elif(i==4 and ch==&quot;a&quot;):<br>        print(&quot;CONGRATULATIONS!!&quot;)<br>        print(&quot;You won 10000 Points&quot;)<br>        points=points+10000<br>        print(&quot;Total Points= &quot;,points)<br>        continue<br>    <br>    else:<br>        print(&quot;Wrong Answer&quot;)<br>        print(&quot;YOU LOSE!! Try Again&quot;)<br>        print(&quot;Obtained points &quot;,points)<br>        break
def display_question(question, options):<br>    print(question)<br>    for i, option in enumerate(options):<br>        print(f&quot;{chr(65 + i)}. {option}&quot;)<br>        <br><br><br># Example usage:<br>question = &quot;1- What is the capital of France?&quot;<br>options = [&quot;London&quot;, &quot;Berlin&quot;, &quot;Paris&quot;, &quot;Madrid&quot;]<br>correct_answers = [&quot;C&quot;]<br><br>display_question(question, options)<br><br>user_answer = input(&quot;Enter your answer (A/B/C/D): &quot;).strip().upper()<br><br>if user_answer == &quot;C&quot;:<br>    print(&quot;Correct!\n&quot;)<br>    print(&quot;YOU WON 1K&quot;)  # Award points for a correct answer<br>else:<br>    print(&quot;Wrong!&quot;)<br>    <br><a href=""http://www.youtube.com/results?search_query=%23_"">#_</a><i>____________________________________________________</i>    <br><br><br>question = &quot;2- Who wrote the play Romeo &amp; Juliet?&quot;<br>options = [&quot;Charles Dickens&quot;, &quot;William Shakespeare&quot;, &quot;Jane Austen&quot;, &quot;Leo Tolstoy&quot;]<br>correct_answers = [&quot;B&quot;]<br><br>display_question(question, options)<br><br>user_answer = input(&quot;Enter your answer (A/B/C/D): &quot;).strip().upper()<br><br>if user_answer == &quot;B&quot;:<br>    print(&quot;Correct!\n&quot;)<br>    print(&quot;YOU WON 10K&quot;)  # Award points for a correct answer<br>else:<br>    print(&quot;Wrong!&quot;)<br>  #  <i>__________________________________________________</i><br><br><br>    <br>question = &quot;3- Who is known as the father of modern physics?&quot;<br>options = [&quot;Isaac Newton&quot;, &quot;Albert Einstein&quot;, &quot;Galileo Galilei&quot;, &quot;Nikola Tesla&quot;]<br>correct_answers = [&quot;B&quot;]<br><br>display_question(question, options)<br><br>user_answer = input(&quot;Enter your answer (A/B/C/D): &quot;).strip().upper()<br><br>if user_answer == &quot;B&quot;:<br>    print(&quot;Correct!\n&quot;)<br>  