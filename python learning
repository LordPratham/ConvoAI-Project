def selectionsorted(list):<br>    removable_list = list.copy()<br>    new_list = []<br>    a = 0<br><br>    while a &lt; len(removable_list):<br>        holder = removable_list[0]<br><br>        for i in range(len(removable_list)):<br>            if removable_list[i] &lt; holder:<br>                holder = removable_list[i]<br><br>        new_list.append(holder)<br>        removable_list.remove(holder)<br><br>    return new_list
def sort(self):<br>    for i in range(a):<br>        minpos=i<br>        for j in range(i+1):<br>            if list[j]&lt;list[minpos]:<br>                minpos=j<br><br><br>                temp=list[i]<br>                list[i]=list[minpos]<br>                list[minpos]=temp<br>        <br>a=int(input(&quot;Enter the no. of elements you want to enter in the list &quot;))<br>list=[]<br>for i in range(a):<br>    li=int(input())<br>    list.append(li)<br>print(&quot;Unsorted list is: &quot;,list)<br><br><br>sort(list)<br><br><br>print (&quot;Sorted list is: &quot;,list)
selection sort using maximum element-<br>def ssortmax(l):<br>    for i in range(len(l)):<br>        maxpos=len(l)-1-i<br>        for j in range(len(l)-i-1):<br>            if l[j]&gt;l[maxpos]:<br>                maxpos=j<br>        l[maxpos],l[len(l)-i-1]=l[len(l)-i-1],l[maxpos]
