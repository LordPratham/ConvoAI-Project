def fib(num):<br>    if num&lt;0 :<br>        print(&quot;Invalid number&quot;)<br>    else:<br>         a=0<br>         b=1<br>    if num==1:<br>       print(a)<br>    else:<br>        a=0<br>        b=1<br>        print(a)<br>        print(b)<br><br>        for i in range(2,num):<br>            c=a+b<br>            a=b<br>            b=c<br>            print(c)<br>num=int(input(&quot;Enter the number till you want to see in fibonacci series : &quot;))<br>fib(num)
