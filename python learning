import time<br>time = time.strftime(&#39;%H:%M&#39;)<br>if time == &quot;<a href=""https://www.youtube.com/watch?v=IZBKXWrbqBM&amp;t=00m00s"">00:00</a>&quot;:<br>    print(&quot;Have A Great Day!&quot;)<br>elif time &lt;= &quot;<a href=""https://www.youtube.com/watch?v=IZBKXWrbqBM&amp;t=12m00s"">12:00</a>&quot;:<br>    print(&quot;Good Morning&quot;)<br>elif time &gt; &quot;<a href=""https://www.youtube.com/watch?v=IZBKXWrbqBM&amp;t=12m00s"">12:00</a>&quot; and time &lt; &quot;<a href=""https://www.youtube.com/watch?v=IZBKXWrbqBM&amp;t=18m00s"">18:00</a>&quot;:<br>    print(&quot;Good Afternoon&quot;)<br>elif time &gt;= &quot;<a href=""https://www.youtube.com/watch?v=IZBKXWrbqBM&amp;t=18m00s"">18:00</a>&quot;:<br>    print(&quot;Good Evening&quot;)
