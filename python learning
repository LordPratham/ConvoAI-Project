class Library:<br>    error = &quot;Something went wrong in your library management&quot;<br><br>    def __init__(self):<br>        self.books = []<br>        self.no_of_books = 0<br><br>    def add(self, *book):<br>        self.books.append(book)<br>        self.no_of_books = len(self.books)<br>        print(&quot;Book added&quot;)<br>    def show_books(self):<br>        return f&quot;The books are {self.books} and No. of books are {self.no_of_books}&quot;<br>    def check(self):<br>        if len(self.books) == self.no_of_books and self.no_of_books != 1 :<br>            print(f&quot;your {self.no_of_books} books are {self.books} &quot;)<br>        elif len(self.books) == self.no_of_books and self.no_of_books == 1:<br>            print(f&quot;You have only 1 book&quot;)<br>        elif len(self.books) == self.no_of_books and self.no_of_books == 0:<br>            print(&quot;YOU have no books&quot;)<br>        else:<br>            print(f&quot;{self.error:!^20}&quot;)<br>obj = Library()<br>obj.add(&quot;Land in mils&quot;, &quot;Ramayana&quot; )<br>print(obj.show_books())<br>print(&quot;Sir please take my name Its Ayush Kumar Srivastava)
<a href=""http://www.youtube.com/results?search_query=%23sir"">#Sir</a>  this can create as many  libraries as user want &#39;book&#39; and &#39;no_books&#39; are variables inside functions <br># Instead of making a class for each library this class take a whole library as an object, this increases complexity but is  <br><a href=""http://www.youtube.com/results?search_query=%23worth"">#worth</a> it<br><a href=""http://www.youtube.com/results?search_query=%23by"">#by</a> the way I had made this for your course &#39;python tutorial for absolute beginners&#39; but you have stopped watching the <a href=""http://www.youtube.com/results?search_query=%23comments"">#comments</a> of that 4 + year old course so I am mentioning it here<br>class Library:<br>    user_values = {}<br>    libraries = []<br>    function_called = 0<br>    stay_in_library = True<br><br>    def lend_book(self):<br>        book_present = False<br>        book_name = input(&quot;Enter book name you want to lend/issue.\n&quot;)<br>        new_lender = input(&quot;Enter name to which this book will be issued.\n&quot;)<br>        for item in self.books_name_and_owner:<br>            if item == book_name:<br>                if self.books_name_and_owner[item] is None:<br>                    self.books_name_and_owner[item] = new_lender<br>                    print(f&quot;{book_name} is successfully lend/issued to {new_lender}.&quot;)<br>                    book_present = True<br>                else:<br>                    print(f&#39;book is currently owned by &quot;{self.books_name_and_owner.get(item)}.&quot;&#39;)<br>                    book_present = True<br>        if book_present is False:<br>            print(&quot;Book is not present in this library.&quot;)<br><br>    def add_books(self):<br>        books = (input(&quot;Enter list of books you want to add.\nSeparate name of books with (-).\n&quot;)).split(&quot;-&quot;)<br>        for items in books:<br>            self.books_name_and_owner[items] = None<br>            self.no_of_books += 1            <br>        print(&quot;Books added successfully.&quot;)<br><br>    def display_books(self):<br>        for books in self.books_name_and_owner:<br>            print(f&quot;&quot;&quot;Book: &quot;{books}&quot; currently owned by &quot;{self.books_name_and_owner.get(books)}&quot;. &quot;&quot;&quot;)<br><br>    def return_books(self):<br>        book_present = False<br>        book_name = input(&quot;Enter book name you want to return.\n&quot;)<br>        for items in self.books_name_and_owner:<br>            if items == book_name:<br>                if self.books_name_and_owner.get(items) is not None:<br>                    self.books_name_and_owner[items] = None<br>                    print(f&quot;You have successfully returned {book_name} to library.&quot;)<br>                    book_present = True<br>                else:<br>                    print(&quot;Book is currently not issued/lend to anybody.&quot;)<br>                    book_present = True<br>        if book_present is False:<br>            print(&quot;Book is not present in Library&quot;)<br><br>    def __init__(self, library_name):<br>        <a href=""http://self.name/"">self.name</a> = library_name<br>        self.books_name_and_owner = {}<br>        self.no_of_books = 0<br>        Library.add_books(self)<br>        Library.libraries.append(self.name)<br>        <br>    def no_books(self):<br>        if self.no_of_books &gt; 1:<br>            print(f&quot;There are {self.no_of_books} books in your library&quot;)<br>        elif self.no_of_books == 1:<br>            print(f&quot;There is only {self.no_of_books} book in your library&quot;)      <br><br>    def manager(self):<br>        if Library.function_called == 0:<br>            print(f&quot;Now you are in ({self.name}&#39;s) Library.&quot;)<br>        work = input(&quot;To add books: press (1);\nTo return books: press (2);\nTo lend/issue books: press (3);\n&quot;<br>                     &quot;To display all books: press (4);\nTo get the number of books: press(5);\nTo exit library: press (6).\n&quot;)<br>        if work == &quot;1&quot;:<br>            self.add_books()<br>            Library.function_called += 1<br>        elif work == &quot;2&quot;:<br>            self.return_books()<br>            Library.function_called += 1<br>        elif work == &quot;3&quot;:<br>            self.lend_book()<br>            Library.function_called += 1<br>        elif work == &quot;4&quot;:<br>            self.display_books()<br>            Library.function_called += 1<br>        elif work == &quot;5&quot;:<br>            self.no_books()<br>            Library.function_called += 1            <br>        elif work == &quot;6&quot;:<br>            Library.function_called = 0<br>            Library.stay_in_library = False<br>        else:<br>            print(&quot;Invalid input please try again.&quot;)<br>            Library.function_called += 1<br><br>    @staticmethod<br>    def constructor():<br>        while True:<br>            Library.stay_in_library = True<br>            work_done = input(&quot;To create Library: press (1);\nTo access Library: press (2).\n&quot;)<br>            if work_done == &quot;1&quot;:<br>                name = input(&quot;Please enter a name for your library.\n&quot;)<br>                Library.user_values[name] = Library(name)<br>                print(&quot;Library Created successfully.&quot;)<br>            elif work_done == &quot;2&quot;:<br>                if len(Library.libraries) != 0:<br>                    lib_name = input(&quot;Please enter your library name.\n&quot;)<br>                    while Library.stay_in_library is True:<br>                        try:<br>                            lib_object = Library.user_values.get(lib_name)<br>                            lib_object.manager()<br>                        except:<br>                            print(&quot;Library is not present;&quot;)<br>                            print(f&quot;All present Libraries are listed as follows:-\n{Library.libraries}\nChoose &quot;<br>                                  f&quot;One of them.&quot;)<br>                            break<br>                else:<br>                    print(&quot;There are no libraries to work on.\nPlease create a library first.\n&quot;)<br><br>            elif work_done == &quot;close&quot;:<br>                break<br>            else:<br>                print(&quot;Invalid input please try again.&quot;)<br><br><br>Library.constructor()
