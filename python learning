def selecttionsort(list):<br>    for i in range(len(list) - 1):<br>        minpos = i<br>        for j in range(i, len(list)):<br>            if list[j] &lt; list[minpos]:<br>                minpos = j<br>        list[i], list[minpos] = list[minpos],list[i]<br>    return list<br>def bubblesort(list):<br>    for i in range(len(list) - 1, 0, -1):<br>        for j in range(i):<br>            if list[i] &lt; list[j]:<br>                list[i], list[j] = list[j], list[i]<br>    return list<br>list1 = [12,8,6,45,99,40,9,46]<br>print(bubblesort(list1))<br>list2 = [12,8,6,45,99,40,9,46]<br>print(selecttionsort(list2))
