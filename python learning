def coding():<br>    code = input (&quot;Enter your name :&quot;)<br>    if (len(code) &lt;= 2):<br>        rev = code[::-1]<br>        print (rev)<br>    else:<br>        start = &quot;che&quot;<br>        end = &quot;thu&quot;<br>        first = code[0:1]<br>        remain = code[1:]<br>        secret = start + remain + first + end<br>        print (&quot;Your secred code is &quot; ,secret , &quot; Do not share this code to anyone&quot;)<br><br>def decoding():<br>    decode = input (&quot;Enter your Secret code :&quot;)<br>    if (len(decode) &lt;= 2):<br>        rev = decode[::-1]<br>        print (rev)<br>    else:<br>        remain = decode[3:]<br>        e_rem = remain[:-3]<br>        name = e_rem[-1] + e_rem[:-1]<br>        print (&quot;Your Name is : &quot;, name)<br>                      <br>        <br>coding()<br>decoding()
