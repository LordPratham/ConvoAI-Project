import os<br>from PyPDF2 import PdfMerger, PdfReader<br><br><br># Reading <br>def extract_info(file):<br>    &#39;This function will read a PDF file&#39;<br>    readerObject = PdfReader(file)<br>    file_len = len(readerObject.pages)<br>    for i in range(file_len):<br>        pages = readerObject.pages[i]<br>        print(pages.extract_text())<br><br># Merging<br>def merge_Pdf(args):<br>    &#39;This function will merge 2 PDF files&#39;<br>    merger = PdfMerger()<br>    <br>    for file in args:<br>        merger.append(PdfReader((file)))<br><br>    merger.write(&#39;merged.pdf&#39;)<br>    merger.close()<br>    <br>    read_file = &#39;C:\Automations\merged.pdf&#39;<br>    extract_info(read_file)<br><br>if <i>_name_</i> == &#39;__main__&#39;:<br>    files = []<br>    <br>    for dirpath,_,filenames in os.walk(&#39;C:\Automations\Clutter&#39;):<br>        for f in filenames:<br>            files.append(os.path.abspath(os.path.join(dirpath, f)))<br>    print(files)<br>    <br>    merge_Pdf(files)
