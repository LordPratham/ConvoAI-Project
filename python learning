quick quiz:<br>class Animal:<br>    def __init__(self,name,color):<br>        self.name=name<br>        self.color=color<br>    def __str__(self):<br>        return f&quot;Name is {self.name} and color is {self.color}.&quot;<br><br>class cat:<br>    def __init__(self,name,age):<br>        self.age=age<br>        Animal.__init__(self, name,color=&quot;Black and white&quot;)<br>    def __str__(self):<br>        return f&quot;Name is {self.name}, color is {self.color}, and age is {self.age}.&quot;<br><br>e1=Animal(&quot;Cow&quot;, &quot;Ghost-white&quot;)<br>print(e1)<br><br>e2=cat(&quot;cat&quot;,9)<br>print(e2)
