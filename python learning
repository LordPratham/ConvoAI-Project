import time<br><br>timestamp = time.strftime(&quot;%H:%M:%S&quot;)<br><br>hr = int(time.strftime(&quot;%H&quot;))<br>mn = int(time.strftime(&quot;%M&quot;))<br>sd = int(time.strftime(&quot;%S&quot;))<br><br>if (0o6 &lt;=hr &lt;12)  &amp;  (mn &lt;= 59)  &amp;  (sd &lt;= 59):<br>    print(&quot;\nGood morning, current time is: &quot;, timestamp)<br>elif (12 &lt;=hr &lt;18)  &amp;  (mn &lt;= 59)  &amp;  (sd &lt;= 59):<br>    print(&quot;\nGood afternoon, current time is: &quot;, timestamp)<br>elif (18 &lt;= hr &lt;24)  &amp;  (mn &lt;= 59)  &amp;  (sd &lt;= 59):<br>    print(&quot;\nGood evening, current time is: &quot;, timestamp)<br>else:<br>    print(&quot;\nGood night current time is: &quot;, timestamp)
