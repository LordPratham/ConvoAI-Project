a=int(input(&quot;first number:&quot;))<br>b=int(input(&quot;second number:&quot;))<br>c=input(&quot;operator:&quot;)<br>match c:<br> case &quot;+&quot;:<br>  print(&quot;a+b:&quot;,a+b)<br> case &quot;-&quot;:<br>   print(&quot;a-b:&quot;,a-b)<br> case &quot;*&quot;:<br>   print(&quot;a*b:&quot;,a*b)<br> case &quot;/&quot;:<br>   print(&quot;a/b:&quot;,a/b)<br> case &quot;%&quot;:<br>   print(&quot;a%b:&quot;,a%b)<br> case _:<br>   print(&quot;Error invalid operator&quot;)
