      print(&quot;Correct!\n&quot;)<br>                        inputCorrectAnswers += 1<br>                        Cash += 1000<br>                    else:<br>                        print(&quot;Incorrect!&quot;)<br>                        inputWrongAnswers -= 1<br>                        chances -= 1<br>                        print(&quot;Chances: &quot;, chances, &quot;\n&quot;)<br><br>                    i += 1<br>                else:<br>                    print(&quot;Invalid Answer\n&quot;)<br>                    break<br>            else:<br>                print(&quot;Invalid!\n&quot;)<br>                break<br>    else:<br>        print(&quot;You donot have enough chances left !\nBetter Luck next time...&quot;)<br><br>else:<br>    print(&quot;Number of questions, answers, and correct answers do not match.\n&quot;)<br><br>print(&quot;You correct Answers were: &quot;, inputCorrectAnswers)<br>print(&quot;You won Rs:&quot;, Cash, &quot;\n&quot;)
