my solution is <br>def player():<br>    import random<br>    n = input(&quot;enter your name &quot;)<br>    print(&quot;hello&quot;, n, &quot;you&#39;ll have 5 chances to beat the computer\nbest of luck&quot;)<br>    chance = 5<br>    player_points = 0<br>    computer_points = 0<br>    while chance &gt; 0:<br>        a = input(&quot;enter the choices as given \nsnake \nwater \ngun \n&quot;).lower()<br>        if a not in [&quot;snake&quot;, &quot;water&quot;, &quot;gun&quot;]:<br>            print(&quot;Invalid character pressed&quot;)<br>        else:<br>            print(&quot;You have chosen &quot;, a)<br>        a1 = random.choice([&quot;snake&quot;, &quot;water&quot;, &quot;gun&quot;])<br>        print(&quot;The computer has chosen &quot;, a1)<br><br>        if (a == &#39;snake&#39; and a1 == &#39;water&#39;) or (a == &#39;water&#39; and a1 == &#39;gun&#39;) or (a == &#39;gun&#39; and a1 == &#39;snake&#39;):<br>            player_points += 1<br>        elif a == a1:<br>            player_points += 1<br>            computer_points += 1<br>        else:<br>            computer_points += 1<br>        chance -= 1<br>        print(&quot;your points :&quot;, player_points)<br>        print(&quot;computer points :&quot;, computer_points)<br>        if player_points &gt; computer_points:<br>            print(&quot;You are winning&quot;)<br>        elif player_points == computer_points:<br>            print(&quot;your point is equal to computer&quot;)<br>        else:<br>            print(&quot;Computer is winning&quot;)<br>        if chance == 0:<br>            if player_points &gt; computer_points:<br>                print(&quot;You won&quot;)<br>            elif player_points == computer_points:<br>                print(&quot;your point is equal to computer&quot;)<br>            else:<br>                print(&quot;Computer won&quot;)<br>            print(&quot;Thank you for playing&quot;)<br>            break<br><br><br>print(&quot;welcome to game of SNAKE WATER &amp; GUN !!! &quot;)<br>while True:<br>    g = int(input(&quot;Do you want to play the game \nPress 1 for game press 0 to exit\n&quot;))<br>    if g == 1:<br>        player()<br>    elif g == 0:<br>        print(&quot;Exiting the game\nThankyou for playing&quot;)<br>        break<br>    else:<br>        print(&quot;invalid key pressed&quot;)
import random<br>name = str(input(&quot;Enter the Name of player &quot;))<br>print(f&quot;Welcome {name}&quot;)<br>won = 0<br>lose = 0<br>Draw = 0<br>while True:<br>    print(f&#39;&#39;&#39;Press 1 For Stone <br>    Press 2 For Scisssor <br>    Press 3 for paper<br>    Press 0 For Exit<br>    WIN = {won}    DRAW = {Draw}     LOSE = {lose}                                                                           Player Name --&gt; {name}<br>          &#39;&#39;&#39;)<br>    a=int(input())<br>    if a == 1:<br>        print(&quot;You-- Stone&quot;)<br>    elif a == 2:<br>        print(&quot;You-- Scisssor&quot;)<br>    elif a == 3:<br>        print(&quot;You-- Paper&quot;)<br>    if a == 0:<br>        if won &gt;0 or Draw &gt;0 or lose &gt;0:<br>            print(&quot;Thanks For Playing &quot;)<br>            exit()<br>        else:<br>            x=str(input(&quot;Are you Sure You want to Exit ? Y/N &quot;))<br>            if x == &#39;N&#39; or x == &#39;n&#39;:<br>                continue<br>            else:<br>                exit()<br><br>    if a == 1 or a==2 or a==3:<br>        computer = random.randint(1,3)<br>        if computer == 1:<br>            print(&quot;Computer-- Stone&quot;)<br>        elif computer == 2:<br>            print(&quot;Computer-- Scisssor&quot;)<br>        elif computer == 3:<br>            print(&quot;Computer-- Paper&quot;)<br><br>        if a == computer:<br>            print(&quot; Match Draw !! \n&quot;)<br>            print(&quot;------------------------------------------------------------------------------------------------------&quot;)<br>            Draw +=1<br>        elif a == 1 and computer == 2 or a==2 and computer ==3 or a==3 and computer == 1:<br>            print(&quot;Wow , Congratulations You Won The Game !! \n&quot;)<br>            print(&quot;------------------------------------------------------------------------------------------------------&quot;)<br>            won +=1<br>        elif a ==1 and computer == 3 or a == 2 and computer == 1 or a == 3 or computer ==2:<br>            print(&quot;Oops !! Better Luck Next Time \n&quot;)<br>            print(&quot;------------------------------------------------------------------------------------------------------&quot;)<br>            lose +=1<br><br>    else:<br>        print(&quot;Enter the Correct Input !! \n &quot;)<br>        print(&quot;------------------------------------------------------------------------------------------------------&quot;)<br>        continue
import  random<br>snake=1<br>water=2<br>gun=3<br>comp=random.randrange(1,3)<br><br>print(&quot;choose snake,water or gun to start the game&quot;)<br>print(&quot;1.snake&quot;)<br>print(&quot;2.water&quot;)<br>print(&quot;3.gun&quot;)<br>while True:<br>  user=int(input(&quot;enter the number according to your option: &quot;))<br>      <br>    <br>    <br>    <br>  if comp==int(gun) and user==2:<br>      print(&quot;you win&quot;)<br>      f=open(&quot;history_and_points&quot;,&quot;a&quot;)<br>      f.write(&quot;\n+10&quot;)<br>      f.close()<br>  elif comp==int(gun) and user==1:<br>      print(&quot;computer win&quot;)<br>      f=open(&quot;history_and_points&quot;,&quot;a&quot;)<br>      f.write(&quot;\n-10&quot;)<br>      f.close()<br>  elif comp==int(water) and user==1:<br>  <br>      print(&quot;you win&quot;)<br>      f=open(&quot;history_and_points&quot;,&quot;a&quot;)<br>      f.write(&quot;\n+10&quot;)<br>      f.close()<br>  elif comp==int(water) and user ==3:<br>       print(&quot;computer win&quot;)<br>       f=open(&quot;history_and_points&quot;,&quot;a&quot;)<br>       f.write(&quot;\n-10&quot;)<br>       f.close()<br>    <br>  elif comp==int(snake) and user==2:<br>       print(&quot;computer win&quot;)<br>       f=open(&quot;history_and_points&quot;,&quot;a&quot;)<br>       f.write(&quot;\n-10&quot;)<br>       f.close()<br>  elif comp==int(snake) and user==2:<br>       print(&#39;computer win&#39;)<br>       f=open(&quot;history_and_points&quot;,&quot;a&quot;)<br>       f.write(&quot;\n-10&quot;)<br>       f.close()<br>  elif comp==int(snake) and user ==3:<br>      print(&quot;you win&quot;)<br>      f=open(&quot;history_and_points&quot;,&quot;a&quot;)<br>      f.write(&quot;\n+10&quot;)<br>      f.close()<br>  else:<br>       print(&quot;draw&quot;)<br>  print(&quot;do you want to play anoter roud then press 1,if no then 0&quot;)<br>  while True:<br>    responce=int(input(&quot;enter 1 or 0: &quot;))<br>    if int(responce)==0:<br>      break<br>    else:<br>      print(&quot;value error&quot;)
ANSWER:<br><br><br><br><br><br><br># Creating Snake, Water and Gun game<br><br>import random<br>import time<br><br>options = [&quot;Snake&quot;, &quot;Water&quot;, &quot;Gun&quot;]<br>chances = [<br>    [[[0, 0], &quot;D&quot;], [[0, 1], &quot;W&quot;], [[0, 2], &quot;L&quot;]],<br>    [[[1, 0], &quot;L&quot;], [[1, 1], &quot;D&quot;], [[1, 2], &quot;W&quot;]],<br>    [[[2, 0], &quot;W&quot;], [[2, 1], &quot;L&quot;], [[2, 2], &quot;D&quot;]],<br>]<br><br>num = 0<br>player = -1<br>computer = -1<br>result = []<br><br><br>def byPlayer():<br>    global num<br>    global player<br>    global result<br>    print(&quot;Chose one:&quot;)<br>    for option in options:<br>        num = num + 1<br>        num = 1 if num &gt; 3 else num<br>        print(str(num) + &quot;)&quot;, option)<br><br>    value = int(input(&quot;Enter anyone option number :&quot;))<br>    if value in (1, 2, 3):<br>        result.append(value - 1)<br>        print(options[value - 1])<br><br><br>def byComputer():<br>    global result<br>    randomNum = random.randint(0, 2)<br>    result.append(randomNum)<br>    print(options[randomNum])<br><br><br>def resultChecker():<br>    print(&quot;Result:&quot;)<br>    for possibilities in chances:<br>        for possibility in possibilities:<br>            if possibility[0] == result:<br>                if possibility[1] == &quot;L&quot;:<br>                    print(&quot;You Got Lose&quot;)<br>                elif possibility[1] == &quot;D&quot;:<br>                    print(&quot;The Match Got Draw&quot;)<br>                else:<br>                    print(&quot;You Got Win&quot;)<br>                result.clear()<br>                break<br><br><br>rounds = int(input(&quot;How Many rounds do you want to play : &quot;))<br>playedRounds = 0<br>while True:<br>    playedRounds = playedRounds + 1<br>    byPlayer()<br>    print(&quot;Computer&#39;s Turn&quot;)<br>    time.sleep(2)<br>    byComputer()<br>    print(&quot;The Result is on a way...&quot;)<br>    time.sleep(3)<br>    resultChecker()<br>    if playedRounds == rounds:<br>        break
