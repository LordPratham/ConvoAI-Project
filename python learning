class Library():<br>    def __init__(self, numBooks, books):<br>        self.no_of_books = numBooks<br>        self.books = books<br>        <br>    def booksName(self):<br>        for index, book in enumerate(self.books, start=1):<br>            print(f&quot;{index}----&gt;{book}&quot;)<br>    <br>    def addBook(self, NewBook):<br>        self.books.append(NewBook)<br>    <br>    def bookCount(self):<br>        count = len(self.books)<br>        print(f&quot;The number of books are: {count}&quot;)<br><br>Lib1 = Library(4, [&#39;Ramayan&#39;, &#39;Mahabharata&#39;, &#39;Godan&#39;, &#39;Kafan&#39;])<br>print(&quot;Existing books in the library:&quot;)<br>Lib1.booksName()<br>Lib1.addBook(&quot;Panchtantra&quot;)<br>print(&quot;\nUpdated books in the library:&quot;)<br>Lib1.booksName()<br>Lib1.bookCount()
