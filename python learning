var1 = int(input(&quot;Entar your first number: &quot;))<br>var2 = int(input(&quot;Entar your second number: &quot;))<br>def add(a, b):<br>    return a + b<br>def sub(a, b):<br>    return a - b<br>def mul(a, b):<br>    return a * b<br>def div(a, b):<br>    return a / b<br>def mod(a, b):<br>    return a // b<br>def rema(a, b):<br>    return a % b<br>def pow(a, b):<br>    a ** b<br><br>op =  int(input(&quot;1) Addition\n2) Subraction\n3) Multiplication\n4) Division\n5) Estimted division\n6) Remainder\n7) Power\n\nEnter the serial number of your opration: &quot;))<br><br>if op == 1:<br>    print(&quot;your answer is &quot;,add(var1, var2))<br>elif op == 2:<br>    print(&quot;your answer is &quot;,sub(var1, var2))<br>elif op == 3:<br>    print(&quot;your answer is &quot;,mul(var1, var2))<br>elif op == 4:<br>    print(&quot;your answer is &quot;,div(var1, var2))<br>elif op == 5:<br>    print(&quot;your answer is &quot;,mod(var1, var2))<br>elif op == 6:<br>    print(&quot;your answer is &quot;,rema(var1, var2))<br>elif op == 7:<br>    print(&quot;your answer is &quot;, pow(var1, var2))<br>else:<br>    print(&quot;Wrong operation&quot;)
