ek jabardast code ise kehte hai:<br><br>import random,shutil,os<br>try:<br>    m=int(input(&#39;enter :\n1 to create a \&#39;cwh\&#39; folder and 10 random files with random name and 1 special file\n2 to organise the existing files\nAny other to exit\n&#39;))<br>except:<br>    exit()<br>match(m):<br>    case 1:<br>        try:<br>            shutil.rmtree(&#39;cwh&#39;)<br>        except:<br>            pass<br>        def rand(x):<br>            a=&#39;&#39;<br>            for i in range(x):<br>               a+=chr(random.randint(97,122))<br>            return a<br>        os.mkdir(&#39;cwh&#39;)<br>        os.chdir(&#39;cwh&#39;)<br>        a=10<br>        for i in range(a):<br>            try:<br>                file=open(f&#39;{rand(random.randint(1,10))}{&quot;.txt&quot; if random.randint(0,1)==1 else &quot;.jpg&quot;}&#39;,&#39;x&#39;)<br>                file.close()<br>            except:<br>                a+=1<br>        file=open(&#39;special.txt&#39;,&#39;x+&#39;)<br>        file.close()<br>    case 2:<br>        try:<br>            os.chdir(&#39;cwh&#39;)<br>            file=open(&#39;special.txt&#39;,&#39;r+&#39;)<br>            file.write(&#39;special &#39;+ input(&#39;Enter content of the special file in a string format&#39;))<br><br>            def ren(y,file):<br>                a=1<br>                l=os.listdir()<br>                for i in l:<br>                    file.seek(0)<br>                    if os.path.isfile(f&#39;{i}&#39;) and i.split(&#39;.&#39;)[0] not in file.read():<br>                        if i.split(&#39;.&#39;)[1]==y:<br>                            os.rename(f&#39;{i}&#39;,f&#39;{a}.{y}&#39;)<br>                            a+=1<br>                        else:<br>                            os.rename(f&#39;{i}&#39;,f&#39;{i[:1].upper()}{i[1:]}&#39;)<br>                            pass<br>            ren(&#39;jpg&#39;,file)<br>        except Exception as e:<br>            print(e,&#39;\nPlease create the files first&#39;)<br>    case _:<br>        exit()
