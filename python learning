print(&quot;Hello, sir&quot;)<br>a = int(input(&quot;Enter the first number: &quot;))<br>b = int(input(&quot;Enter the second number: &quot;))<br>operator = input(&quot;Operator: &quot;)<br><br>if operator == &quot;+&quot;:<br>    print(&quot;The sum is =&quot;, a + b)<br>elif operator == &quot;-&quot;:<br>    print(&quot;The subtraction is =&quot;, a - b)<br>elif operator == &quot;*&quot;:<br>    print(&quot;The multiplication is =&quot;, a * b)<br>elif operator == &quot;/&quot;:<br>    if b != 0:  # Check for division by zero<br>        print(&quot;The division is =&quot;, a / b)<br>    else:<br>        print(&quot;Error: Division by zero is not allowed&quot;)<br>elif operator == &quot;%&quot;:<br>    if b != 0:  # Check for division by zero<br>        print(&quot;The modulo is =&quot;, a % b)<br>    else:<br>        print(&quot;Error: Modulo by zero is not allowed&quot;)<br>else:<br>    print(&quot;Invalid operator&quot;)
