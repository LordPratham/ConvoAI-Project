ques = [&quot;Who is the prime minister of India?&quot;, &quot;What is another name for India?&quot;, &quot;How many states are there in India?&quot;]<br>ans = [&quot;Narendra Modi&quot;, &quot;Bharat&quot;, &quot;29&quot;]<br>sum = 0<br>for i in range(len(ques)):<br>    print(ques[i])<br>    print(&quot;Enter your Answer&quot;)<br>    a = input()<br>    <br>    if a == ans[i]:<br>        print(&quot;Correct Answer&quot;)<br>        sum +=10000<br>    else:<br>        print(&quot;Oopss!! Wrong Answer&quot;)<br>print(&quot;Total Winning Amount&quot;,sum)
