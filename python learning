class Model:<br><br>    # private Variable<br>    __conf = 6<br><br>    # public Variable<br>    name = &quot;Mubeen&quot;<br><br>    def __init__(self):<br>        self.init_var = &quot;Hy&quot;<br><br>    # instance methods<br>    def get_name(self):<br>        return &quot;I am Instance Method&quot;<br><br>    def __getattribute__(self, item):<br>        if item == f&quot;_{__class__.__name__}__conf&quot;:<br>             return &quot;Sorry Can&#39;t Access this private variable&quot;<br><br>        # execute other code<br>        else:<br>            return object.__getattribute__(self,item)<br><br><br># trick to protect the private Variables in python
