def secretcode(message):<br>  b=[]<br>  a=message.split(&#39; &#39;)<br>  string=&#39; &#39;<br>  for elem in a:<br>      if len(elem)&gt;=3:<br>        elems=elem.replace(elem[0],&#39;&#39;)<br>        el=elems+elem[0]<br>        r=&#39;kbc&#39;<br>        code=r+el+r<br>        b.append(code)<br><br>      else:<br>        f=elem[::-1]<br>        b.append(f)<br>  return string.join(b)<br>def decodecode(message):<br>  b=[]<br>  a=message.split(&#39; &#39;)<br>  string=&#39; &#39;<br>  for elem in a:<br>      if len(elem)&gt;=3:<br>        elems=elem[-4]+elem[3:-4]<br>        <br>#         elemss=elems[-4]+elems.replace(elems[-5:1:-1],&#39;&#39;)<br>        b.append(elems)<br><br>      else:<br>        f=elem[::-1]<br>        b.append(f)<br>  <br>  return string.join(b)<br>print(&#39;you want to code or decode?&#39;)<br>answer=input()<br>message=input()<br>if answer==&#39;code&#39;:<br>  print(secretcode(message))<br>else:<br>  print(decodecode(message))
