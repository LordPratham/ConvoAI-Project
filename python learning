import random<br><br>choices = (&#39;snake&#39;, &#39;water&#39;, &#39;gun&#39;)<br><br>def swg():<br>  while True:<br>    computer = random.choice(choices)<br>    print(&#39;Choose Snake, Water, or Gun? &#39;)<br>    user_input = input(&#39;&gt; &#39;).strip()<br>    user_input = user_input.lower()<br>    # Winning conditions<br>    if (user_input == &#39;snake&#39; and computer == &#39;water&#39;) or (user_input == &#39;water&#39; and computer == &#39;gun&#39;) or (user_input == &#39;gun&#39; and computer == &#39;snake&#39;):<br>        print(f&#39;You choose {user_input} and computer choose {computer}, You win..!!&#39;)<br>    # Draw Game<br>    elif user_input == computer:<br>        print(f&#39;You choose {user_input} and computer choose {computer}, Game Tie...!!&#39;)<br>    # Losing conditions<br>    else:<br>        print(f&#39;You choose {user_input} and computer choose {computer}, You lose..!!&#39;)<br>      <br>  <br>swg()
# Snake Water Gun Game in Python - Exercise 5 - Solution | Python Tutorial - Day #63<br><br>import random<br><br>def check(comp, user):<br>    if(comp == user):<br>        return 0<br>    elif(comp == 0 and user == 2):<br>        return -1<br>    elif(comp == 1 and user == 0):<br>        return -1<br>    elif(comp == 2 and user == 1):<br>        return -1<br>    return 1<br><br>choice = &quot;y&quot;<br>while(choice == &quot;y&quot;):<br>    comp = random.randint(0, 2)<br>    user = int(input(&quot;Enter 0 for Snake or 1 for Water or 2 for Gun : &quot;))<br>    complst = [&quot;Snake&quot;, &quot;Water&quot;, &quot;Gun&quot;]<br><br>    score = check(comp, user)<br><br>    print(f&quot;Computer selected : {complst[comp]}&quot;)<br><br>    if (score == 0):<br>        print(&quot;It&#39;s a draw&quot;)<br>    elif (score == -1):<br>        print(&quot;You Won&quot;)<br>    else:<br>        print(&quot;You Lose&quot;)
from random import choice<br>A= [&quot;snake&quot;, &quot;water&quot;, &quot;gun&quot;]<br>class Game():<br>    def __init__(self):<br>        self.user = None<br>        <a href=""http://self.computer/"">self.computer</a> = None<br>        <br>    def user_choice(self):<br>        while True:<br>            user= str(input(&quot;Please choice one of them, snake, water, gun:-  &quot;))<br>            if user in A:<br>                self.user = user<br>                break<br>            else:<br>                print(&quot;invalid instructions&quot;)<br>                <br>    def computer_choice(self):<br>        <a href=""http://self.computer/"">self.computer</a> = str(choice(A))<br>        <br>    def game_rules(self):<br>        if self.user == &quot;snake&quot; and <a href=""http://self.computer/"">self.computer</a> == &quot;water&quot;:<br>            return &quot;User won&quot;<br>        elif self.user == &quot;water&quot; and <a href=""http://self.computer/"">self.computer</a> == &quot;gun&quot;:<br>            return &quot;User won&quot; <br>        elif self.user == &quot;gun&quot; and <a href=""http://self.computer/"">self.computer</a> == &quot;snake&quot;:<br>            return &quot;User won&quot;<br>        elif self.user == <a href=""http://self.computer/"">self.computer:</a><br>            return &quot;its a tie&quot;<br>        else:<br>            return &quot;Computer won&quot;<br>            <br>    def play_game(self):<br>        self.user_choice()<br>        self.computer_choice()<br>        result = self.game_rules()<br><br>        print(f&quot;User chose {self.user} and computer chose {self.computer}&quot;)<br>        print(result)<br><br>if <i>_name_</i> == &quot;__main__&quot;:<br>    game = Game()<br>    game.play_game()
import random<br>def game():<br>    user=int(input(&quot;enter 1 for stone\nenter 2 for scissor\nenter 3 for paper\n-&gt;&quot;))<br>    comp=random.randint(1,3)<br>    print(f&quot;user choice:{user}&quot;)<br>    print(f&quot;computer choice is:{comp}&quot;)<br>    if user==comp:<br>        print(&quot;Draw!&quot;)<br>    elif user==1 and comp==2:<br>        print(&quot;You won!&quot;)<br>    elif user==2 and comp==1:<br>        print(&quot;You won!&quot;)<br>    elif user==3 and comp==1:<br>        print(&quot;You won!&quot;)<br>    else:<br>        print(&quot;Computer won!&quot;)<br>print(&quot;                                 :Welcome to the game:                                 &quot;)<br>game()<br>def exit():<br>    print(&quot;Thank you for playing !&quot;)<br><br>choice=input(&quot;do u want to play again enter(yes/no):&quot;)<br>while True:<br>    if choice==&quot;yes&quot;:<br>        game()<br>    else:<br>        exit()<br>        break<br>    choice = input(&quot;do u want to play again enter(yes/no):&quot;)
