import random<br>import string<br>print(&quot;Welcome to the Secret Language Project\n&quot;)<br><br>print(&quot;Press 1 for I have the message\nPress 2 for I wanna send the message:&quot;)<br>inp = int(input())<br><br>if inp == 1:<br>    encoded_message = input(&quot;Enter the enocoded message you have:&quot;)<br>    decoded_message_list = []<br>    words = encoded_message.split()<br>    for word in words:<br>        if len(word) &lt; 3:<br>            word = word[::-1]<br>            decoded_message_list.append(word)<br>        else:<br>            new_word = word[3:len(word)-3]<br>            last_part = new_word[len(new_word)-3:]<br>            first_part = new_word[:len(new_word)-3]<br>            new = last_part+first_part<br>            # print(<br>            #     f&quot;last_part:{last_part} and first_part:{first_part} and new:{new}&quot;)<br>            decoded_message_list.append(new)<br>    decoded_message = &quot; &quot;.join(decoded_message_list)<br>    print(f&quot;The decode message is :{decoded_message}&quot;)<br><br>else:<br>    decoded_message = input(&quot;Enter the message to be sent: &quot;)<br>    words = decoded_message.split()<br>    encoded_message_list = []<br>    for word in words:<br>        if len(word) &lt; 3:<br>            word = word[::-1]<br>            encoded_message_list.append(word)<br>        else:<br>            first_part = word[:3]<br>            last_part = word[3:]<br>            new_word = last_part+first_part<br>            first_three = &quot;&quot;<br>            for i in range(3):<br>                char = random.choice(string.ascii_letters)<br>                first_three = first_three+char<br>            new_word = first_three+new_word<br>            last_three = &quot;&quot;<br>            for i in range(3):<br>                char = random.choice(string.ascii_letters)<br>                last_three = last_three+char<br>            new_word = new_word+last_three<br>            encoded_message_list.append(new_word)<br>    encoded_message = &quot; &quot;.join(encoded_message_list)<br>    print(f&quot;The encode message is: {encoded_message}&quot;)
import random<br><br>word=input()<br>sp=word.split(&quot; &quot;)<br>coding=False<br>if(coding):<br>  k=[]<br>  for i in sp:<br>    if(len(i)&gt;=3):<br>      k.append(chr(random.randint(97,122))+chr(random.randint(97,122))+chr(random.randint(97,122))+i[1:]+i[0]+chr(random.randint(97,122))+chr(random.randint(97,122))+chr(random.randint(97,122)))<br>    else:<br>      k.append(i[::-1])<br>  print(&quot; &quot;.join(k))<br>else:<br>   k=[]<br>   for i in sp:<br>    if(len(i)&gt;=3):<br>      i=i[3:-3]<br>      i=i[-1]+i[:-1]<br>      k.append(i)<br>    else:<br>      k.append(i[::-1])<br>   print(&quot; &quot;.join(k))
