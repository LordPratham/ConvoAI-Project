import random<br>import string<br><br># Function to generate a random word of a given length<br>def random_word(length):<br>    return &#39;&#39;.join(random.choice(string.ascii_lowercase) for _ in range(length))<br><br>def encode(text):<br>    words = text.split()<br>    encoded_words = []<br><br>    for word in words:<br>        if len(word) &lt;= 3:<br>            # Reverse the word<br>            word = word[::-1]<br>        else:<br>            # Swap the first and last characters and insert random words at the beginning and end<br>            random_start = random_word(3)<br>            random_end = random_word(3)<br>            word = random_start + word[-1] + word[1:-1] + word[0] + random_end<br><br>        encoded_words.append(word)<br><br>    encoded_text = &#39; &#39;.join(encoded_words)<br><br>    return encoded_text<br><br><br><br>def decode(text):<br>  text_words=text.split()<br>  decoded_words=[]<br>  for word in text_words:<br>     if len(word)&lt;=3:<br>      word=word[::-1]<br>     else:<br>      word1 = word[3:-3]<br>      word=word1[-1] + word1[1:-1] + word1[0]<br>     decoded_words.append(word)<br>     print(decoded_words)<br><br>  decoded_text = &#39; &#39;.join(decoded_words)<br>  print(decoded_text)<br><br>  return decoded_text<br><br>encode_or_decode = input(&quot;Do you want to encode or decode? &quot;)<br><br>if encode_or_decode.lower() == &quot;encode&quot;:<br>	text = input(&quot;Enter the text you want to encode: &quot;)<br>	encoded_text = encode(text)<br>	print(&quot;Encoded text:&quot;, encoded_text)<br><br>elif encode_or_decode.lower() == &quot;decode&quot;:<br>	text = input(&quot;Enter the text you want to decode: &quot;)<br>	decoded_text = decode(text)<br>	print(&quot;Decoded text:&quot;, decoded_text)<br><br>else:<br>	print(&quot;Invalid choice. Please enter &#39;encode&#39; or &#39;decode&#39;.&quot;)
