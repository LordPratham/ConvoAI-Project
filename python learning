# take 10 names from user and count, and display the no of user names having length of letter &gt; 5 <br><br>def users(names):<br>    count = 0  # total no of user having &gt; 5 letters <br><br>    for i in range(len(names)):<br>        if len(names[i]) &gt; 5:<br>            count+=1<br>        else:<br>            pass<br>    return count<br><br><br>names = []<br>nos = int(input(&#39;Enter the nos of names u want to check: &#39;))<br><br>for i in range(nos):<br>    names.append(input(&#39;Enter the name: &#39;))<br><br>print(names)<br># len = greater than 5 letters length list of names <br>len = users(names)<br><br>print(&quot;The no of user having letters length &gt; 5 is {} &quot;.format(len))
