import valkmodules as v <a href=""http://www.youtube.com/results?search_query=%23my"">#my</a> custom module<br><br>reference = {<br>    &#39;snake&#39;:0,<br>    &#39;water&#39;:1,<br>    &#39;gun&#39;:2<br>}<br>wC = 0<br>wP = 0<br><br>def ValueToNum(val,j = False):<br>  &#39;&#39;&#39;This function returns the key if value is given and vice versa. returns key if 2nd argument is True&#39;&#39;&#39;<br>  for key,value in reference.items():<br>    if not j:<br>     if key.lower() ==  val.lower():<br>        return value<br>    else:<br>      if value == val:<br>        return key<br><br>def beautifyOutput(string,r = False):<br>    global wP<br>    global wC<br>    print(string.center(20,&#39;-&#39;))<br>    if string == &#39;Computer Wins!&#39;:<br>        wC += 1<br>    elif string == &#39;You Win!&#39;:<br>        wP += 1<br>    if not r:<br>        if wP &gt; wC:<br>            beautifyOutput(&#39;You are in a lead! BOTH WINS : &#39; + str(wP) + &#39; COMP : &#39; + str(wC),True) <a href=""http://www.youtube.com/results?search_query=%23recursion"">#Recursion</a><br>        elif wP == wC:<br>            beautifyOutput(&#39;Both of you have same wins! Both WINS : YOU :&#39; + str(wP) + &#39; COMP : &#39; + str(wC),True)<br>        else:<br>            beautifyOutput(&#39;Computer is on lead! Defeat it! BOTH WINS : &#39; + str(wP) + &#39; COMP : &#39; + str(wC),True)<br>        beautifyOutput(&#39;-&#39;,True)<br><br>while input(&#39;continue playing ? n for exitting. :  &#39;).lower() != &#39;n&#39;:<br>    plr = input(&quot;Enter your choice : &quot;)<br>    if v.is_indexible(reference, plr):<br>        comp = list(reference.values())[v.random_number(len(reference)-1)]<br>        print(&#39;&#39;.center(50,&#39;-&#39;))<br>        print(&quot;Computer chooses : &quot;, ValueToNum(comp,True).capitalize())<br>        <a href=""http://www.youtube.com/results?search_query=%23checking"">#Checking</a> win<br>        if (ValueToNum(plr) + comp) == 0:<br>            beautifyOutput(&quot;It&#39;s a Draw!&quot;)<br>        elif (ValueToNum(plr) + comp) == 1:<br>            if ValueToNum(plr) == 0:<br>                beautifyOutput(&#39;You Win!&#39;)<br>            else:<br>                beautifyOutput(&#39;Computer Wins!&#39;)<br>        elif (ValueToNum(plr) + comp) == 2:<br>            if comp == 0:<br>                beautifyOutput(&#39;You Win!&#39;)<br>            elif (comp == 2):<br>                beautifyOutput(&#39;Computer Wins!&#39;)<br>            elif (comp == ValueToNum(plr)):<br>                beautifyOutput(&quot;It&#39;s a draw!&quot;)<br>        elif (ValueToNum(plr) + comp) == 3:<br>            if comp &lt; ValueToNum(plr) :<br>                beautifyOutput(&#39;Computer Wins!&#39;)<br>            else:<br>                beautifyOutput(&quot;You Win!&quot;)<br>        elif (ValueToNum(plr) + comp) == 4:<br>            beautifyOutput(&quot;It&#39;s a Draw&quot;)<br>    else:<br>        print(&quot;Error : Your choice isn&#39;t a part of the game!&quot;)
# Snake Water Gun<br><br>import random<br><br>user = int(input(&quot;Enter one of the following:\nSnake ğŸ, Water ğŸŒŠ, Gun ğŸ”«\n       0         1       2\n\n\t&gt; &quot;))<br>emojis = [&#39;ğŸ&#39;, &#39;ğŸŒŠ&#39;, &#39;ğŸ”«&#39;]<br>computer = random.choice([0, 1, 2])<br>          #  0  1  2   # computer ğŸ‘ˆ<br>win_seq = [[0, 1, -1], # 0<br>           [-1, 0, 1], # 1<br>           [1, -1, 0]] # 2<br>                       # user ğŸ‘†<br>end_val = win_seq[user][computer]<br>match end_val:<br>    case -1:<br>        print(f&quot;Computer chose {emojis[computer]}. You lost!&quot;)<br>    case 0:<br>        print(f&quot;Computer chose {emojis[computer]}. It&#39;s a draw!&quot;)<br>    case 1: <br>        print(f&quot;Computer chose {emojis[computer]}. You won!&quot;)<br><br><br># Thanks a lot harry bhai!
