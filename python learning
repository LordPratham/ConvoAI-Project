>print(&quot;Great bro&quot;)
question={&quot;whats your name&quot;:&quot;zaeem&quot;,&quot;whats your age&quot;:20,&quot;whats your father name&quot;:&quot;nadeem&quot;}<br>option=(&#39;naeem&#39;,&quot;zaeem&quot;,&quot;shafiq&quot;,12,34,20,&quot;nadeem&quot;,&quot;sabir&quot;,&quot;sohail&quot;)<br>total=0<br>for index,key in enumerate(question.keys()):<br>    if index==0:<br>        print(key)<br>        for i in option[0:3]:<br>            print(i)<br>        user = input(&quot;enter right answer: &quot;)<br>        if user == question.get(key):<br>            print(&quot;right answer&quot;)<br>            total+=1<br>        else:<br>            print(&quot;wrong answer&quot;)<br>    elif index==1:<br>        print(key)<br>        for i in option[3:6]:<br>            print(i)<br>        user = input(&quot;enter right answer: &quot;)<br>        if user == question.get(key):<br>            print(&quot;right answer&quot;)<br>            total += 1<br>        else:<br>            print(&quot;wrong answer&quot;)<br>    elif index==2:<br>        print(key)<br>        for i in option[6:]:<br>            print(i)<br>        user = input(&quot;enter right answer: &quot;)<br>        if user == question.get(key):<br>            print(&quot;right answer&quot;)<br>            total += 1<br>        else:<br>            print(&quot;wrong answer&quot;)<br>print(f&quot;Total number of questions is 3 you right answer is {total}&quot;)
