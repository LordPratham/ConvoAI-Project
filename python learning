import os <br>     <br>os.chdir(&#39;Testing&#39;)<br>cwd = os.getcwd() <br><br>def Rename():<br>    items = os.listdir(cwd)<br>    print(items)<br>    for index, item in enumerate(items):<br>        extension = (item.split(&quot;.&quot;))[1]        <br>        indexed = str(index + 1) + f&quot;.{extension}&quot;<br>        try:<br>            os.rename(item , indexed)        <br>        except:<br>            pass<br>Rename()<br>print(&quot;Current working directory:&quot;, cwd)
