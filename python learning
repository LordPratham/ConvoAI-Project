I have created a school management system. Please leave a review about it.<br>import os<br><br>class School:<br>    school_name = &quot;Army Public School&quot;<br>    school_campus = &quot;City ABC Campus&quot;<br>    def __init__(self,info = &#39;student, 00&#39;):<br>        info_splitted = info.split(&#39;,&#39;)<br>        name = info_splitted[0]<br>        roll_no = info_splitted[1]<br>        self.StudentName = name<br>        self.Roll_Number = roll_no<br>        <br>                <br>    @property<br>    def getset(self):<br>        print(&quot;This is Getter running.&quot;)<br>        print(f&quot;{self.StudentName} is a student of {School.school_name}, {School.school_campus}, He is in {self.school_class}, his roll number is {self.Roll_Number}.&quot;)<br>        <br>    @getset.setter<br>    def getset(self, info):<br>        print(&quot;This is Setter running.&quot;)<br>        name_roll = info.split(&quot;,&quot;)<br>        choice_name = input(f&quot;Do you want to change student&#39;s name from[{self.StudentName}] to [{name_roll[0]}]? Type 1 for yes and 0 to cancel.&quot;)<br>        if choice_name == &quot;1&quot;:<br>            self.StudentName = name_roll[0]<br>        else:<br>            pass<br>        <br>        choice_roll_no = input(f&quot;Do you want to change student&#39;s roll number from[{self.Roll_Number}] to [{name_roll[1]}]? Type 1 for yes and 0 to cancel.&quot;)<br>        if choice_roll_no == &quot;1&quot;:<br>            self.Roll_Number = name_roll[1]<br>            pass<br>    <br>    def __str__(self):<br>        return f&quot;Student: {self.StudentName}, {self.school_class}, Roll No: {self.Roll_Number}&quot;<br>    <br>    def get_student_info_by_roll_no(self, roll_no):<br>        if self.Roll_Number == roll_no:<br>            return self.StudentName, f&quot;is a student of {School.school_name} ({School.school_campus}), He is in {self.school_class} his roll number is {self.Roll_Number}.&quot;<br>        else:<br>            return None, None<br>    <br>    def get_student_info_by_name(self, name):<br>        if self.StudentName == name:<br>            return f&quot;{self.StudentName} is a student of {School.school_name} ({School.school_campus}), He is in {self.school_class} and his roll number is {self.Roll_Number}.&quot;<br>        else:<br>            return None<br>        <br>class ClassX(School):<br>    school_class = &quot;Class X&quot;<br>class ClassIX(School):<br>    school_class = &quot;Class IX&quot;<br>class ClassVIII(School):<br>    school_class = &quot;Class VIII&quot;<br>class ClassVII(School):<br>    school_class = &quot;Class VII&quot;<br>class ClassVI(School):<br>    school_class = &quot;Class VI&quot;<br>class ClassV(School):<br>    school_class = &quot;Class V&quot;<br>    <br>class_mapping = {&quot;ClassV&quot;:ClassV,<br>                 &quot;ClassVI&quot;:ClassVI,<br>                 &quot;ClassVII&quot;:ClassVII,<br>                 &quot;ClassVIII&quot;:ClassVIII,<br>                 &quot;ClassIX&quot;:ClassIX,<br>                 &quot;ClassX&quot;:ClassX}<br>    <br><br>os.chdir(&quot;Testing\Project School&quot;)<br><br>class_mapping = {&quot;ClassV&quot;: ClassV, &quot;ClassVI&quot;: ClassVI, &quot;ClassVII&quot;: ClassVII, &quot;ClassVIII&quot;: ClassVIII, &quot;ClassIX&quot;: ClassIX, &quot;ClassX&quot;: ClassX}<br><br>all_students = []<br><br>for i in os.listdir():<br>    class_name = class_mapping.get(i.removesuffix(&quot;.txt&quot;))  <br>    if class_name:<br>        with open(i, &#39;r&#39;) as file:<br>            file_opened = file.read()<br>            file_opened_without_backslash = file_opened.replace(&quot;\n&quot;, &quot;.&quot;)<br>            file_completed = file_opened_without_backslash.split(&quot;.&quot;)<br>            <br>            for x in file_completed:<br>                name, roll_no = x.split(&quot;,&quot;)<br>                student = class_name(f&quot;{name},{roll_no}&quot;)<br>                all_students.append(student)<br><br>roll_no = &quot;420&quot;<br>for student in all_students:<br>    if student.Roll_Number == roll_no and isinstance(student, ClassVIII):<br>        print(student)<br>        break<br>else:<br>    print(&quot;Student not found.&quot;)<br><br>class_input_choice = &quot;Class&quot; + input(&quot;Enter Class of Student: &quot;)<br>name_or_roll_no_choice = input(&quot;Enter 1 to lookup using Roll No and 2 to lookup using Name: &quot;)<br><br>if class_input_choice in class_mapping:<br>    class_defined = class_mapping[class_input_choice]<br>    if name_or_roll_no_choice == &quot;1&quot;:<br>        roll_no = input(&quot;Enter Roll Number of Student: &quot;)<br>        for student in all_students:<br>            if student.Roll_Number == roll_no and isinstance(student, class_defined):<br>                print(student.get_student_info_by_roll_no(roll_no))<br>                break<br>        else:<br>            print(&quot;Student not found.&quot;)<br>    elif name_or_roll_no_choice == &quot;2&quot;:<br>        name = input(&quot;Enter Name of Student: &quot;)<br>        for student in all_students:<br>            if student.StudentName == name and isinstance(student, class_defined):<br>                print(student.get_student_info_by_name(name))<br>                break<br>        else:<br>            print(&quot;Student not found.&quot;)<br>    else:<br>        print(&quot;Invalid choice for name_or_roll_no_choice.&quot;)<br>else:
