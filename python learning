from array import *<br><br>class New:<br>    def __init__(self):<br>        self.value = array(&#39;i&#39;, [])<br>        <br>    def arr(self, num):<br>        self.value.append(num)<br>        <br>    def resend(self, num):<br>        if num in self.value:<br>            return self.value.index(num)<br><br>test = New()<br>while True:<br>    while True:<br>        try:<br>            n = int(input(&quot;Enter a number: &quot;))<br>            break<br>        except ValueError:<br>            print(&#39;Please enter a number only.&#39;)<br>    test.arr(n)<br>    if n == 0:<br>        user = int(input(&quot;Enter a number to search: &quot;))<br>        values = test.resend(user)<br>        print(f&#39;the index of the number u are searching is located in : {values}&#39;)<br>        break
