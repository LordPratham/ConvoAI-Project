def factorial(n):<br>    for i in range(n,2,-1):<br>        a =i-1<br>        n= n*a<br><br>    return n<br><br>print(factorial(5))
from finding_closet import closet  # just a simple func which can find the closet my subtracting<br><br>class fibonacci_Sequence:<br><br>   def  fibonacci_numbers(self,limit): # basicallay gives a fibonacci_numbers in ur sudden limit<br>            self.limit = limit<br>            if self.limit &lt; 0:<br>               print(&quot;Ahh that shit bruh&quot;)<br><br>            else:<br>               previous = 0<br>                  new = 1<br>                  f_count = 0<br>                  print(&quot;{} -f{}&quot;.format(previous,f_count))<br>                  f_count += 1<br>                  print(&quot;{} -f{}&quot;.format(new,f_count))<br>                  f_count += 1<br><br>                  for i in range(1,limit):<br>                        result = previous + new <br>                        previous = new<br>                        new = result<br>                        print(&quot;{} -f{}&quot;.format(result,f_count))<br>                        f_count += 1<br>      <br>      def find_closet(self,number): # return the closet fibonacci_numbers as from ur input<br>         self.number = number<br>         new = 1<br>         previous = 0<br><br>         while number &gt; new:<br>            result = previous + new<br>            previous = new <br>            new = result <br><br>         x = closet(new,self.number,previous)<br>         return x<br><br>      <br><br><br>   x = fibonacci_Sequence()<br><br>   x.find_closet(5702884)
