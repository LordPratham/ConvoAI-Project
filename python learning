def fundManager(ans, queNo, basePrice=5000):<br>    win = basePrice * (2**queNo)<br>    return win<br><br>def checkAns(QueNo, givenAns):<br>    corrAns = (3, 2, 3, 1, 1, 2, 3, 4, 3, 2, 1, 3, 3, 1, 4)<br>    if int(givenAns) == int(corrAns[QueNo]):<br>        print(&quot;Congratulations, you have chosen the correct answer.&quot;)<br>        currentWin = fundManager(True , QueNo)<br>        print(&quot;Current win :&quot; , currentWin)<br>    else:<br>        print(&quot;Sorry, you have chosen incorrect answer.&quot;)<br>        currentWin = fundManager(False , QueNo)<br>        print(&quot;You&#39;re going home with :&quot;,currentWin , &quot;RS&quot;)<br>        exit()<br><br>print(&quot;\n&quot;)<br><br><br>def showQuestion(que, opt):<br>    for i in range(0, 15, 1):<br>        print(&quot;Question no :&quot;, i + 1)<br>        print(que[i])<br>        currQueOpt = opt[i]<br>        print(&quot;Options :&quot;)<br>        for j in range(0, 4, 1):<br>            print(j + 1, &quot; : &quot;, currQueOpt[j])<br>        givenAns = input(&quot;\nChoose the correct option index : &quot;)<br>        checkAns(i, givenAns)<br>        print(&quot;\n&quot;)<br><br><br>que = [<br>    &quot;Which one of the following is prime number?&quot;,<br>    &quot;How many days are in leap year?&quot;,<br>    &quot;What is the sum of first 10 nutural numbers?&quot;,<br>    &quot;Which animal is known as the &#39;Ship of the Desert&#39;?&quot;,<br>    &quot;How many days are there in a week?&quot;,<br>    &quot;How many hours are there in day?&quot;,<br>    &quot;How many letters are there in the English alphabet?&quot;,<br>    &quot;How many seconds make one hour?&quot;,<br>    &quot;How many Consonants are there in the English alphabet?&quot;,<br>    &quot;How many vowels are there in the English alphabet?&quot;,<br>    &quot;Which animal is known as the king of the jungle?&quot;,<br>    &quot;Name the national bird of India?&quot;,<br>    &quot;Name the national animal of India&quot;,<br>    &quot;If the sum of two angle of triangle is 150 then the thired angle is?&quot;,<br>    &quot;Who is the current PM of india?&quot;,<br>]<br>opt = [<br>    [9, 21, 2, 69],<br>    [365, 366, 367, 368],<br>    [45, 50, 55, 60],<br>    [&quot;Camel&quot;, &quot;Horse&quot;, &quot;Tiger&quot;, &quot;Lion&quot;],<br>    [7, 8, 9, 10],<br>    [23, 24, 25, 26],<br>    [21, 22, 26, 24],<br>    [55, 58, 30, 60],<br>    [20, 22, 21, 26],<br>    [1, 5, 4, 2],<br>    [&quot;Lion&quot;, &quot;Tiger&quot;, &quot;Elegphent&quot;, &quot;Human&quot;],<br>    [&quot;Crow&quot;, &quot;Eagle&quot;, &quot;Peacock&quot;, &quot;Swan&quot;],<br>    [&quot;Lion&quot;, &quot;Elephant&quot;, &quot;Tiger&quot;, &quot;Cow&quot;],<br>    [30, 60, 90, 120],<br>    [&quot;Rahul Gandhi&quot;, &quot;Manmohan Singh&quot;, &quot;Amit Shaha&quot;, &quot;Narendra Modi&quot;],<br>]<br><br>showQuestion(que, opt)
