import random<br>word=input()<br>if(len(word)&gt;=3):<br>  print(chr(random.randint(97,122))+chr(random.randint(97,122))+chr(random.randint(97,122))+word[1:]+word[0]+chr(random.randint(97,122))+chr(random.randint(97,122))+chr(random.randint(97,122)))<br>else:<br>  print(word[::-1])
import random<br>import string<br><br>def random_text_generator(length):<br>    letter = &quot;&quot;<br>    word = &quot;&quot;<br>    for i in range(length):<br>        a = random.randint(1,100)<br>        if a&gt;=1 and a&lt;=13:<br>            letter = &#39;f&#39;<br>        elif a&gt;13 and a&lt;=24:<br>            letter = &#39;q&#39;<br>        elif a&gt;24 and a&lt;=33:<br>            letter = &#39;s&#39;<br>        elif a&gt;33 and a&lt;=42:<br>            letter = &#39;k&#39;<br>        elif a&gt;42 and a&lt;=53:<br>            letter = &#39;a&#39;<br>        elif a&gt;53 and a&lt;=62:<br>            letter = &#39;j&#39;<br>        elif a&gt;62 and a&lt;=73:<br>            letter = &#39;c&#39;<br>        elif a&gt;70 and a&lt;=89:<br>            letter = &#39;v&#39;<br>        else:<br>            letter = &#39;i&#39;<br>        word +=letter<br>    return word<br><br>def reverse_word(x):<br>    word = &quot;&quot;<br>    for i in range(len(x)):<br>        word += x[len(x)-i-1]<br>    return word<br><br>def encoding(str1):<br>    temp = &quot;&quot;<br>    words = &quot;&quot;<br>    words_list = str1.split(&quot; &quot;)<br>    for val in words_list:<br>        if(len(val)&gt;3):<br>            a = random_text_generator(3)<br>            b = random_text_generator(3)<br>            temp = list(val)<br>            x = temp.pop(0)<br>            temp.append(x)<br>            temp = &quot;&quot;.join(temp)<br>            temp = a + temp + b<br>            words += temp + &quot; &quot;<br>        else:<br>            temp = reverse_word(val)<br>            words += temp + &quot; &quot;<br>    return words<br><br>def decoding(str1):<br>    words_list = str1.split(&quot; &quot;)<br>    words = &quot;&quot;<br>    temp = &quot;&quot;<br>    for val in words_list:<br>        if(len(val)&gt;3):<br>            temp = val[3:]<br>            temp = temp[:-3]<br>            temp = list(temp)<br>            a = temp.pop()<br>            temp.insert(0,a)<br>            temp = &quot;&quot;.join(temp)<br>            words += temp + &quot; &quot;<br>        else:<br>            temp = reverse_word(val)<br>            words += temp + &quot; &quot;<br>    return words<br><br>flag = 1<br>a = &quot;&quot;<br>while flag:<br>    user_input = input(&quot;Enter Message To Encode in to Secret Language: &quot;)<br><br>    if len(user_input)&lt;3:<br>      raise ValueError(&quot;The Length of the Message should be atleast 3&quot;)<br>    else:<br>        cipher_text = encoding(user_input)<br>        a = cipher_text<br>        print(&quot;The Message Encoded Sucessfully:&quot;,end=&quot; &quot;)<br>        print(cipher_text)<br>        <br>        check = input(&quot;Do you want to decode the secret message, Enter &#39;y&#39; or &#39;n&#39;: &quot;)<br>        check = check.upper()<br><br>        if check==&#39;N&#39;:<br>            break<br>        elif check==&#39;Y&#39;:<br>            decoded_text = decoding(a)<br>            print(&quot;The Message Decoded Sucessfully:&quot;,end=&quot; &quot;)<br>            print(decoded_text)<br>        else:<br>            raise ValueError(&quot;Invalid Input!!&quot;)<br>        <br>    check = input(&quot;Do you want to use the program again, Enter &#39;c&#39; to continue or &#39;e&#39; to exit: &quot;)<br>    check = check.upper()<br>    if check == &#39;C&#39;:<br>        continue<br>    elif check== &#39;E&#39;:<br>        flag = 0<br>    else:<br>        raise ValueError(&quot;Invalid Input!!&quot;)
print(&quot;&quot;&quot;the code language&quot;&quot;&quot;)<br>a=int(input(&quot;&quot;&quot;you want to decode code or code <br>        choise no.1=code<br>        choise no.2=decode<br>        please tell your choice 1 or 2   &quot;&quot;&quot;))<br>b=input(&quot;enter word   &quot;)<br>c=b[0:3]<br>if a==1:<br>    if len(b)==3:<br>        print(b.__reversed__)<br>    else:<br>        print(b[3:100000000]+c)<br>elif a==1:<br>    if len(b)==3:<br>        print(b.__reversed__)<br>    else:<br>        print(b[3:6]+c)<br>else:<br>    print(&quot;choose either option 1 or 2&quot;)
import random<br><br>typ = input(&quot;You want to Code or Decode: &quot;)<br><br>if typ == &quot;code&quot;:<br>    ch = input(&quot;Enter word: &quot;)<br>    if len(ch) &lt;= 3:<br>        ch1 = ch[0]<br>        ch = ch.replace(ch[0], &quot;&quot;)<br>        ch = ch + ch1<br>        for i in range(1, len(ch) + 1):<br>            ch = chr(random.randint(97, 122)) + ch<br>            ch = ch + chr(random.randint(97, 122))<br>        print(ch)<br>    else:<br>        print(ch[::-1], end=&quot;&quot;)<br><br>elif typ == &quot;decode&quot;:<br>    ch = input(&quot;Enter word: &quot;)<br>    if len(ch) &lt;= 3:<br>        ch1 = ch[len(ch) - 1]<br>        ch = ch[:-1]<br>        ch = ch1 + ch<br>        print(ch)<br><br><br>    else:<br>        ch = ch[3:-3]<br>        ch1 = ch[len(ch) - 1]<br>        ch = ch[:-1]<br>        ch = ch1 + ch<br>        print(ch)
