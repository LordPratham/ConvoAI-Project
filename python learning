from array import array<br><br>arr = array(&#39;i&#39;, [4, 5, 9, 1, 8])<br>print([i for i in arr])<br>n = len(arr)<br><br>for i in range(n // 2):<br>    arr[i],arr[n-i-1] = arr[n-i-1],arr[i]<br>print([i for i in arr])
