import random<br>print(&quot;Welcome to the Snake,Water ,Gun Game &quot;)<br>print(&quot;1.Snake 2.Water 3.Gun&quot;)<br>l=[&quot;Snake&quot;,&quot;Water&quot;,&quot;Gun&quot;]<br>user_input=int(input(&quot;enter the input:&quot;))<br>comp_list=[1,2,3]<br>com_input=random.choice(comp_list)<br>def game (com_input,user_input):<br>    print(f&quot;computer entered {l[com_input-1]}&quot;)<br>    if(com_input==1 and user_input==3):<br>        return &quot;you win!&quot;<br>    elif(com_input==2 and user_input==1):<br>        return &quot;you win!&quot;<br>    elif(com_input==3 and user_input==20):<br>        return &quot;you win!&quot;<br>    elif(com_input==user_input):<br>        return &quot;draw&quot;<br>    else:<br>        print(&quot;computer wins&quot;)<br>print(game(com_input,user_input))
import random<br><br>options = (&quot;snake&quot;, &quot;water&quot;, &quot;gun&quot;)<br>running = True<br><br>while running:<br><br>    player = None<br>    computer = random.choice(options)<br><br>    while player not in options:<br>        player = input(&quot;Enter a choice (snake,water,gun): &quot;)<br><br>    print(f&quot;Player: {player}&quot;)<br>    print(f&quot;Computer: {computer}&quot;)<br><br>    if player == computer:<br>        print(&quot;It&#39;s a tie!&quot;)<br>    elif player == &quot;snake&quot; and computer == &quot;water&quot;:<br>        print(&quot;You win!&quot;)<br>    elif player == &quot;gun&quot; and computer == &quot;snake&quot;:<br>        print(&quot;You win!&quot;)<br>    elif player == &quot;water&quot; and computer == &quot;gun&quot;:<br>        print(&quot;You win!&quot;)<br>    else:<br>        print(&quot;You lose!&quot;)<br><br>    if not input(&quot;Play again? (y/n): &quot;).lower() == &quot;y&quot;:<br>        running = False<br><br>print(&quot;Thanks for playing!&quot;)
import random<br>result_game=[[&quot;Draw&quot;,&quot;pl1&quot;,&quot;pl2&quot;],<br>             [&quot;pl2&quot;,&quot;Draw&quot;,&quot;pl1&quot;],<br>             [&quot;pl1&quot;,&quot;pl2&quot;,&quot;Draw&quot;]]<br>p1=0<br>p2=0 <br>print(&quot;There are of total 10 Games Each of 1 point Winner Get 1 point in each round\n&quot;)<br>i=1<br>while i&lt;=10:<br> print(f&quot;round : {i}\n&quot;)   <br> input1=int(input(&quot;Enter Your Choice 0-2 (Choose 0-&gt;Stone, 1-&gt;Paper, 2-&gt; Scissor):&quot;))<br> <br> if(input1==0):<br>  print(&quot;You Choose : Stone&quot;)<br> elif(input1==1):<br>     print(&quot;You Choose : Paper&quot;)<br> elif(input1==2):<br>     print(&quot;You Choose : Scissor&quot;)<br> else:<br>     print(&quot;Please Choose Correct Input Between 0 To 2&quot;)  <br>     exit()<br>     <br> random.seed()  <br> input2=random.randint(0,2)<br> <br> if(input2==0):<br>  print(&quot;Computer Choose : Stone&quot;)<br> elif(input2==1):<br>     print(&quot;Computer Choose : Paper&quot;)<br> elif(input2==2):<br>     print(&quot;Computer Choose : Scissor&quot;)<br> else:<br>    print(&quot;Please Choose Correct Input Between 0 To 2&quot;)  <br>    exit()<br>    <br>   <br> if(result_game[input1][input2]==&quot;Draw&quot;):<br>    print(f&quot;Match Draw\n Score: Player 1= {p1} and Computer= {p2}\n&quot;) <br>    <br> elif (result_game[input1][input2]==&quot;pl1&quot;):<br>    p1+=1<br>    print(f&quot;Player 1 Won the Game and Earn One Point\n Score : Player 1= {p1} and Computer= {p2}\n&quot;)<br> else:<br>    p2+=1<br>    print(f&quot;Computer Won the Game and Earn One Point\n Score : Player 1= {p1} and Computer= {p2}\n&quot;)<br>    <br> i+=1    <br> <br>print(f&quot;Total Score :\nPlayer 1 = {p1} and Computer = {p2}&quot;)<br>if(p1&gt;p2):<br>    print(&quot;Player 1 Won The Match&quot;) <br>elif(p2&gt;p1):<br>    print(&quot;Computer Won The Match&quot;)    <br>else:<br>    print(&quot;Match Draw&quot;)
import random<br>lst=[1,2,3]<br>def winner(x,y):<br>    print(&quot;Hurray!!! You won&quot;) if((x==1 and y==2) or(x==2 and y==3) or(x==3 and y==1)) else print(&quot;Ohhh!! It&#39;s a tie. Try again....&quot;) if (x==y) else print(&quot;Ouchh!! You lost&quot;)<br>while True:<br>    User_Input=int(input(&quot;Enter what you want to select among 1-3&gt;&gt;&gt;&gt; 1-Snake, 2-Water, 3-Gun:  &quot;))<br>    if(not (User_Input==1 or User_Input==2 or User_Input==3)):<br>        raise IndexError(&quot;You have selected a wrong choice. Please select 1-3 as 1-Snake, 2-Water, 3-Gun&quot;)<br>    Computer_Input=random.choice(lst)<br>    print(f&quot;The computer have choosen {Computer_Input}&quot;)<br>    winner(User_Input,Computer_Input)<br>    play=input(&quot;Do you want to play more?? yes/no or y/n:  &quot;)<br>    if(play.lower()==&quot;no&quot; or play.lower()==&#39;n&#39;):<br>        print(&quot;Thanks for playing. See you next time::))&quot;)<br>        break
<a href=""http://www.youtube.com/results?search_query=%23game"">#GAME</a>   snake defeat water , water defeat gun , gun defeat snake<br>        # 1 for snake 2 for water 3 for gun<br>import random<br>def take_input():<br>    choice=int(input())<br>    return choice<br><br>def play(choice,choice1):<br>    if(choice == choice1):<br>        print(&quot;Game draw&quot;)<br>        return 0<br>    elif(choice==1 and choice1==2):<br>        print(&quot;You won. You chose Snake and  PC chose Water &quot;)<br>        return -1  # user won<br>    elif(choice==1 and choice1==3):<br>        print(&quot;PC won. You chose Snake and  PC chose Gun &quot;)<br>        return -2 # pc won<br>    elif(choice==2 and choice1==3):<br>        print(&quot;You won. You chose Water and  PC chose Gun &quot;)<br>        return -1 # user won<br>    elif(choice==2 and choice1==1):<br>        print(&quot;PC won. You chose Water and  PC chose Snake &quot;)<br>        return -2 # pc won<br>    elif(choice==3 and choice1==1):<br>        print(&quot;You won. You chose GUN and  PC chose Snake &quot;)<br>        return -1 # user won<br>    elif(choice==3 and choice1==2):<br>        print(&quot;PC won. You chose GUN and  PC chose Water &quot;)<br>        return -2 <a href=""http://www.youtube.com/results?search_query=%23pc"">#pc</a> won<br><br><br><br>save_result = []<br>print(&quot;                     Snake,Water,Gun Game&quot;)<br>print(&quot;Game will be played total 5 times&quot;)<br>for i in range(0,5):<br>    print(&quot;Enter your choice 1 for SNAKE 2 for Water 3 for GUN&quot;)<br>    user_choice=  take_input()<br>    pc_choice=random.randint(1,3)<br>    result=play(user_choice,pc_choice)<br>    save_result.append(result)<br><br><br>times_userwon=save_result.count(-1)<br>times_pcwon=save_result.count(-2)<br><br>if(times_userwon&gt;times_pcwon):<br>    print(&quot;You Won more games than PC&quot;)<br>    print(&quot;You Won&quot;, times_userwon, &quot;times &quot;)<br>    print(&quot;PC Won&quot;, times_pcwon, &quot;times &quot;)<br><br>elif(times_userwon&lt;times_pcwon):<br>    print(&quot;You Won more games than PC&quot;)<br>    print(&quot;You Won&quot;, times_userwon, &quot;times &quot;)<br>    print(&quot;PC Won&quot;, times_pcwon, &quot;times &quot;)<br>elif(times_userwon == times_pcwon):<br>    print(&quot;You and PC both won equal no of times.&quot;)<br>    print(&quot;Game is DRAW&quot;)<br>    print(&quot;You Won&quot;, times_userwon, &quot;times &quot;)<br>    print(&quot;PC Won&quot;, times_pcwon, &quot;times &quot;)
import random<br><a href=""http://www.youtube.com/results?search_query=%23inputing"">#inputing</a> rounds<br>rounds = int(input(&quot;Enter the number of rounds: &quot;))<br>i = 0<br>your_score = 0<br>computer_score = 0<br><br>while i &lt; rounds:<br><a href=""http://www.youtube.com/results?search_query=%23your"">#your</a> choice<br>    n = input(&quot;Choose (rock/paper/scissors): &quot;)<br>    print(&quot;Your choice: &quot; + n)<br><a href=""http://www.youtube.com/results?search_query=%23computer"">#computer</a> choice<br>    game = [&quot;rock&quot;, &quot;paper&quot;, &quot;scissors&quot;]<br>    random_choice = random.choice(game)<br>    print(&quot;Computer&#39;s choice: &quot; + random_choice)<br><a href=""http://www.youtube.com/results?search_query=%23game"">#game</a><br>    if n == random_choice:<br>        print(&quot;Draw&quot;)<br>        print(&quot; &quot;)<br>    elif (n == &quot;rock&quot; and random_choice == &quot;scissors&quot;) or (n == &quot;paper&quot; and random_choice == &quot;rock&quot;) or (n == &quot;scissors&quot; and random_choice == &quot;paper&quot;):<br>        print(&quot;Win&quot;)<br>        print(&quot; &quot;)<br>        your_score += 1<br>    else:<br>        print(&quot;Loss&quot;)<br>        print(&quot; &quot;)<br>        computer_score += 1<br>    i+=1<br><a href=""http://www.youtube.com/results?search_query=%23final"">#final</a> result<br>print(&quot;Your final score: &quot;, your_score)<br>print(&quot;Computer&#39;s final score: &quot;, computer_score)<br>if your_score &gt; computer_score:<br>    print(&quot; &quot;)<br>    print(&quot;Congratulations! YOU WON&quot;)<br>elif your_score == computer_score:<br>    print(&quot; &quot;)<br>    print(&quot;Pretty good! GAME DRAW&quot;)<br>else:<br>    print(&quot; &quot;)<br>    print(&quot;Better luck next time! YOU LOST&quot;)
