import time<br>import pyttsx3<br>engine=pyttsx3.init()<br>t1=time.strftime(&#39;%H:%M:%S&#39;)<br>print(t1)<br>t2=int(time.strftime(&#39;%H&#39;))<br>print(t2)<br>t3=int(time.strftime(&#39;%M&#39;))<br>print(t3)<br>t4=int(time.strftime(&#39;%S&#39;))<br>print(t4)<br><br># Good Morning Sir<br>h=int(time.strftime(&#39;%H&#39;))<br>if (5&lt;=h&lt;12):<br>    engine.say(&quot;Good Morning Aditya&quot;)<br>elif (12&lt;=h&lt;17):<br>    engine.say(&quot;Good Afternoon Aditya&quot;)<br>elif (17&lt;=h&lt;21):<br>    engine.say(&quot;Good Evening Aditya&quot;)<br>elif (21&lt;=h&lt;24):<br>    engine.say(&quot;Good Night Aditya&quot;)<br>engine.runAndWait()
