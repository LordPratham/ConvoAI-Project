def sort(nums):<br>    for i in range(len(nums) - 1, 0, -1):<br>        for j in range (i):<br>            if nums[j] &lt; nums[j + 1]:<br>                continue<br>            else:<br><br>                nums[j+1], nums[j] = nums[j], nums[j+1]<br><br><br><br>nums = [5, 8, 9, 71, 10, 2,589,1,0]<br>sort(nums)<br>print(nums)
