# function to find the maximum value from an array or any other iterable object<br>from array import array<br>a = array(&quot;i&quot;,[67,3,1,0,-1])<br>b = array(&quot;I&quot;,[2,3,4,1,9])<br>def func(it_obj):<br>    maximum = it_obj[0]<br>    for i in it_obj:<br>        if maximum &lt; i:<br>            maximum = i<br>    return maximum<br>print(func(a))<br>print(func(b))
